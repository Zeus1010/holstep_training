N IS_INTERVAL_IMP_FSIGMA
C |- (!s. ((is_interval s) ==> (fsigma s)))
T c==> cis_interval f0 cfsigma f0
D TRUTH
A |- T
T cT
D IS_INTERVAL_IMP_ENR
A |- (!s. ((is_interval s) ==> (ENR s)))
T c==> cis_interval f0 cENR f0
D ENR_IMP_FSGIMA
A |- (!s. ((ENR s) ==> (fsigma s)))
T c==> cENR f0 cfsigma f0
- (is_interval s) |- (T = (is_interval s))
T c==> cis_interval f0 c= cT cis_interval f0
+ |- (((is_interval s) ==> ((fsigma s) = q')) ==> (((is_interval s) ==> (fsigma s)) = ((is_interval s) ==> q')))
T c==> c==> cis_interval f0 c= cfsigma f0 f1 c= c==> cis_interval f0 cfsigma f0 c==> cis_interval f0 f1
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!s. t) = t)
T c= ! f0 f0
+ |- ((\s. ((is_interval s) ==> (ENR s))) s)
T * / c==> cis_interval b0 cENR b0 f0
+ (is_interval s) |- ((is_interval s) /\ ((fsigma s) = T))
T c==> cis_interval f0 c/\ cis_interval f0 c= cfsigma f0 cT
+ |- ((!q'. ((p = p') ==> ((p' ==> (q = q')) ==> ((p ==> q) = (p' ==> q'))))) = T)
T c= ! c==> c= f0 f1 c==> c==> f1 c= f2 b0 c= c==> f0 f2 c==> f1 b0 cT
- (is_interval s) |- ((is_interval s) /\ ((ENR s) = T))
T c==> cis_interval f0 c/\ cis_interval f0 c= cENR f0 cT
- (is_interval s) |- (T = (ENR s))
T c==> cis_interval f0 c= cT cENR f0
- (is_interval s) |- ((is_interval s) = T)
T c==> cis_interval f0 c= cis_interval f0 cT
- |- ((is_interval s) ==> ((ENR s) = T))
T c==> cis_interval f0 c= cENR f0 cT
