N INDEPENDENT_EQ_DIM_EQ_CARD
C |- (!s. ((independent s) = ((FINITE s) /\ ((dim s) = (CARD s)))))
T c= cindependent f0 c/\ cFINITE f0 c= cdim f0 cCARD f0
D TRUTH
A |- T
T cT
D LT_REFL
A |- (!n. (~ (n < n)))
T c~ c< f0 f0
D independent
A |- (!s. ((independent s) = (~ (dependent s))))
T c= cindependent f0 c~ cdependent f0
D INDEPENDENT_IMP_FINITE
A |- (!s. ((independent s) ==> (FINITE s)))
T c==> cindependent f0 cFINITE f0
D DIM_EQ_CARD
A |- (!s. ((independent s) ==> ((dim s) = (CARD s))))
T c==> cindependent f0 c= cdim f0 cCARD f0
D DEPENDENT_EQ_DIM_LT_CARD
