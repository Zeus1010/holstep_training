N CONTINUOUS_ON_CX_NORM
C |- (!s. ((continuous_on (\z. (Cx (vector_norm z)))) s))
T ccontinuous_on / cCx cvector_norm b0 f0
D TRUTH
A |- T
T cT
D CONTINUOUS_ON_EQ_CONTINUOUS_WITHIN
A |- (!f. (!s. (((continuous_on f) s) = (!x. ((x IN s) ==> ((continuous f) ((within (at x)) s)))))))
T c= ccontinuous_on f0 f1 ! c==> cIN b0 f1 ccontinuous f0 cwithin cat b0 f1
D CONTINUOUS_WITHIN_CX_NORM
A |- (!z. (!s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s))))
T ccontinuous / cCx cvector_norm b0 cwithin cat f0 f1
+ |- (((!s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s))) ==> ((\s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s))) s)) = ((\s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s))) s))
T c= c==> ! ccontinuous / cCx cvector_norm b1 cwithin cat f0 b0 * / ccontinuous / cCx cvector_norm b1 cwithin cat f0 b0 f1 * / ccontinuous / cCx cvector_norm b1 cwithin cat f0 b0 f1
+ |- (((!z. (!s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s)))) ==> ((\z. (!s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s)))) z)) = ((\z. (!s. ((continuous (\z. (Cx (vector_norm z)))) ((within (at z)) s)))) z))
T c= c==> ! ! ccontinuous / cCx cvector_norm b2 cwithin cat b0 b1 * / ! ccontinuous / cCx cvector_norm b2 cwithin cat b0 b1 f0 * / ! ccontinuous / cCx cvector_norm b2 cwithin cat b0 b1 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (x IN s) |- ((x IN s) = T)
T c==> cIN f0 f1 c= cIN f0 f1 cT
