N REAL_CONTINUOUS_WITHIN_SUBSET
C |- (!f. (!s. (!t. ((((real_continuous f) ((within (at x)) s)) /\ (t SUBSET s)) ==> ((real_continuous f) ((within (at x)) t))))))
T c==> c/\ creal_continuous f0 cwithin cat f1 f2 cSUBSET f3 f2 creal_continuous f0 cwithin cat f1 f3
D TRUTH
A |- T
T cT
D REALLIM_WITHIN_SUBSET
A |- (!f. (!l. (!a. (!s. (!t. (((((---> f) l) ((within (at a)) s)) /\ (t SUBSET s)) ==> (((---> f) l) ((within (at a)) t))))))))
T c==> c/\ c---> f0 f1 cwithin cat f2 f3 cSUBSET f4 f3 c---> f0 f1 cwithin cat f2 f4
D REAL_CONTINUOUS_WITHIN
A |- (!f. (!x. (!s. (((real_continuous f) ((within (at x)) s)) = (((---> f) (f x)) ((within (at x)) s))))))
T c= creal_continuous f0 cwithin cat f1 f2 c---> f0 * f0 f1 cwithin cat f1 f2
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\l. (!a. (!s. (!t. (((((---> f) l) ((within (at a)) s)) /\ (t SUBSET s)) ==> (((---> f) l) ((within (at a)) t))))))) l)
T * / ! ! ! c==> c/\ c---> f0 b0 cwithin cat b1 b2 cSUBSET b3 b2 c---> f0 b0 cwithin cat b1 b3 f1
+ |- (((!l. (!a. (!s. (!t. (((((---> f) l) ((within (at a)) s)) /\ (t SUBSET s)) ==> (((---> f) l) ((within (at a)) t))))))) ==> ((\l. (!a. (!s. (!t. (((((---> f) l) ((within (at a)) s)) /\ (t SUBSET s)) ==> (((---> f) l) ((within (at a)) t))))))) l)) = ((\l. (!a. (!s. (!t. (((((---> f) l) ((within (at a)) s)) /\ (t SUBSET s)) ==> (((---> f) l) ((within (at a)) t))))))) l))
T c= c==> ! ! ! ! c==> c/\ c---> f0 b0 cwithin cat b1 b2 cSUBSET b3 b2 c---> f0 b0 cwithin cat b1 b3 * / ! ! ! c==> c/\ c---> f0 b0 cwithin cat b1 b2 cSUBSET b3 b2 c---> f0 b0 cwithin cat b1 b3 f1 * / ! ! ! c==> c/\ c---> f0 b0 cwithin cat b1 b2 cSUBSET b3 b2 c---> f0 b0 cwithin cat b1 b3 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
