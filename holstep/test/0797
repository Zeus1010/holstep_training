N REAL_NEGLIGIBLE_DIFF
C |- (!s. (!t. ((real_negligible s) ==> (real_negligible (s DIFF t)))))
T c==> creal_negligible f0 creal_negligible * * part cDIFF f0 f1
D TRUTH
A |- T
T cT
D SUBSET_DIFF
A |- (!s. (!t. ((s DIFF t) SUBSET s)))
T cSUBSET * * part cDIFF f0 f1 f0
D REAL_NEGLIGIBLE_SUBSET
A |- (!s. (!t. (((real_negligible s) /\ (t SUBSET s)) ==> (real_negligible t))))
T c==> c/\ creal_negligible f0 cSUBSET f1 f0 creal_negligible f1
+ (!s. (!t. (((real_negligible s) /\ (t SUBSET s)) ==> (real_negligible t)))) |- (((!t. (((real_negligible s) /\ (t SUBSET s)) ==> (real_negligible t))) ==> ((\t. (((real_negligible s) /\ (t SUBSET s)) ==> (real_negligible t))) t)) = ((\t. (((real_negligible s) /\ (t SUBSET s)) ==> (real_negligible t))) t))
T c==> ! ! c==> c/\ creal_negligible b0 cSUBSET b1 b0 creal_negligible b1 c= c==> ! c==> c/\ creal_negligible f0 cSUBSET b0 f0 creal_negligible b0 * / c==> c/\ creal_negligible f0 cSUBSET b0 f0 creal_negligible b0 f1 * / c==> c/\ creal_negligible f0 cSUBSET b0 f0 creal_negligible b0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
