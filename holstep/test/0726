N INT_SGN_NEG
C |- (!x. ((int_sgn (int_neg x)) = (int_neg (int_sgn x))))
T c= cint_sgn cint_neg f0 cint_neg cint_sgn f0
D REAL_SGN_NEG
A |- (!x. ((real_sgn (real_neg x)) = (real_neg (real_sgn x))))
T c= creal_sgn creal_neg f0 creal_neg creal_sgn f0
D int_eq
A |- (!x. (!y. ((x = y) = ((real_of_int x) = (real_of_int y)))))
T c= c= f0 f1 c= creal_of_int f0 creal_of_int f1
D int_neg_th
A |- (!x. ((real_of_int (int_neg x)) = (real_neg (real_of_int x))))
T c= creal_of_int cint_neg f0 creal_neg creal_of_int f0
D int_sgn_th
A |- (!x. ((real_of_int (int_sgn x)) = (real_sgn (real_of_int x))))
T c= creal_of_int cint_sgn f0 creal_sgn creal_of_int f0
