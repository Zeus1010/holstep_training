N NPRODUCT_CONST_NUMSEG_1
C |- (!c. (!n. (((nproduct ((NUMERAL (BIT1 _0)) .. n)) (\x. c)) = (c EXP n))))
T c= cnproduct * * part c.. cNUMERAL cBIT1 c_0 f0 / f1 cEXP f1 f0
D TRUTH
A |- T
T cT
D FINITE_NUMSEG
A |- (!m. (!n. (FINITE (m .. n))))
T cFINITE * * part c.. f0 f1
D CARD_NUMSEG_1
A |- (!n. ((CARD ((NUMERAL (BIT1 _0)) .. n)) = n))
T c= cCARD * * part c.. cNUMERAL cBIT1 c_0 f0 f0
D NPRODUCT_CONST
A |- (!c. (!s. ((FINITE s) ==> (((nproduct s) (\x. c)) = (c EXP (CARD s))))))
T c==> cFINITE f0 c= cnproduct f0 / f1 cEXP f1 cCARD f0
+ |- (((!c. (!s. ((FINITE s) ==> (((nproduct s) (\x. c)) = (c EXP (CARD s)))))) ==> ((\c. (!s. ((FINITE s) ==> (((nproduct s) (\x. c)) = (c EXP (CARD s)))))) c)) = ((\c. (!s. ((FINITE s) ==> (((nproduct s) (\x. c)) = (c EXP (CARD s)))))) c))
T c= c==> ! ! c==> cFINITE b1 c= cnproduct b1 / b0 cEXP b0 cCARD b1 * / ! c==> cFINITE b1 c= cnproduct b1 / b0 cEXP b0 cCARD b1 f0 * / ! c==> cFINITE b1 c= cnproduct b1 / b0 cEXP b0 cCARD b1 f0
- (FINITE s) |- ((FINITE s) /\ (((nproduct s) (\x. c)) = (c EXP (CARD s))))
T c==> cFINITE f0 c/\ cFINITE f0 c= cnproduct f0 / f1 cEXP f1 cCARD f0
