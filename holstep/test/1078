N STC_IDEMP
C |- (!R. ((STC (STC R)) = (STC R)))
T c= * part cSTC * part cSTC f0 * part cSTC f0
D TRUTH
A |- T
T cT
D STC_SYM
A |- (!R. (!x. (!y. ((((STC R) x) y) ==> (((STC R) y) x)))))
T c==> cSTC f0 f1 f2 cSTC f0 f2 f1
D STC_TRANS
A |- (!R. (!x. (!y. (!z. (((((STC R) x) y) /\ (((STC R) y) z)) ==> (((STC R) x) z))))))
T c==> c/\ cSTC f0 f1 f2 cSTC f0 f2 f3 cSTC f0 f1 f3
D STC_CLOSED
A |- (!R. (((STC R) = R) = ((!x. (!y. (((R x) y) ==> ((R y) x)))) /\ (!x. (!y. (!z. ((((R x) y) /\ ((R y) z)) ==> ((R x) z))))))))
T c= c= * part cSTC f0 f0 c/\ ! ! c==> * * f0 b0 b1 * * f0 b1 b0 ! ! ! c==> c/\ * * f0 b0 b1 * * f0 b1 b2 * * f0 b0 b2
+ |- (((!z. (((((STC R) x) y) /\ (((STC R) y) z)) ==> (((STC R) x) z))) ==> ((\z. (((((STC R) x) y) /\ (((STC R) y) z)) ==> (((STC R) x) z))) z)) = ((\z. (((((STC R) x) y) /\ (((STC R) y) z)) ==> (((STC R) x) z))) z))
T c= c==> ! c==> c/\ cSTC f0 f1 f2 cSTC f0 f2 b0 cSTC f0 f1 b0 * / c==> c/\ cSTC f0 f1 f2 cSTC f0 f2 b0 cSTC f0 f1 b0 f3 * / c==> c/\ cSTC f0 f1 f2 cSTC f0 f2 b0 cSTC f0 f1 b0 f3
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
