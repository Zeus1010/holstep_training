N FRONTIER_FRONTIER_FRONTIER
C |- (!s. ((frontier (frontier (frontier s))) = (frontier (frontier s))))
T c= * part cfrontier * part cfrontier * part cfrontier f0 * part cfrontier * part cfrontier f0
D TRUTH
A |- T
T cT
D FRONTIER_CLOSED
A |- (!s. (closed (frontier s)))
T cclosed * part cfrontier f0
D FRONTIER_FRONTIER
A |- (!s. (((open s) \/ (closed s)) ==> ((frontier (frontier s)) = (frontier s))))
T c==> c\/ copen f0 cclosed f0 c= * part cfrontier * part cfrontier f0 * part cfrontier f0
+ |- (T = (!s. ((frontier (frontier (frontier s))) = (frontier (frontier s)))))
T c= cT ! c= * part cfrontier * part cfrontier * part cfrontier b0 * part cfrontier * part cfrontier b0
+ |- ((open (frontier s)) \/ (closed (frontier s)))
T c\/ copen * part cfrontier f0 cclosed * part cfrontier f0
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
- ((open s) \/ (closed s)) |- (((open s) \/ (closed s)) /\ ((frontier (frontier s)) = (frontier s)))
T c==> c\/ copen f0 cclosed f0 c/\ c\/ copen f0 cclosed f0 c= * part cfrontier * part cfrontier f0 * part cfrontier f0
