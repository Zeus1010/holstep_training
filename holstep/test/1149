N HOMOTOPIC_NEARBY_PATHS
C |- (!g. (!s. (((path g) /\ ((open s) /\ ((path_image g) SUBSET s))) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!h. (((path h) /\ (((pathstart h) = (pathstart g)) /\ (((pathfinish h) = (pathfinish g)) /\ (!t. ((t IN (closed_interval ((CONS ((vec (NUMERAL _0)) , (vec (NUMERAL (BIT1 _0))))) NIL))) ==> ((real_lt (vector_norm ((vector_sub (h t)) (g t)))) e)))))) ==> (((homotopic_paths s) g) h))))))))
T c==> c/\ cpath f0 c/\ copen f1 cSUBSET * part cpath_image f0 f1 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b0 ! c==> c/\ cpath b1 c/\ c= cpathstart b1 cpathstart f0 c/\ c= cpathfinish b1 cpathfinish f0 ! c==> cIN b2 * part cclosed_interval cCONS c, cvec cNUMERAL c_0 cvec cNUMERAL cBIT1 c_0 cNIL creal_lt cvector_norm cvector_sub * b1 b2 * f0 b2 b0 chomotopic_paths f1 f0 b1
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D NOT_EXISTS_THM
A |- (!P. ((~ (?x. (P x))) = (!x. (~ (P x)))))
T c= c~ c? / * f0 b0 ! c~ * f0 b0
D I_THM
A |- (!x. ((I x) = x))
T c= cI f0 f0
D REAL_LTE_TRANS
A |- (!x. (!y. (!z. ((((real_lt x) y) /\ ((real_le y) z)) ==> ((real_lt x) z)))))
T c==> c/\ creal_lt f0 f1 creal_le f1 f2 creal_lt f0 f2
D IN
A |- (!P. (!x. ((x IN P) = (P x))))
T c= cIN f0 f1 * f1 f0
D EXTENSION
A |- (!s. (!t. ((s = t) = (!x. ((x IN s) = (x IN t))))))
T c= c= f0 f1 ! c= cIN b0 f0 cIN b0 f1
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D NOT_IN_EMPTY
A |- (!x. (~ (x IN EMPTY)))
T c~ cIN f0 part cEMPTY
D IN_UNIV
A |- (!x. (x IN UNIV))
T cIN f0 part cUNIV
D IN_INTER
A |- (!s. (!t. (!x. ((x IN (s INTER t)) = ((x IN s) /\ (x IN t))))))
T c= cIN f0 * * part cINTER f1 f2 c/\ cIN f0 f1 cIN f0 f2
D IN_DIFF
A |- (!s. (!t. (!x. ((x IN (s DIFF t)) = ((x IN s) /\ (~ (x IN t)))))))
T c= cIN f0 * * part cDIFF f1 f2 c/\ cIN f0 f1 c~ cIN f0 f2
D IN_IMAGE
A |- (!y. (!s. (!f. ((y IN ((IMAGE f) s)) = (?x. ((y = (f x)) /\ (x IN s)))))))
T c= cIN f0 * * part cIMAGE f1 f2 c? / c/\ c= f0 * f1 b0 cIN b0 f2
D dist
A |- (!x. (!y. ((distance (x , y)) = (vector_norm ((vector_sub x) y)))))
T c= cdistance c, f0 f1 cvector_norm cvector_sub f0 f1
D OPEN_CLOSED
A |- (!s. ((open s) = (closed (UNIV DIFF s))))
T c= copen f0 cclosed * * part cDIFF part cUNIV f0
D SEPARATE_COMPACT_CLOSED
A |- (!s. (!t. (((compact s) /\ ((closed t) /\ ((s INTER t) = EMPTY))) ==> (?d. (((real_lt (real_of_num (NUMERAL _0))) d) /\ (!x. (!y. (((x IN s) /\ (y IN t)) ==> ((real_le d) (distance (x , y)))))))))))
T c==> c/\ ccompact f0 c/\ cclosed f1 c= * * part cINTER f0 f1 part cEMPTY c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b0 ! ! c==> c/\ cIN b1 f0 cIN b2 f1 creal_le b0 cdistance c, b1 b2
D path_image
A |- (!g. ((path_image g) = ((IMAGE g) (closed_interval ((CONS ((vec (NUMERAL _0)) , (vec (NUMERAL (BIT1 _0))))) NIL)))))
T c= * part cpath_image f0 * * part cIMAGE f0 * part cclosed_interval cCONS c, cvec cNUMERAL c_0 cvec cNUMERAL cBIT1 c_0 cNIL
D COMPACT_PATH_IMAGE
A |- (!g. ((path g) ==> (compact (path_image g))))
T c==> cpath f0 ccompact * part cpath_image f0
D HOMOTOPIC_PATHS_NEARBY_EXPLICIT
A |- (!g. (!s. (!h. (((path g) /\ ((path h) /\ (((pathstart h) = (pathstart g)) /\ (((pathfinish h) = (pathfinish g)) /\ (!t. (!x. (((t IN (closed_interval ((CONS ((vec (NUMERAL _0)) , (vec (NUMERAL (BIT1 _0))))) NIL))) /\ (~ (x IN s))) ==> ((real_lt (vector_norm ((vector_sub (h t)) (g t)))) (vector_norm ((vector_sub (g t)) x)))))))))) ==> (((homotopic_paths s) g) h)))))
T c==> c/\ cpath f0 c/\ cpath f1 c/\ c= cpathstart f1 cpathstart f0 c/\ c= cpathfinish f1 cpathfinish f0 ! ! c==> c/\ cIN b0 * part cclosed_interval cCONS c, cvec cNUMERAL c_0 cvec cNUMERAL cBIT1 c_0 cNIL c~ cIN b1 f2 creal_lt cvector_norm cvector_sub * f1 b0 * f0 b0 cvector_norm cvector_sub * f0 b0 b1 chomotopic_paths f2 f0 f1
