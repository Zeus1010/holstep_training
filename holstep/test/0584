N SUBSPACE_REAL
C |- (subspace real)
T csubspace part creal
D TRUTH
A |- T
T cT
D IN
A |- (!P. (!x. ((x IN P) = (P x))))
T c= cIN f0 f1 * f1 f0
D subspace
A |- (!s. ((subspace s) = (((vec (NUMERAL _0)) IN s) /\ ((!x. (!y. (((x IN s) /\ (y IN s)) ==> (((vector_add x) y) IN s)))) /\ (!c. (!x. ((x IN s) ==> (((% c) x) IN s))))))))
T c= csubspace f0 c/\ cIN cvec cNUMERAL c_0 f0 c/\ ! ! c==> c/\ cIN b0 f0 cIN b1 f0 cIN cvector_add b0 b1 f0 ! ! c==> cIN b1 f0 cIN c% b0 b1 f0
D COMPLEX_VEC_0
A |- ((vec (NUMERAL _0)) = (Cx (real_of_num (NUMERAL _0))))
T c= cvec cNUMERAL c_0 cCx creal_of_num cNUMERAL c_0
D COMPLEX_CMUL
A |- (!c. (!x. (((% c) x) = ((complex_mul (Cx c)) x))))
T c= c% f0 f1 ccomplex_mul cCx f0 f1
D REAL_CX
A |- (!x. (real (Cx x)))
T creal cCx f0
D REAL_ADD
A |- (!w. (!z. (((real w) /\ (real z)) ==> (real ((vector_add w) z)))))
T c==> c/\ creal f0 creal f1 creal cvector_add f0 f1
D REAL_MUL
A |- (!w. (!z. (((real w) /\ (real z)) ==> (real ((complex_mul w) z)))))
T c==> c/\ creal f0 creal f1 creal ccomplex_mul f0 f1
+ |- ((\q'. ((((x IN real) /\ (y IN real)) = p') ==> ((p' ==> ((((vector_add x) y) IN real) = q')) ==> ((((x IN real) /\ (y IN real)) ==> (((vector_add x) y) IN real)) = (p' ==> q'))))) q')
T * / c==> c= c/\ cIN f0 part creal cIN f1 part creal f2 c==> c==> f2 c= cIN cvector_add f0 f1 part creal b0 c= c==> c/\ cIN f0 part creal cIN f1 part creal cIN cvector_add f0 f1 part creal c==> f2 b0 f3
+ |- ((\s. ((subspace s) = (((vec (NUMERAL _0)) IN s) /\ ((!x. (!y. (((x IN s) /\ (y IN s)) ==> (((vector_add x) y) IN s)))) /\ (!c. (!x. ((x IN s) ==> (((% c) x) IN s)))))))) s)
T * / c= csubspace b0 c/\ cIN cvec cNUMERAL c_0 b0 c/\ ! ! c==> c/\ cIN b1 b0 cIN b2 b0 cIN cvector_add b1 b2 b0 ! ! c==> cIN b2 b0 cIN c% b1 b2 b0 f0
+ |- (((!s. ((subspace s) = (((vec (NUMERAL _0)) IN s) /\ ((!x. (!y. (((x IN s) /\ (y IN s)) ==> (((vector_add x) y) IN s)))) /\ (!c. (!x. ((x IN s) ==> (((% c) x) IN s)))))))) ==> ((\s. ((subspace s) = (((vec (NUMERAL _0)) IN s) /\ ((!x. (!y. (((x IN s) /\ (y IN s)) ==> (((vector_add x) y) IN s)))) /\ (!c. (!x. ((x IN s) ==> (((% c) x) IN s)))))))) s)) = ((\s. ((subspace s) = (((vec (NUMERAL _0)) IN s) /\ ((!x. (!y. (((x IN s) /\ (y IN s)) ==> (((vector_add x) y) IN s)))) /\ (!c. (!x. ((x IN s) ==> (((% c) x) IN s)))))))) s))
T c= c==> ! c= csubspace b0 c/\ cIN cvec cNUMERAL c_0 b0 c/\ ! ! c==> c/\ cIN b1 b0 cIN b2 b0 cIN cvector_add b1 b2 b0 ! ! c==> cIN b2 b0 cIN c% b1 b2 b0 * / c= csubspace b0 c/\ cIN cvec cNUMERAL c_0 b0 c/\ ! ! c==> c/\ cIN b1 b0 cIN b2 b0 cIN cvector_add b1 b2 b0 ! ! c==> cIN b2 b0 cIN c% b1 b2 b0 f0 * / c= csubspace b0 c/\ cIN cvec cNUMERAL c_0 b0 c/\ ! ! c==> c/\ cIN b1 b0 cIN b2 b0 cIN cvector_add b1 b2 b0 ! ! c==> cIN b2 b0 cIN c% b1 b2 b0 f0
+ |- ((((x IN real) /\ (y IN real)) = p') ==> ((p' ==> ((((vector_add x) y) IN real) = q')) ==> ((((x IN real) /\ (y IN real)) ==> (((vector_add x) y) IN real)) = (p' ==> q'))))
T c==> c= c/\ cIN f0 part creal cIN f1 part creal f2 c==> c==> f2 c= cIN cvector_add f0 f1 part creal f3 c= c==> c/\ cIN f0 part creal cIN f1 part creal cIN cvector_add f0 f1 part creal c==> f2 f3
+ |- ((\z. (((real w) /\ (real z)) ==> (real ((vector_add w) z)))) z)
T * / c==> c/\ creal f0 creal b0 creal cvector_add f0 b0 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((real x) /\ (real y)) |- ((real x) = T)
T c==> c/\ creal f0 creal f1 c= creal f0 cT
- ((real x) /\ (real y)) |- ((real y) = T)
T c==> c/\ creal f0 creal f1 c= creal f1 cT
- ((real x) /\ (real y)) |- (T = ((real x) /\ (real y)))
T c==> c/\ creal f0 creal f1 c= cT c/\ creal f0 creal f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
