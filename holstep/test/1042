N FRECHET_DERIVATIVE_CONST_AT
C |- (!c. (!a. (((frechet_derivative (\x. c)) (at a)) = (\h. (vec (NUMERAL _0))))))
T c= * * part cfrechet_derivative / f0 cat f1 / cvec cNUMERAL c_0
D TRUTH
A |- T
T cT
D HAS_DERIVATIVE_CONST
A |- (!c. (!net. (((has_derivative (\x. c)) (\h. (vec (NUMERAL _0)))) net)))
T chas_derivative / f0 / cvec cNUMERAL c_0 f1
D HAS_FRECHET_DERIVATIVE_UNIQUE_AT
A |- (!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f')))))
T c==> chas_derivative f0 f1 cat f2 c= * * part cfrechet_derivative f0 cat f2 f1
+ |- ((!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))) ==> (!f. (!x. (!f'. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))))
T c==> ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 ! ! ! c==> chas_derivative b0 b2 cat b1 c= * * part cfrechet_derivative b0 cat b1 b2
+ |- ((\x. (!f'. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f')))) x)
T * / ! c==> chas_derivative f0 b1 cat b0 c= * * part cfrechet_derivative f0 cat b0 b1 f1
+ (!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))) |- ((!f'. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))) = T)
T c==> ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 c= ! c==> chas_derivative f0 b0 cat f1 c= * * part cfrechet_derivative f0 cat f1 b0 cT
- (!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))), (((has_derivative f) f') (at x)) |- ((((has_derivative f) f') (at x)) /\ (((frechet_derivative f) (at x)) = f'))
T c==> ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 c==> chas_derivative f0 f1 cat f2 c/\ chas_derivative f0 f1 cat f2 c= * * part cfrechet_derivative f0 cat f2 f1
- (((has_derivative f) f') (at x)) |- ((!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))) ==> (((frechet_derivative f) (at x)) = f'))
T c==> chas_derivative f0 f1 cat f2 c==> ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 c= * * part cfrechet_derivative f0 cat f2 f1
- (!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))), (((has_derivative f) f') (at x)) |- ((!f. (!f'. (!x. ((((has_derivative f) f') (at x)) ==> (((frechet_derivative f) (at x)) = f'))))) /\ (((frechet_derivative f) (at x)) = f'))
T c==> ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 c==> chas_derivative f0 f1 cat f2 c/\ ! ! ! c==> chas_derivative b0 b1 cat b2 c= * * part cfrechet_derivative b0 cat b2 b1 c= * * part cfrechet_derivative f0 cat f2 f1
