N CONTINUOUS_WITHIN_CNJ
C |- (!s. (!z. ((continuous cnj) ((within (at z)) s))))
T ccontinuous part ccnj cwithin cat f0 f1
D TRUTH
A |- T
T cT
D LINEAR_CONTINUOUS_WITHIN
A |- (!f. (!s. (!x. ((linear f) ==> ((continuous f) ((within (at x)) s))))))
T c==> clinear f0 ccontinuous f0 cwithin cat f1 f2
D LINEAR_CNJ
A |- (linear cnj)
T clinear part ccnj
+ |- (T = (!s. (!z. ((continuous cnj) ((within (at z)) s)))))
T c= cT ! ! ccontinuous part ccnj cwithin cat b1 b0
+ |- ((!z. t) = t)
T c= ! f0 f0
- |- ((linear cnj) = T)
T c= clinear part ccnj cT
- |- (T = (linear cnj))
T c= cT clinear part ccnj
