N COUNTABLE_CASES
C |- (!s. ((COUNTABLE s) = ((FINITE s) \/ (s =_c UNIV))))
T c= cCOUNTABLE f0 c\/ cFINITE f0 c=_c f0 part cUNIV
D TRUTH
A |- T
T cT
D CARD_LE_LT
A |- (!s. (!t. ((s <=_c t) = ((s <_c t) \/ (s =_c t)))))
T c= c<=_c f0 f1 c\/ c<_c f0 f1 c=_c f0 f1
D FINITE_CARD_LT
A |- (!s. ((FINITE s) = (s <_c UNIV)))
T c= cFINITE f0 c<_c f0 part cUNIV
D COUNTABLE_ALT
A |- (!s. ((COUNTABLE s) = (s <=_c UNIV)))
T c= cCOUNTABLE f0 c<=_c f0 part cUNIV
+ |- ((\s. ((COUNTABLE s) = (s <=_c UNIV))) s)
T * / c= cCOUNTABLE b0 c<=_c b0 part cUNIV f0
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
