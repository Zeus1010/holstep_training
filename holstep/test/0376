N CENTRE_IN_MCBALL_EQ
C |- (!m. (!x. (!r. ((x IN ((mcball m) (x , r))) = ((x IN (mspace m)) /\ ((real_le (real_of_num (NUMERAL _0))) r))))))
T c= cIN f0 * * part cmcball f1 c, f0 f2 c/\ cIN f0 * part cmspace f1 creal_le creal_of_num cNUMERAL c_0 f2
D TRUTH
A |- T
T cT
D EXCLUDED_MIDDLE
A |- (!t. (t \/ (~ t)))
T c\/ f0 c~ f0
D MDIST_REFL
A |- (!m. (!x. ((x IN (mspace m)) ==> (((mdist m) (x , x)) = (real_of_num (NUMERAL _0))))))
T c==> cIN f0 * part cmspace f1 c= cmdist f1 c, f0 f0 creal_of_num cNUMERAL c_0
D IN_MCBALL
A |- (!m. (!x. (!r. (!y. ((y IN ((mcball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_le ((mdist m) (x , y))) r))))))))
T c= cIN f0 * * part cmcball f1 c, f2 f3 c/\ cIN f2 * part cmspace f1 c/\ cIN f0 * part cmspace f1 creal_le cmdist f1 c, f2 f0 f3
+ (x IN (mspace m)) |- ((x IN (mspace m)) = T)
T c==> cIN f0 * part cmspace f1 c= cIN f0 * part cmspace f1 cT
+ |- ((x IN (mspace m)) \/ (~ (x IN (mspace m))))
T c\/ cIN f0 * part cmspace f1 c~ cIN f0 * part cmspace f1
+ |- ((\y. ((y IN ((mcball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_le ((mdist m) (x , y))) r))))) y)
T * / c= cIN b0 * * part cmcball f0 c, f1 f2 c/\ cIN f1 * part cmspace f0 c/\ cIN b0 * part cmspace f0 creal_le cmdist f0 c, f1 b0 f2 f3
+ |- (((!m. (!x. ((x IN (mspace m)) ==> (((mdist m) (x , x)) = (real_of_num (NUMERAL _0)))))) ==> ((\m. (!x. ((x IN (mspace m)) ==> (((mdist m) (x , x)) = (real_of_num (NUMERAL _0)))))) m)) = ((\m. (!x. ((x IN (mspace m)) ==> (((mdist m) (x , x)) = (real_of_num (NUMERAL _0)))))) m))
T c= c==> ! ! c==> cIN b1 * part cmspace b0 c= cmdist b0 c, b1 b1 creal_of_num cNUMERAL c_0 * / ! c==> cIN b1 * part cmspace b0 c= cmdist b0 c, b1 b1 creal_of_num cNUMERAL c_0 f0 * / ! c==> cIN b1 * part cmspace b0 c= cmdist b0 c, b1 b1 creal_of_num cNUMERAL c_0 f0
- (~ (x IN (mspace m))) |- (F = (x IN (mspace m)))
T c==> c~ cIN f0 * part cmspace f1 c= cF cIN f0 * part cmspace f1
- (x IN (mspace m)) |- (T = (x IN (mspace m)))
T c==> cIN f0 * part cmspace f1 c= cT cIN f0 * part cmspace f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- (x IN (mspace m)) |- ((x IN (mspace m)) /\ (((mdist m) (x , x)) = (real_of_num (NUMERAL _0))))
T c==> cIN f0 * part cmspace f1 c/\ cIN f0 * part cmspace f1 c= cmdist f1 c, f0 f0 creal_of_num cNUMERAL c_0
