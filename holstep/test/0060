N STC_INC_SC
C |- (!R. (!x. (!y. ((((SC R) x) y) ==> (((STC R) x) y)))))
T c==> cSC f0 f1 f2 cSTC f0 f1 f2
D TRUTH
A |- T
T cT
D TC_INC
A |- (!R. (!x. (!y. (((R x) y) ==> (((TC R) x) y)))))
T c==> * * f0 f1 f2 cTC f0 f1 f2
D STC
A |- (!R. ((STC R) = (TC (SC R))))
T c= * part cSTC f0 * part cTC * part cSC f0
+ |- (((!x. (!y. (((R x) y) ==> (((TC R) x) y)))) ==> ((\x. (!y. (((R x) y) ==> (((TC R) x) y)))) x)) = ((\x. (!y. (((R x) y) ==> (((TC R) x) y)))) x))
T c= c==> ! ! c==> * * f0 b0 b1 cTC f0 b0 b1 * / ! c==> * * f0 b0 b1 cTC f0 b0 b1 f1 * / ! c==> * * f0 b0 b1 cTC f0 b0 b1 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
