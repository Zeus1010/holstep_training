N INT_ABS_BETWEEN1
C |- (!x. (!y. (!z. ((((int_lt x) z) /\ ((int_lt (int_abs ((int_sub y) x))) ((int_sub z) x))) ==> ((int_lt y) z)))))
T c==> c/\ cint_lt f0 f1 cint_lt cint_abs cint_sub f2 f0 cint_sub f1 f0 cint_lt f2 f1
D REAL_ABS_BETWEEN1
A |- (!x. (!y. (!z. ((((real_lt x) z) /\ ((real_lt (real_abs ((real_sub y) x))) ((real_sub z) x))) ==> ((real_lt y) z)))))
T c==> c/\ creal_lt f0 f1 creal_lt creal_abs creal_sub f2 f0 creal_sub f1 f0 creal_lt f2 f1
D int_lt
A |- (!x. (!y. (((int_lt x) y) = ((real_lt (real_of_int x)) (real_of_int y)))))
T c= cint_lt f0 f1 creal_lt creal_of_int f0 creal_of_int f1
D int_sub_th
A |- (!x. (!y. ((real_of_int ((int_sub x) y)) = ((real_sub (real_of_int x)) (real_of_int y)))))
T c= creal_of_int cint_sub f0 f1 creal_sub creal_of_int f0 creal_of_int f1
D int_abs_th
A |- (!x. ((real_of_int (int_abs x)) = (real_abs (real_of_int x))))
T c= creal_of_int cint_abs f0 creal_abs creal_of_int f0
