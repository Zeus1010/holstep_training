N TRACE_MATRIX_INV_RMUL
C |- (!A. ((trace ((matrix_mul A) (matrix_inv A))) = (real_of_num (rank A))))
T c= ctrace cmatrix_mul f0 cmatrix_inv f0 creal_of_num crank f0
D TRUTH
A |- T
T cT
D MATRIX_TRANSP_MUL
A |- (!A. (!B. ((transp ((matrix_mul A) B)) = ((matrix_mul (transp B)) (transp A)))))
T c= ctransp cmatrix_mul f0 f1 cmatrix_mul ctransp f1 ctransp f0
D RANK_TRANSP
A |- (!A. ((rank (transp A)) = (rank A)))
T c= crank ctransp f0 crank f0
D TRANSP_MATRIX_INV
A |- (!A. ((transp (matrix_inv A)) = (matrix_inv (transp A))))
T c= ctransp cmatrix_inv f0 cmatrix_inv ctransp f0
D TRACE_TRANSP
A |- (!A. ((trace (transp A)) = (trace A)))
T c= ctrace ctransp f0 ctrace f0
D TRACE_MATRIX_INV_LMUL
A |- (!A. ((trace ((matrix_mul (matrix_inv A)) A)) = (real_of_num (rank A))))
T c= ctrace cmatrix_mul cmatrix_inv f0 f0 creal_of_num crank f0
