N CROSS_INTER
C |- ((!s. (!t. (!u. ((s CROSS (t INTER u)) = ((s CROSS t) INTER (s CROSS u)))))) /\ (!s. (!t. (!u. (((s INTER t) CROSS u) = ((s CROSS u) INTER (t CROSS u)))))))
T c/\ ! ! ! c= * * part cCROSS b0 * * part cINTER b1 b2 * * part cINTER * * part cCROSS b0 b1 * * part cCROSS b0 b2 ! ! ! c= * * part cCROSS * * part cINTER b0 b1 b2 * * part cINTER * * part cCROSS b0 b2 * * part cCROSS b1 b2
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D FORALL_PAIR_THM
A |- (!P. ((!p. (P p)) = (!p1. (!p2. (P (p1 , p2))))))
T c= ! * f0 b0 ! ! * f0 c, b0 b1
D EXTENSION
A |- (!s. (!t. ((s = t) = (!x. ((x IN s) = (x IN t))))))
T c= c= f0 f1 ! c= cIN b0 f0 cIN b0 f1
D IN_INTER
A |- (!s. (!t. (!x. ((x IN (s INTER t)) = ((x IN s) /\ (x IN t))))))
T c= cIN f0 * * part cINTER f1 f2 c/\ cIN f0 f1 cIN f0 f2
D IN_CROSS
A |- (!x. (!y. (!s. (!t. (((x , y) IN (s CROSS t)) = ((x IN s) /\ (y IN t)))))))
T c= cIN c, f0 f1 * * part cCROSS f2 f3 c/\ cIN f0 f2 cIN f1 f3
+ ((p1 IN t) = T) |- (((p1 IN t) = T) /\ (((p1 IN t) /\ (p2 IN u)) = ((p2 IN u) /\ ((p1 IN t) /\ (p2 IN u)))))
T c==> c= cIN f0 f1 cT c/\ c= cIN f0 f1 cT c= c/\ cIN f0 f1 cIN f2 f3 c/\ cIN f2 f3 c/\ cIN f0 f1 cIN f2 f3
- |- ((!p2. t) = t)
T c= ! f0 f0
