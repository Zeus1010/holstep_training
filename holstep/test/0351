N DIST_FSTCART
C |- (!x. (!y. ((real_le (distance ((fstcart x) , (fstcart y)))) (distance (x , y)))))
T creal_le cdistance c, cfstcart f0 cfstcart f1 cdistance c, f0 f1
D TRUTH
A |- T
T cT
D dist
A |- (!x. (!y. ((distance (x , y)) = (vector_norm ((vector_sub x) y)))))
T c= cdistance c, f0 f1 cvector_norm cvector_sub f0 f1
D FSTCART_SUB
A |- (!x. (!y. ((fstcart ((vector_sub x) y)) = ((vector_sub (fstcart x)) (fstcart y)))))
T c= cfstcart cvector_sub f0 f1 cvector_sub cfstcart f0 cfstcart f1
D NORM_FSTCART
A |- (!x. ((real_le (vector_norm (fstcart x))) (vector_norm x)))
T creal_le cvector_norm cfstcart f0 cvector_norm f0
