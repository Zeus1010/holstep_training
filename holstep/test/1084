N Lp_gen_theory.iso_dart_pairs_trans
C |- (!H. (!G. (!g. (((hyp_iso g) (G , H)) ==> (!P. ((!p. ((p IN ((dart H) CROSS (dart H))) ==> (P p))) ==> (!p. ((p IN ((dart G) CROSS (dart G))) ==> (P ((g (FST p)) , (g (SND p))))))))))))
T c==> chyp_iso f0 c, f1 f2 ! c==> ! c==> cIN b1 * * part cCROSS * part cdart f2 * part cdart f2 * b0 b1 ! c==> cIN b1 * * part cCROSS * part cdart f1 * part cdart f1 * b0 c, * f0 cFST b1 * f0 cSND b1
D TRUTH
A |- T
T cT
D FST
A |- (!x. (!y. ((FST (x , y)) = x)))
T c= cFST c, f0 f1 f0
D SND
A |- (!x. (!y. ((SND (x , y)) = y)))
T c= cSND c, f0 f1 f1
D pair_INDUCT
A |- (!P. ((!x. (!y. (P (x , y)))) ==> (!p. (P p))))
T c==> ! ! * f0 c, b0 b1 ! * f0 b0
D IN_CROSS
A |- (!x. (!y. (!s. (!t. (((x , y) IN (s CROSS t)) = ((x IN s) /\ (y IN t)))))))
T c= cIN c, f0 f1 * * part cCROSS f2 f3 c/\ cIN f0 f2 cIN f1 f3
D Hypermap_iso.hyp_iso_dart
A |- (!H. (!f. (!G. (((hyp_iso f) (H , G)) ==> (!x. ((x IN (dart H)) ==> ((f x) IN (dart G))))))))
T c==> chyp_iso f0 c, f1 f2 ! c==> cIN b0 * part cdart f1 cIN * f0 b0 * part cdart f2
