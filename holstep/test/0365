N REAL_OPEN_REAL_CLOSED
C |- (!s. ((real_open s) = (real_closed (UNIV DIFF s))))
T c= creal_open f0 creal_closed * * part cDIFF part cUNIV f0
D TRUTH
A |- T
T cT
D SUBSET_UNIV
A |- (!s. (s SUBSET UNIV))
T cSUBSET f0 part cUNIV
D OPEN_IN_CLOSED_IN_EQ
A |- (!top. (!s. (((open_in top) s) = ((s SUBSET (topspace top)) /\ ((closed_in top) ((topspace top) DIFF s))))))
T c= copen_in f0 f1 c/\ cSUBSET f1 * part ctopspace f0 cclosed_in f0 * * part cDIFF * part ctopspace f0 f1
D REAL_OPEN_IN
A |- (!s. ((real_open s) = ((open_in euclideanreal) s)))
T c= creal_open f0 copen_in ceuclideanreal f0
D TOPSPACE_EUCLIDEANREAL
A |- ((topspace euclideanreal) = UNIV)
T c= * part ctopspace ceuclideanreal part cUNIV
D REAL_CLOSED_IN
A |- (!s. ((real_closed s) = ((closed_in euclideanreal) s)))
T c= creal_closed f0 cclosed_in ceuclideanreal f0
+ |- ((\s. (((open_in top) s) = ((s SUBSET (topspace top)) /\ ((closed_in top) ((topspace top) DIFF s))))) s)
T * / c= copen_in f0 b0 c/\ cSUBSET b0 * part ctopspace f0 cclosed_in f0 * * part cDIFF * part ctopspace f0 b0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
