N AR_INTERVAL
C |- ((!a. (!b. ((AR (closed_interval ((CONS (a , b)) NIL))) = (~ ((closed_interval ((CONS (a , b)) NIL)) = EMPTY))))) /\ (!a. (!b. ((AR (open_interval (a , b))) = (~ ((open_interval (a , b)) = EMPTY))))))
T c/\ ! ! c= cAR * part cclosed_interval cCONS c, b0 b1 cNIL c~ c= * part cclosed_interval cCONS c, b0 b1 cNIL part cEMPTY ! ! c= cAR * part copen_interval c, b0 b1 c~ c= * part copen_interval c, b0 b1 part cEMPTY
D TRUTH
A |- T
T cT
D CONVEX_INTERVAL
A |- (!a. (!b. ((convex (closed_interval ((CONS (a , b)) NIL))) /\ (convex (open_interval (a , b))))))
T c/\ cconvex * part cclosed_interval cCONS c, f0 f1 cNIL cconvex * part copen_interval c, f0 f1
D CONVEX_IMP_CONTRACTIBLE
A |- (!s. ((convex s) ==> (contractible s)))
T c==> cconvex f0 ccontractible f0
D AR_ANR
A |- (!s. ((AR s) = ((ANR s) /\ ((contractible s) /\ (~ (s = EMPTY))))))
T c= cAR f0 c/\ cANR f0 c/\ ccontractible f0 c~ c= f0 part cEMPTY
D ANR_INTERVAL
A |- ((!a. (!b. (ANR (closed_interval ((CONS (a , b)) NIL))))) /\ (!a. (!b. (ANR (open_interval (a , b))))))
T c/\ ! ! cANR * part cclosed_interval cCONS c, b0 b1 cNIL ! ! cANR * part copen_interval c, b0 b1
+ |- ((\b. ((convex (closed_interval ((CONS (a , b)) NIL))) /\ (convex (open_interval (a , b))))) b)
T * / c/\ cconvex * part cclosed_interval cCONS c, f0 b0 cNIL cconvex * part copen_interval c, f0 b0 f1
+ |- (((!b. ((convex (closed_interval ((CONS (a , b)) NIL))) /\ (convex (open_interval (a , b))))) ==> ((\b. ((convex (closed_interval ((CONS (a , b)) NIL))) /\ (convex (open_interval (a , b))))) b)) = ((\b. ((convex (closed_interval ((CONS (a , b)) NIL))) /\ (convex (open_interval (a , b))))) b))
T c= c==> ! c/\ cconvex * part cclosed_interval cCONS c, f0 b0 cNIL cconvex * part copen_interval c, f0 b0 * / c/\ cconvex * part cclosed_interval cCONS c, f0 b0 cNIL cconvex * part copen_interval c, f0 b0 f1 * / c/\ cconvex * part cclosed_interval cCONS c, f0 b0 cNIL cconvex * part copen_interval c, f0 b0 f1
+ |- ((\s. ((convex s) ==> (contractible s))) s)
T * / c==> cconvex b0 ccontractible b0 f0
+ |- ((!b. t) = t)
T c= ! f0 f0
- |- ((convex (open_interval (a , b))) = T)
T c= cconvex * part copen_interval c, f0 f1 cT
+ |- ((\b. (ANR (closed_interval ((CONS (a , b)) NIL)))) b)
T * / cANR * part cclosed_interval cCONS c, f0 b0 cNIL f1
- |- ((convex (closed_interval ((CONS (a , b)) NIL))) = T)
T c= cconvex * part cclosed_interval cCONS c, f0 f1 cNIL cT
- |- (T = (convex (closed_interval ((CONS (a , b)) NIL))))
T c= cT cconvex * part cclosed_interval cCONS c, f0 f1 cNIL
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- (T = (convex (open_interval (a , b))))
T c= cT cconvex * part copen_interval c, f0 f1
