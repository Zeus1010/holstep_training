N REAL_OPEN_DIFF
C |- (!s. (!t. (((real_open s) /\ (real_closed t)) ==> (real_open (s DIFF t)))))
T c==> c/\ creal_open f0 creal_closed f1 creal_open * * part cDIFF f0 f1
D TRUTH
A |- T
T cT
D OPEN_IN_DIFF
A |- (!top. (!s. (!t. ((((open_in top) s) /\ ((closed_in top) t)) ==> ((open_in top) (s DIFF t))))))
T c==> c/\ copen_in f0 f1 cclosed_in f0 f2 copen_in f0 * * part cDIFF f1 f2
D REAL_OPEN_IN
A |- (!s. ((real_open s) = ((open_in euclideanreal) s)))
T c= creal_open f0 copen_in ceuclideanreal f0
D REAL_CLOSED_IN
A |- (!s. ((real_closed s) = ((closed_in euclideanreal) s)))
T c= creal_closed f0 cclosed_in ceuclideanreal f0
+ |- (((!t. ((((open_in top) s) /\ ((closed_in top) t)) ==> ((open_in top) (s DIFF t)))) ==> ((\t. ((((open_in top) s) /\ ((closed_in top) t)) ==> ((open_in top) (s DIFF t)))) t)) = ((\t. ((((open_in top) s) /\ ((closed_in top) t)) ==> ((open_in top) (s DIFF t)))) t))
T c= c==> ! c==> c/\ copen_in f0 f1 cclosed_in f0 b0 copen_in f0 * * part cDIFF f1 b0 * / c==> c/\ copen_in f0 f1 cclosed_in f0 b0 copen_in f0 * * part cDIFF f1 b0 f2 * / c==> c/\ copen_in f0 f1 cclosed_in f0 b0 copen_in f0 * * part cDIFF f1 b0 f2
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!t. t) = t)
T c= ! f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
