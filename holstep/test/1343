N RSTC_INC_STC
C |- (!R. (!x. (!y. ((((STC R) x) y) ==> (((RSTC R) x) y)))))
T c==> cSTC f0 f1 f2 cRSTC f0 f1 f2
D TRUTH
A |- T
T cT
D RC_INC
A |- (!R. (!x. (!y. (((R x) y) ==> (((RC R) x) y)))))
T c==> * * f0 f1 f2 cRC f0 f1 f2
D STC
A |- (!R. ((STC R) = (TC (SC R))))
T c= * part cSTC f0 * part cTC * part cSC f0
D RSTC
A |- (!R. ((RSTC R) = (RC (TC (SC R)))))
T c= * part cRSTC f0 * part cRC * part cTC * part cSC f0
+ |- ((\y. (((R x) y) ==> (((RC R) x) y))) y)
T * / c==> * * f0 f1 b0 cRC f0 f1 b0 f2
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
