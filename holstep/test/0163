N CONVEX_ON_IS_INDEFINITE_INTEGRAL
C |- (!f. (!s. ((((convex_on (drop o f)) s) /\ ((is_interval s) /\ (open s))) ==> (?f'. ((!x. (!y. (((x IN s) /\ ((y IN s) /\ ((real_le (drop x)) (drop y)))) ==> ((real_le (drop (f' x))) (drop (f' y)))))) /\ (!x. (!y. (((x IN s) /\ ((y IN s) /\ ((real_le (drop x)) (drop y)))) ==> (((has_integral f') ((vector_sub (f y)) (f x))) (closed_interval ((CONS (x , y)) NIL)))))))))))
