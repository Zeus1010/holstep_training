N AR_BALL
C |- (!a. (!r. ((AR (ball (a , r))) = ((real_lt (real_of_num (NUMERAL _0))) r))))
T c= cAR * part cball c, f0 f1 creal_lt creal_of_num cNUMERAL c_0 f1
D TRUTH
A |- T
T cT
D REAL_NOT_LE
A |- (!x. (!y. ((~ ((real_le x) y)) = ((real_lt y) x))))
T c= c~ creal_le f0 f1 creal_lt f1 f0
D BALL_EQ_EMPTY
A |- (!x. (!e. (((ball (x , e)) = EMPTY) = ((real_le e) (real_of_num (NUMERAL _0))))))
T c= c= * part cball c, f0 f1 part cEMPTY creal_le f1 creal_of_num cNUMERAL c_0
D CONVEX_BALL
A |- (!x. (!e. (convex (ball (x , e)))))
T cconvex * part cball c, f0 f1
D CONVEX_IMP_CONTRACTIBLE
A |- (!s. ((convex s) ==> (contractible s)))
T c==> cconvex f0 ccontractible f0
D AR_ANR
A |- (!s. ((AR s) = ((ANR s) /\ ((contractible s) /\ (~ (s = EMPTY))))))
T c= cAR f0 c/\ cANR f0 c/\ ccontractible f0 c~ c= f0 part cEMPTY
D ANR_BALL
A |- (!a. (!r. (ANR (ball (a , r)))))
T cANR * part cball c, f0 f1
+ |- ((\x. (!e. (convex (ball (x , e))))) x)
T * / ! cconvex * part cball c, b0 b1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
