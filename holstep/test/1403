N INT_MAX_LE
C |- (!x. (!y. (!z. (((int_le ((int_max x) y)) z) = (((int_le x) z) /\ ((int_le y) z))))))
T c= cint_le cint_max f0 f1 f2 c/\ cint_le f0 f2 cint_le f1 f2
D REAL_MAX_LE
A |- (!x. (!y. (!z. (((real_le ((real_max x) y)) z) = (((real_le x) z) /\ ((real_le y) z))))))
T c= creal_le creal_max f0 f1 f2 c/\ creal_le f0 f2 creal_le f1 f2
D int_le
A |- (!x. (!y. (((int_le x) y) = ((real_le (real_of_int x)) (real_of_int y)))))
T c= cint_le f0 f1 creal_le creal_of_int f0 creal_of_int f1
D int_max_th
A |- (!x. (!y. ((real_of_int ((int_max x) y)) = ((real_max (real_of_int x)) (real_of_int y)))))
T c= creal_of_int cint_max f0 f1 creal_max creal_of_int f0 creal_of_int f1
