N RSTC_RULES
C |- (!R. ((!x. (!y. (((R x) y) ==> (((RSTC R) x) y)))) /\ ((!x. (((RSTC R) x) x)) /\ ((!x. (!y. ((((RSTC R) x) y) ==> (((RSTC R) y) x)))) /\ (!x. (!y. (!z. (((((RSTC R) x) y) /\ (((RSTC R) y) z)) ==> (((RSTC R) x) z)))))))))
T c/\ ! ! c==> * * f0 b0 b1 cRSTC f0 b0 b1 c/\ ! cRSTC f0 b0 b0 c/\ ! ! c==> cRSTC f0 b0 b1 cRSTC f0 b1 b0 ! ! ! c==> c/\ cRSTC f0 b0 b1 cRSTC f0 b1 b2 cRSTC f0 b0 b2
D TRUTH
A |- T
T cT
D RSTC_INC
A |- (!R. (!x. (!y. (((R x) y) ==> (((RSTC R) x) y)))))
T c==> * * f0 f1 f2 cRSTC f0 f1 f2
D RSTC_REFL
A |- (!R. (!x. (((RSTC R) x) x)))
T cRSTC f0 f1 f1
D RSTC_SYM
A |- (!R. (!x. (!y. ((((RSTC R) x) y) ==> (((RSTC R) y) x)))))
T c==> cRSTC f0 f1 f2 cRSTC f0 f2 f1
D RSTC_TRANS
A |- (!R. (!x. (!y. (!z. (((((RSTC R) x) y) /\ (((RSTC R) y) z)) ==> (((RSTC R) x) z))))))
T c==> c/\ cRSTC f0 f1 f2 cRSTC f0 f2 f3 cRSTC f0 f1 f3
+ |- ((((RSTC R) x) x) = T)
T c= cRSTC f0 f1 f1 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
