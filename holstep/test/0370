N LIFT_IN_INTERVAL
C |- ((!a. (!b. (!x. (((lift x) IN (closed_interval ((CONS (a , b)) NIL))) = (x IN (closed_real_interval ((CONS ((drop a) , (drop b))) NIL))))))) /\ (!a. (!b. (!x. (((lift x) IN (open_interval (a , b))) = (x IN (open_real_interval ((drop a) , (drop b)))))))))
T c/\ ! ! ! c= cIN clift b2 * part cclosed_interval cCONS c, b0 b1 cNIL cIN b2 * part cclosed_real_interval cCONS c, cdrop b0 cdrop b1 cNIL ! ! ! c= cIN clift b2 * part copen_interval c, b0 b1 cIN b2 * part copen_real_interval c, cdrop b0 cdrop b1
D TRUTH
A |- T
T cT
D LIFT_DROP
A |- ((!x. ((lift (drop x)) = x)) /\ (!x. ((drop (lift x)) = x)))
T c/\ ! c= clift cdrop b0 b0 ! c= cdrop clift b0 b0
D FORALL_DROP
A |- ((!x. (P x)) = (!x. (P (drop x))))
T c= ! * f0 b0 ! * f0 cdrop b0
D DROP_IN_REAL_INTERVAL
A |- ((!a. (!b. (!x. (((drop x) IN (closed_real_interval ((CONS (a , b)) NIL))) = (x IN (closed_interval ((CONS ((lift a) , (lift b))) NIL))))))) /\ (!a. (!b. (!x. (((drop x) IN (open_real_interval (a , b))) = (x IN (open_interval ((lift a) , (lift b)))))))))
