+ |- (((!b. (!x. (((drop x) IN (open_real_interval (a , b))) = (x IN (open_interval ((lift a) , (lift b))))))) ==> ((\b. (!x. (((drop x) IN (open_real_interval (a , b))) = (x IN (open_interval ((lift a) , (lift b))))))) b)) = ((\b. (!x. (((drop x) IN (open_real_interval (a , b))) = (x IN (open_interval ((lift a) , (lift b))))))) b))
T c= c==> ! ! c= cIN cdrop b1 * part copen_real_interval c, f0 b0 cIN b1 * part copen_interval c, clift f0 clift b0 * / ! c= cIN cdrop b1 * part copen_real_interval c, f0 b0 cIN b1 * part copen_interval c, clift f0 clift b0 f1 * / ! c= cIN cdrop b1 * part copen_real_interval c, f0 b0 cIN b1 * part copen_interval c, clift f0 clift b0 f1
