N VECTOR_DIFF_CHAIN_AT
C |- (!f. (!g. (!f'. (!g'. (!x. (((((has_vector_derivative f) f') (at x)) /\ (((has_vector_derivative g) g') (at (f x)))) ==> (((has_vector_derivative (g o f)) ((% (drop f')) g')) (at x))))))))
T c==> c/\ chas_vector_derivative f0 f1 cat f2 chas_vector_derivative f3 f4 cat * f0 f2 chas_vector_derivative * * part co f3 f0 c% cdrop f1 f4 cat f2
D TRUTH
A |- T
T cT
D o_DEF
A |- (!f. (!g. ((f o g) = (\x. (f (g x))))))
T c= * * part co f0 f1 / * f0 * f1 b0
D VECTOR_MUL_ASSOC
A |- (!a. (!b. (!x. (((% a) ((% b) x)) = ((% ((real_mul a) b)) x)))))
T c= c% f0 c% f1 f2 c% creal_mul f0 f1 f2
D DROP_CMUL
A |- (!x. (!c. ((drop ((% c) x)) = ((real_mul c) (drop x)))))
T c= cdrop c% f0 f1 creal_mul f0 cdrop f1
D DIFF_CHAIN_AT
A |- (!f. (!g. (!f'. (!g'. (!x. (((((has_derivative f) f') (at x)) /\ (((has_derivative g) g') (at (f x)))) ==> (((has_derivative (g o f)) (g' o f')) (at x))))))))
T c==> c/\ chas_derivative f0 f1 cat f2 chas_derivative f3 f4 cat * f0 f2 chas_derivative * * part co f3 f0 * * part co f4 f1 cat f2
D has_vector_derivative
A |- (!f. (!f'. (!net. ((((has_vector_derivative f) f') net) = (((has_derivative f) (\x. ((% (drop x)) f'))) net)))))
T c= chas_vector_derivative f0 f1 f2 chas_derivative f0 / c% cdrop b0 f1 f2
+ |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- (((!f. (!g. ((f o g) = (\x. (f (g x)))))) ==> ((\f. (!g. ((f o g) = (\x. (f (g x)))))) f)) = ((\f. (!g. ((f o g) = (\x. (f (g x)))))) f))
T c= c==> ! ! c= * * part co b0 b1 / * b0 * b1 b2 * / ! c= * * part co b0 b1 / * b0 * b1 b2 f0 * / ! c= * * part co b0 b1 / * b0 * b1 b2 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- (((\x. (f x)) y) = (f y))
T c= * / * f0 b0 f1 * f0 f1
