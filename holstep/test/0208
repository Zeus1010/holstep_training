N FRONTIER_SPHERE
C |- (!a. (!r. ((frontier (sphere (a , r))) = (sphere (a , r)))))
T c= * part cfrontier * part csphere c, f0 f1 * part csphere c, f0 f1
D TRUTH
A |- T
T cT
D DIFF_EMPTY
A |- (!s. ((s DIFF EMPTY) = s))
T c= * * part cDIFF f0 part cEMPTY f0
D frontier
A |- (!s. ((frontier s) = ((closure s) DIFF (interior s))))
T c= * part cfrontier f0 * * part cDIFF * part cclosure f0 * part cinterior f0
D INTERIOR_SPHERE
A |- (!a. (!r. ((interior (sphere (a , r))) = EMPTY)))
T c= * part cinterior * part csphere c, f0 f1 part cEMPTY
D CLOSURE_SPHERE
A |- (!a. (!r. ((closure (sphere (a , r))) = (sphere (a , r)))))
T c= * part cclosure * part csphere c, f0 f1 * part csphere c, f0 f1
