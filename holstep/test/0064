N Hypermap_iso.finite_inj_eq_bij
C |- (!s. (!t. ((FINITE s) ==> ((FINITE t) ==> (((CARD s) = (CARD t)) ==> (!f. ((((INJ f) s) t) = (((BIJ f) s) t))))))))
T c==> cFINITE f0 c==> cFINITE f1 c==> c= cCARD f0 cCARD f1 ! c= cINJ b0 f0 f1 cBIJ b0 f0 f1
D TRUTH
A |- T
T cT
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D INJ
A |- (!t. (!s. (!f. ((((INJ f) s) t) = ((!x. ((x IN s) ==> ((f x) IN t))) /\ (!x. (!y. (((x IN s) /\ ((y IN s) /\ ((f x) = (f y)))) ==> (x = y)))))))))
T c= cINJ f0 f1 f2 c/\ ! c==> cIN b0 f1 cIN * f0 b0 f2 ! ! c==> c/\ cIN b0 f1 c/\ cIN b1 f1 c= * f0 b0 * f0 b1 c= b0 b1
D SURJ
A |- (!t. (!s. (!f. ((((SURJ f) s) t) = ((!x. ((x IN s) ==> ((f x) IN t))) /\ (!x. ((x IN t) ==> (?y. ((y IN s) /\ ((f y) = x))))))))))
T c= cSURJ f0 f1 f2 c/\ ! c==> cIN b0 f1 cIN * f0 b0 f2 ! c==> cIN b0 f2 c? / c/\ cIN b1 f1 c= * f0 b1 b0
D BIJ
A |- (!f. (!s. (!t. ((((BIJ f) s) t) = ((((INJ f) s) t) /\ (((SURJ f) s) t))))))
T c= cBIJ f0 f1 f2 c/\ cINJ f0 f1 f2 cSURJ f0 f1 f2
D IN_IMAGE
A |- (!y. (!s. (!f. ((y IN ((IMAGE f) s)) = (?x. ((y = (f x)) /\ (x IN s)))))))
T c= cIN f0 * * part cIMAGE f1 f2 c? / c/\ c= f0 * f1 b0 cIN b0 f2
D SURJECTIVE_IFF_INJECTIVE_GEN
A |- (!s. (!t. (!f. (((FINITE s) /\ ((FINITE t) /\ (((CARD s) = (CARD t)) /\ (((IMAGE f) s) SUBSET t)))) ==> ((!y. ((y IN t) ==> (?x. ((x IN s) /\ ((f x) = y))))) = (!x. (!y. (((x IN s) /\ ((y IN s) /\ ((f x) = (f y)))) ==> (x = y)))))))))
T c==> c/\ cFINITE f0 c/\ cFINITE f1 c/\ c= cCARD f0 cCARD f1 cSUBSET * * part cIMAGE f2 f0 f1 c= ! c==> cIN b0 f1 c? / c/\ cIN b1 f0 c= * f2 b1 b0 ! ! c==> c/\ cIN b0 f0 c/\ cIN b1 f0 c= * f2 b0 * f2 b1 c= b0 b1
D Ssrbool.andbb
A |- (!b. ((b /\ b) = b))
T c= c/\ f0 f0 f0
D Ssrbool.andb_id2l
A |- (!a. (!b. (!c. ((a ==> (b = c)) ==> ((a /\ b) = (a /\ c))))))
T c==> c==> f0 c= f1 f2 c= c/\ f0 f1 c/\ f0 f2
