N INT_POW_EQ_ABS
C |- (!n. (!x. (!y. (((~ (n = (NUMERAL _0))) /\ (((int_pow x) n) = ((int_pow y) n))) ==> ((int_abs x) = (int_abs y))))))
T c==> c/\ c~ c= f0 cNUMERAL c_0 c= cint_pow f1 f0 cint_pow f2 f0 c= cint_abs f1 cint_abs f2
D REAL_POW_EQ_ABS
A |- (!n. (!x. (!y. (((~ (n = (NUMERAL _0))) /\ (((real_pow x) n) = ((real_pow y) n))) ==> ((real_abs x) = (real_abs y))))))
T c==> c/\ c~ c= f0 cNUMERAL c_0 c= creal_pow f1 f0 creal_pow f2 f0 c= creal_abs f1 creal_abs f2
D int_eq
A |- (!x. (!y. ((x = y) = ((real_of_int x) = (real_of_int y)))))
T c= c= f0 f1 c= creal_of_int f0 creal_of_int f1
D int_abs_th
A |- (!x. ((real_of_int (int_abs x)) = (real_abs (real_of_int x))))
T c= creal_of_int cint_abs f0 creal_abs creal_of_int f0
D int_pow_th
A |- (!x. (!n. ((real_of_int ((int_pow x) n)) = ((real_pow (real_of_int x)) n))))
T c= creal_of_int cint_pow f0 f1 creal_pow creal_of_int f0 f1
