D REAL_LE_LMUL_EQ
A |- (!x. (!y. (!z. (((real_lt (real_of_num (NUMERAL _0))) z) ==> (((real_le ((real_mul z) x)) ((real_mul z) y)) = ((real_le x) y))))))
