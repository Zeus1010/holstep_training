N VECTOR_VARIATION_INTEGRAL_NORM_DERIVATIVE
C |- (!f. (!s. (!a. (!b. (((COUNTABLE s) /\ (((continuous_on f) (closed_interval ((CONS (a , b)) NIL))) /\ ((!x. ((x IN ((closed_interval ((CONS (a , b)) NIL)) DIFF s)) ==> ((differentiable f) (at x)))) /\ ((has_bounded_variation_on f) (closed_interval ((CONS (a , b)) NIL)))))) ==> (((vector_variation (closed_interval ((CONS (a , b)) NIL))) f) = (drop ((integral (closed_interval ((CONS (a , b)) NIL))) (\x. (lift (vector_norm ((vector_derivative f) (at x)))))))))))))
