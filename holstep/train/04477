T c==> c/\ ccontinuous_on f0 f1 cSUBSET f2 f1 ccontinuous_on f0 f2
D INTERVAL_NE_EMPTY_1
A |- ((!a. (!b. ((~ ((closed_interval ((CONS (a , b)) NIL)) = EMPTY)) = ((real_le (drop a)) (drop b))))) /\ (!a. (!b. ((~ ((open_interval (a , b)) = EMPTY)) = ((real_lt (drop a)) (drop b))))))
