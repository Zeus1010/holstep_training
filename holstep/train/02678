N OPEN_CLOSED
C |- (!s. ((open s) = (closed (UNIV DIFF s))))
T c= copen f0 cclosed * * part cDIFF part cUNIV f0
D TRUTH
A |- T
T cT
D SUBSET_UNIV
A |- (!s. (s SUBSET UNIV))
T cSUBSET f0 part cUNIV
D OPEN_IN_CLOSED_IN_EQ
A |- (!top. (!s. (((open_in top) s) = ((s SUBSET (topspace top)) /\ ((closed_in top) ((topspace top) DIFF s))))))
T c= copen_in f0 f1 c/\ cSUBSET f1 * part ctopspace f0 cclosed_in f0 * * part cDIFF * part ctopspace f0 f1
D OPEN_IN
A |- (!s. ((open s) = ((open_in euclidean) s)))
T c= copen f0 copen_in ceuclidean f0
D TOPSPACE_EUCLIDEAN
A |- ((topspace euclidean) = UNIV)
T c= * part ctopspace ceuclidean part cUNIV
D CLOSED_IN
A |- (!s. ((closed s) = ((closed_in euclidean) s)))
T c= cclosed f0 cclosed_in ceuclidean f0
+ |- ((\s. (((open_in top) s) = ((s SUBSET (topspace top)) /\ ((closed_in top) ((topspace top) DIFF s))))) s)
T * / c= copen_in f0 b0 c/\ cSUBSET b0 * part ctopspace f0 cclosed_in f0 * * part cDIFF * part ctopspace f0 b0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
