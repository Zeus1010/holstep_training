N CONNECTED_LINEAR_IMAGE
C |- (!f. (!s. (((connected s) /\ (linear f)) ==> (connected ((IMAGE f) s)))))
T c==> c/\ cconnected f0 clinear f1 cconnected * * part cIMAGE f1 f0
D TRUTH
A |- T
T cT
D LINEAR_CONTINUOUS_ON
A |- (!f. (!s. ((linear f) ==> ((continuous_on f) s))))
T c==> clinear f0 ccontinuous_on f0 f1
D CONNECTED_CONTINUOUS_IMAGE
A |- (!f. (!s. ((((continuous_on f) s) /\ (connected s)) ==> (connected ((IMAGE f) s)))))
T c==> c/\ ccontinuous_on f0 f1 cconnected f1 cconnected * * part cIMAGE f0 f1
+ |- (((!s. ((linear f) ==> ((continuous_on f) s))) ==> ((\s. ((linear f) ==> ((continuous_on f) s))) s)) = ((\s. ((linear f) ==> ((continuous_on f) s))) s))
T c= c==> ! c==> clinear f0 ccontinuous_on f0 b0 * / c==> clinear f0 ccontinuous_on f0 b0 f1 * / c==> clinear f0 ccontinuous_on f0 b0 f1
+ |- ((\f. (!s. ((linear f) ==> ((continuous_on f) s)))) f)
T * / ! c==> clinear b0 ccontinuous_on b0 b1 f0
+ |- ((\q'. ((((connected s) /\ (linear f)) = p') ==> ((p' ==> ((connected ((IMAGE f) s)) = q')) ==> ((((connected s) /\ (linear f)) ==> (connected ((IMAGE f) s))) = (p' ==> q'))))) q')
T * / c==> c= c/\ cconnected f0 clinear f1 f2 c==> c==> f2 c= cconnected * * part cIMAGE f1 f0 b0 c= c==> c/\ cconnected f0 clinear f1 cconnected * * part cIMAGE f1 f0 c==> f2 b0 f3
+ |- ((((continuous_on f) s) /\ (connected s)) ==> ((connected ((IMAGE f) s)) = T))
T c==> c/\ ccontinuous_on f0 f1 cconnected f1 c= cconnected * * part cIMAGE f0 f1 cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((connected s) /\ (linear f)) |- ((linear f) = T)
T c==> c/\ cconnected f0 clinear f1 c= clinear f1 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- ((connected s) /\ (linear f)) |- (T = (linear f))
T c==> c/\ cconnected f0 clinear f1 c= cT clinear f1
