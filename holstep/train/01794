N OPEN_PCROSS
C |- (!s. (!t. (((open s) /\ (open t)) ==> (open (s PCROSS t)))))
T c==> c/\ copen f0 copen f1 copen * * part cPCROSS f0 f1
D TRUTH
A |- T
T cT
D OPEN_PCROSS_EQ
A |- (!s. (!t. ((open (s PCROSS t)) = ((s = EMPTY) \/ ((t = EMPTY) \/ ((open s) /\ (open t)))))))
T c= copen * * part cPCROSS f0 f1 c\/ c= f0 part cEMPTY c\/ c= f1 part cEMPTY c/\ copen f0 copen f1
+ |- (((!t. ((open (s PCROSS t)) = ((s = EMPTY) \/ ((t = EMPTY) \/ ((open s) /\ (open t)))))) ==> ((\t. ((open (s PCROSS t)) = ((s = EMPTY) \/ ((t = EMPTY) \/ ((open s) /\ (open t)))))) t)) = ((\t. ((open (s PCROSS t)) = ((s = EMPTY) \/ ((t = EMPTY) \/ ((open s) /\ (open t)))))) t))
T c= c==> ! c= copen * * part cPCROSS f0 b0 c\/ c= f0 part cEMPTY c\/ c= b0 part cEMPTY c/\ copen f0 copen b0 * / c= copen * * part cPCROSS f0 b0 c\/ c= f0 part cEMPTY c\/ c= b0 part cEMPTY c/\ copen f0 copen b0 f1 * / c= copen * * part cPCROSS f0 b0 c\/ c= f0 part cEMPTY c\/ c= b0 part cEMPTY c/\ copen f0 copen b0 f1
+ |- ((((open s) /\ (open t)) = p') ==> ((p' ==> ((open (s PCROSS t)) = q')) ==> ((((open s) /\ (open t)) ==> (open (s PCROSS t))) = (p' ==> q'))))
T c==> c= c/\ copen f0 copen f1 f2 c==> c==> f2 c= copen * * part cPCROSS f0 f1 f3 c= c==> c/\ copen f0 copen f1 copen * * part cPCROSS f0 f1 c==> f2 f3
+ |- ((t \/ T) = T)
T c= c\/ f0 cT cT
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
- |- ((x = x) = T)
T c= c= f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
