N LINEAR_CX_IM
C |- (linear (Cx o Im))
T clinear * * part co part cCx part cIm
D TRUTH
A |- T
T cT
D o_THM
A |- (!f. (!g. (!x. (((f o g) x) = (f (g x))))))
T c= co f0 f1 f2 * f0 * f1 f2
D linear
A |- (!f. ((linear f) = ((!x. (!y. ((f ((vector_add x) y)) = ((vector_add (f x)) (f y))))) /\ (!c. (!x. ((f ((% c) x)) = ((% c) (f x))))))))
T c= clinear f0 c/\ ! ! c= * f0 cvector_add b0 b1 cvector_add * f0 b0 * f0 b1 ! ! c= * f0 c% b0 b1 c% b0 * f0 b1
D CX_ADD
A |- (!x. (!y. ((Cx ((real_add x) y)) = ((vector_add (Cx x)) (Cx y)))))
T c= cCx creal_add f0 f1 cvector_add cCx f0 cCx f1
D CX_MUL
A |- (!x. (!y. ((Cx ((real_mul x) y)) = ((complex_mul (Cx x)) (Cx y)))))
T c= cCx creal_mul f0 f1 ccomplex_mul cCx f0 cCx f1
D IM_ADD
A |- (!x. (!y. ((Im ((vector_add x) y)) = ((real_add (Im x)) (Im y)))))
T c= cIm cvector_add f0 f1 creal_add cIm f0 cIm f1
D IM_MUL_CX
A |- (!x. (!z. (((Im ((complex_mul (Cx x)) z)) = ((real_mul x) (Im z))) /\ ((Im ((complex_mul z) (Cx x))) = ((real_mul (Im z)) x)))))
T c/\ c= cIm ccomplex_mul cCx f0 f1 creal_mul f0 cIm f1 c= cIm ccomplex_mul f1 cCx f0 creal_mul cIm f1 f0
D COMPLEX_CMUL
A |- (!c. (!x. (((% c) x) = ((complex_mul (Cx c)) x))))
T c= c% f0 f1 ccomplex_mul cCx f0 f1
+ |- ((\x. (!z. (((Im ((complex_mul (Cx x)) z)) = ((real_mul x) (Im z))) /\ ((Im ((complex_mul z) (Cx x))) = ((real_mul (Im z)) x))))) x)
T * / ! c/\ c= cIm ccomplex_mul cCx b0 b1 creal_mul b0 cIm b1 c= cIm ccomplex_mul b1 cCx b0 creal_mul cIm b1 b0 f0
+ |- (((!c. (!x. (((% c) x) = ((complex_mul (Cx c)) x)))) ==> ((\c. (!x. (((% c) x) = ((complex_mul (Cx c)) x)))) c)) = ((\c. (!x. (((% c) x) = ((complex_mul (Cx c)) x)))) c))
T c= c==> ! ! c= c% b0 b1 ccomplex_mul cCx b0 b1 * / ! c= c% b0 b1 ccomplex_mul cCx b0 b1 f0 * / ! c= c% b0 b1 ccomplex_mul cCx b0 b1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- ((Im ((complex_mul z) (Cx x))) = ((real_mul (Im z)) x))
T c= cIm ccomplex_mul f0 cCx f1 creal_mul cIm f0 f1
