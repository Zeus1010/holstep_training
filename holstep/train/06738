N REAL_CONTINUOUS_AT
C |- (!f. (!x. (((real_continuous f) (at x)) = (((---> f) (f x)) (at x)))))
T c= creal_continuous f0 cat f1 c---> f0 * f0 f1 cat f1
D TRUTH
A |- T
T cT
D WITHIN_UNIV
A |- (!x. (((within (at x)) UNIV) = (at x)))
T c= cwithin cat f0 part cUNIV cat f0
D REAL_CONTINUOUS_WITHIN
A |- (!f. (!x. (!s. (((real_continuous f) ((within (at x)) s)) = (((---> f) (f x)) ((within (at x)) s))))))
T c= creal_continuous f0 cwithin cat f1 f2 c---> f0 * f0 f1 cwithin cat f1 f2
- |- ((\x. (x IN UNIV)) x)
T * / cIN b0 part cUNIV f0
+ |- ((!x. t) = t)
T c= ! f0 f0
+ |- ((\x. ((at x) = ((within (at x)) UNIV))) x)
T * / c= cat b0 cwithin cat b0 part cUNIV f0
+ |- (((!x. ((at x) = ((within (at x)) UNIV))) ==> ((\x. ((at x) = ((within (at x)) UNIV))) x)) = ((\x. ((at x) = ((within (at x)) UNIV))) x))
T c= c==> ! c= cat b0 cwithin cat b0 part cUNIV * / c= cat b0 cwithin cat b0 part cUNIV f0 * / c= cat b0 cwithin cat b0 part cUNIV f0
+ |- (((!x. (!s. (((real_continuous f) ((within (at x)) s)) = (((---> f) (f x)) ((within (at x)) s))))) ==> ((\x. (!s. (((real_continuous f) ((within (at x)) s)) = (((---> f) (f x)) ((within (at x)) s))))) x)) = ((\x. (!s. (((real_continuous f) ((within (at x)) s)) = (((---> f) (f x)) ((within (at x)) s))))) x))
T c= c==> ! ! c= creal_continuous f0 cwithin cat b0 b1 c---> f0 * f0 b0 cwithin cat b0 b1 * / ! c= creal_continuous f0 cwithin cat b0 b1 c---> f0 * f0 b0 cwithin cat b0 b1 f1 * / ! c= creal_continuous f0 cwithin cat b0 b1 c---> f0 * f0 b0 cwithin cat b0 b1 f1
- |- ((x IN UNIV) = T)
T c= cIN f0 part cUNIV cT
- |- (((!x. (x IN UNIV)) ==> ((\x. (x IN UNIV)) x)) = ((\x. (x IN UNIV)) x))
T c= c==> ! cIN b0 part cUNIV * / cIN b0 part cUNIV f0 * / cIN b0 part cUNIV f0
- |- (x IN UNIV)
T cIN f0 part cUNIV
