N Hypermap.lemma_in_couple
C |- (!x. (!a. (!b. ((x IN (a INSERT (b INSERT EMPTY))) = ((x = a) \/ (x = b))))))
T c= cIN f0 * * part cINSERT f1 * * part cINSERT f2 part cEMPTY c\/ c= f0 f1 c= f0 f2
D TRUTH
A |- T
T cT
D NOT_IN_EMPTY
A |- (!x. (~ (x IN EMPTY)))
T c~ cIN f0 part cEMPTY
D IN_INSERT
A |- (!x. (!y. (!s. ((x IN (y INSERT s)) = ((x = y) \/ (x IN s))))))
T c= cIN f0 * * part cINSERT f1 f2 c\/ c= f0 f1 cIN f0 f2
+ |- ((t \/ F) = t)
T c= c\/ f0 cF f0
- |- ((\t1. (!t2. (!t3. ((t1 \/ (t2 \/ t3)) = ((t1 \/ t2) \/ t3))))) t1)
T * / ! ! c= c\/ b0 c\/ b1 b2 c\/ c\/ b0 b1 b2 f0
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
- |- ((\t3. ((t1 \/ (t2 \/ t3)) = ((t1 \/ t2) \/ t3))) t3)
T * / c= c\/ f0 c\/ f1 b0 c\/ c\/ f0 f1 b0 f2
+ |- ((!b. t) = t)
T c= ! f0 f0
- |- ((t1 \/ (t2 \/ t3)) = ((t1 \/ t2) \/ t3))
T c= c\/ f0 c\/ f1 f2 c\/ c\/ f0 f1 f2
- |- ((\t2. (!t3. ((t1 \/ (t2 \/ t3)) = ((t1 \/ t2) \/ t3)))) t2)
T * / ! c= c\/ f0 c\/ b0 b1 c\/ c\/ f0 b0 b1 f1
+ |- ((x = x) = T)
T c= c= f0 f0 cT
+ |- (T = (!x. (!a. (!b. ((x IN (a INSERT (b INSERT EMPTY))) = ((x = a) \/ (x = b)))))))
T c= cT ! ! ! c= cIN b0 * * part cINSERT b1 * * part cINSERT b2 part cEMPTY c\/ c= b0 b1 c= b0 b2
+ |- ((x IN EMPTY) = F)
T c= cIN f0 part cEMPTY cF
