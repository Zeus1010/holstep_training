N ONORM_NEG
C |- (!f. ((linear f) ==> ((onorm (\x. (vector_neg (f x)))) = (onorm f))))
T c==> clinear f0 c= conorm / cvector_neg * f0 b0 conorm f0
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D REAL_LE_ANTISYM
A |- (!x. (!y. ((((real_le x) y) /\ ((real_le y) x)) = (x = y))))
T c= c/\ creal_le f0 f1 creal_le f1 f0 c= f0 f1
D VECTOR_NEG_NEG
A |- ((vector_neg (vector_neg x)) = x)
T c= cvector_neg cvector_neg f0 f0
D LINEAR_COMPOSE_NEG
A |- (!f. ((linear f) ==> (linear (\x. (vector_neg (f x))))))
T c==> clinear f0 clinear / cvector_neg * f0 b0
D ONORM_NEG_LEMMA
A |- (!f. ((linear f) ==> ((real_le (onorm (\x. (vector_neg (f x))))) (onorm f))))
T c==> clinear f0 creal_le conorm / cvector_neg * f0 b0 conorm f0
+ (linear f), (f = (\x. (vector_neg (vector_neg (f x))))) |- ((real_le (onorm f)) (onorm (\x. (vector_neg (f x)))))
T c==> clinear f0 c==> c= f0 / cvector_neg cvector_neg * f0 b0 creal_le conorm f0 conorm / cvector_neg * f0 b0
+ |- (f = (\x. (vector_neg (vector_neg (f x)))))
T c= f0 / cvector_neg cvector_neg * f0 b0
+ |- (((!y. ((x = y) = (((real_le x) y) /\ ((real_le y) x)))) ==> ((\y. ((x = y) = (((real_le x) y) /\ ((real_le y) x)))) y)) = ((\y. ((x = y) = (((real_le x) y) /\ ((real_le y) x)))) y))
T c= c==> ! c= c= f0 b0 c/\ creal_le f0 b0 creal_le b0 f0 * / c= c= f0 b0 c/\ creal_le f0 b0 creal_le b0 f0 f1 * / c= c= f0 b0 c/\ creal_le f0 b0 creal_le b0 f0 f1
- (linear f) |- ((linear f) = T)
T c==> clinear f0 c= clinear f0 cT
- (linear f) |- (T = (linear f))
T c==> clinear f0 c= cT clinear f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
