N IS_INTERVAL_PATH_CONNECTED
C |- (!s. ((is_interval s) ==> (path_connected s)))
T c==> cis_interval f0 cpath_connected f0
D TRUTH
A |- T
T cT
D IS_INTERVAL_CONVEX
A |- (!s. ((is_interval s) ==> (convex s)))
T c==> cis_interval f0 cconvex f0
D CONVEX_IMP_PATH_CONNECTED
A |- (!s. ((convex s) ==> (path_connected s)))
T c==> cconvex f0 cpath_connected f0
- (is_interval s) |- (T = (is_interval s))
T c==> cis_interval f0 c= cT cis_interval f0
+ |- ((convex s) ==> ((path_connected s) = T))
T c==> cconvex f0 c= cpath_connected f0 cT
+ (is_interval s) |- ((convex s) = T)
T c==> cis_interval f0 c= cconvex f0 cT
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (is_interval s) |- ((is_interval s) = T)
T c==> cis_interval f0 c= cis_interval f0 cT
