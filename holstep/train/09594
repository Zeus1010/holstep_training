N Packing3.AFFINE_BIS
C |- (!a. (!b. (affine ((bis a) b))))
T caffine * * part cbis f0 f1
D TRUTH
A |- T
T cT
D AFFINE_HYPERPLANE
A |- (!a. (!b. (affine (GSPEC (\GEN%PVAR%1401. (?x. (((SETSPEC GEN%PVAR%1401) (((dot a) x) = b)) x)))))))
T caffine * part cGSPEC / c? / cSETSPEC b0 c= cdot f0 b1 f1 b1
D Packing3.BIS_EQ_HYPERPLANE
A |- (!u. (!v. (((bis u) v) = (GSPEC (\GEN%PVAR%6025. (?x. (((SETSPEC GEN%PVAR%6025) (((dot ((% (real_of_num (NUMERAL (BIT0 (BIT1 _0))))) ((vector_sub v) u))) x) = ((real_sub ((dot v) v)) ((dot u) u)))) x)))))))
T c= * * part cbis f0 f1 * part cGSPEC / c? / cSETSPEC b0 c= cdot c% creal_of_num cNUMERAL cBIT0 cBIT1 c_0 cvector_sub f1 f0 b1 creal_sub cdot f1 f1 cdot f0 f0 b1
+ |- ((\v. (((bis u) v) = (GSPEC (\GEN%PVAR%6025. (?x. (((SETSPEC GEN%PVAR%6025) (((dot ((% (real_of_num (NUMERAL (BIT0 (BIT1 _0))))) ((vector_sub v) u))) x) = ((real_sub ((dot v) v)) ((dot u) u)))) x)))))) v)
T * / c= * * part cbis f0 b0 * part cGSPEC / c? / cSETSPEC b1 c= cdot c% creal_of_num cNUMERAL cBIT0 cBIT1 c_0 cvector_sub b0 f0 b2 creal_sub cdot b0 b0 cdot f0 f0 b2 f1
+ |- ((!b. t) = t)
T c= ! f0 f0
- |- ((?x. t) = t)
T c= c? / f0 f0
- |- ((x = x) = T)
T c= c= f0 f0 cT
