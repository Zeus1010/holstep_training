N EL_TL
C |- (!n. (((EL n) (TL l)) = ((EL (n + (NUMERAL (BIT1 _0)))) l)))
T c= cEL f0 cTL f1 cEL c+ f0 cNUMERAL cBIT1 c_0 f1
D TRUTH
A |- T
T cT
D ADD1
A |- (!m. ((SUC m) = (m + (NUMERAL (BIT1 _0)))))
T c= cSUC f0 c+ f0 cNUMERAL cBIT1 c_0
D EL
A |- ((((EL (NUMERAL _0)) l) = (HD l)) /\ (((EL (SUC n)) l) = ((EL n) (TL l))))
T c/\ c= cEL cNUMERAL c_0 f0 cHD f0 c= cEL cSUC f1 f0 cEL f1 cTL f0
+ |- ((!n. t) = t)
T c= ! f0 f0
- |- (((EL (NUMERAL _0)) l) = (HD l))
T c= cEL cNUMERAL c_0 f0 cHD f0
