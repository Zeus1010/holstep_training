N PATH_CONNECTED_EQ_CONNECTED
C |- (!s. ((open s) ==> ((path_connected s) = (connected s))))
T c==> copen f0 c= cpath_connected f0 cconnected f0
D TRUTH
A |- T
T cT
D OPEN_IMP_LOCALLY_PATH_CONNECTED
A |- (!s. ((open s) ==> ((locally path_connected) s)))
T c==> copen f0 clocally part cpath_connected f0
D PATH_CONNECTED_EQ_CONNECTED_LPC
A |- (!s. (((locally path_connected) s) ==> ((path_connected s) = (connected s))))
T c==> clocally part cpath_connected f0 c= cpath_connected f0 cconnected f0
+ |- (((open s) ==> ((path_connected s) = (connected s))) = ((open s) ==> T))
T c= c==> copen f0 c= cpath_connected f0 cconnected f0 c==> copen f0 cT
+ |- (((!s. ((open s) ==> ((locally path_connected) s))) ==> ((\s. ((open s) ==> ((locally path_connected) s))) s)) = ((\s. ((open s) ==> ((locally path_connected) s))) s))
T c= c==> ! c==> copen b0 clocally part cpath_connected b0 * / c==> copen b0 clocally part cpath_connected b0 f0 * / c==> copen b0 clocally part cpath_connected b0 f0
+ (open s) |- ((open s) /\ (((path_connected s) = (connected s)) = T))
T c==> copen f0 c/\ copen f0 c= c= cpath_connected f0 cconnected f0 cT
+ (open s) |- (((locally path_connected) s) = T)
T c==> copen f0 c= clocally part cpath_connected f0 cT
- (open s) |- (T = (open s))
T c==> copen f0 c= cT copen f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((locally path_connected) s) |- (((locally path_connected) s) /\ ((path_connected s) = (connected s)))
T c==> clocally part cpath_connected f0 c/\ clocally part cpath_connected f0 c= cpath_connected f0 cconnected f0
- (open s) |- ((open s) = T)
T c==> copen f0 c= copen f0 cT
