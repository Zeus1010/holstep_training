D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D IN
A |- (!P. (!x. ((x IN P) = (P x))))
T c= cIN f0 f1 * f1 f0
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D IN_DIFF
A |- (!s. (!t. (!x. ((x IN (s DIFF t)) = ((x IN s) /\ (~ (x IN t)))))))
T c= cIN f0 * * part cDIFF f1 f2 c/\ cIN f0 f1 c~ cIN f0 f2
D EMPTY_SUBSET
A |- (!s. (EMPTY SUBSET s))
T cSUBSET part cEMPTY f0
D UNION_SUBSET
A |- (!s. (!t. (!u. (((s UNION t) SUBSET u) = ((s SUBSET u) /\ (t SUBSET u))))))
T c= cSUBSET * * part cUNION f0 f1 f2 c/\ cSUBSET f0 f2 cSUBSET f1 f2
D INSERT_SUBSET
A |- (!x. (!s. (!t. (((x INSERT s) SUBSET t) = ((x IN t) /\ (s SUBSET t))))))
T c= cSUBSET * * part cINSERT f0 f1 f2 c/\ cIN f0 f2 cSUBSET f1 f2
D SEGMENT_CLOSED_OPEN
A |- (!a. (!b. ((closed_segment ((CONS (a , b)) NIL)) = ((open_segment (a , b)) UNION (a INSERT (b INSERT EMPTY))))))
T c= * part cclosed_segment cCONS c, f0 f1 cNIL * * part cUNION * part copen_segment c, f0 f1 * * part cINSERT f0 * * part cINSERT f1 part cEMPTY
D relative_frontier
A |- (!s. ((relative_frontier s) = ((closure s) DIFF (relative_interior s))))
T c= * part crelative_frontier f0 * * part cDIFF * part cclosure f0 * part crelative_interior f0
