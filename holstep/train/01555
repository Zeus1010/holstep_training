N SEGMENT_SUBSET_RELATIVE_FRONTIER_CONVEX
C |- (!s. (!a. (!b. (!c. (((convex s) /\ ((c IN (open_segment (a , b))) /\ ((a INSERT (b INSERT (c INSERT EMPTY))) SUBSET (relative_frontier s)))) ==> ((closed_segment ((CONS (a , b)) NIL)) SUBSET (relative_frontier s)))))))
