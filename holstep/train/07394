N ODD_MULT
C |- (!m. (!n. ((ODD (m * n)) = ((ODD m) /\ (ODD n)))))
T c= cODD c* f0 f1 c/\ cODD f0 cODD f1
D NOT_EVEN
A |- (!n. ((~ (EVEN n)) = (ODD n)))
T c= c~ cEVEN f0 cODD f0
D EVEN_MULT
A |- (!m. (!n. ((EVEN (m * n)) = ((EVEN m) \/ (EVEN n)))))
T c= cEVEN c* f0 f1 c\/ cEVEN f0 cEVEN f1
+ ((~ (EVEN m)) /\ (~ (EVEN n))) |- (((~ (EVEN m)) /\ (~ (EVEN n))) /\ (~ ((EVEN m) \/ (EVEN n))))
T c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c/\ c/\ c~ cEVEN f0 c~ cEVEN f1 c~ c\/ cEVEN f0 cEVEN f1
+ ((~ (EVEN m)) /\ (~ (EVEN n))) |- (((EVEN m) \/ (EVEN n)) ==> F)
T c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c==> c\/ cEVEN f0 cEVEN f1 cF
+ |- ((((~ (EVEN m)) /\ (~ (EVEN n))) ==> (~ ((EVEN m) \/ (EVEN n)))) = (((~ ((EVEN m) \/ (EVEN n))) ==> ((~ (EVEN m)) /\ (~ (EVEN n)))) /\ (((~ (EVEN m)) /\ (~ (EVEN n))) ==> (~ ((EVEN m) \/ (EVEN n))))))
T c= c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c~ c\/ cEVEN f0 cEVEN f1 c/\ c==> c~ c\/ cEVEN f0 cEVEN f1 c/\ c~ cEVEN f0 c~ cEVEN f1 c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c~ c\/ cEVEN f0 cEVEN f1
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
+ |- (((~ (EVEN m)) /\ (~ (EVEN n))) ==> (~ ((EVEN m) \/ (EVEN n))))
T c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c~ c\/ cEVEN f0 cEVEN f1
+ ((~ (EVEN m)) /\ (~ (EVEN n))), ((EVEN n) ==> F), ((EVEN m) \/ (EVEN n)) |- F
T c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c==> c==> cEVEN f1 cF c==> c\/ cEVEN f0 cEVEN f1 cF
- (EVEN m), ((~ (EVEN m)) /\ (~ (EVEN n))) |- ((EVEN m) /\ F)
T c==> cEVEN f0 c==> c/\ c~ cEVEN f0 c~ cEVEN f1 c/\ cEVEN f0 cF
+ |- ((\m. (!n. ((EVEN (m * n)) = ((EVEN m) \/ (EVEN n))))) m)
T * / ! c= cEVEN c* b0 b1 c\/ cEVEN b0 cEVEN b1 f0
+ (~ ((EVEN m) \/ (EVEN n))), ((EVEN m) \/ (EVEN n)) |- F
T c==> c~ c\/ cEVEN f0 cEVEN f1 c==> c\/ cEVEN f0 cEVEN f1 cF
- (EVEN n), ((~ (EVEN m)) /\ (~ (EVEN n))) |- ((EVEN n) /\ F)
T c==> cEVEN f0 c==> c/\ c~ cEVEN f1 c~ cEVEN f0 c/\ cEVEN f0 cF
- |- ((x = x) = T)
T c= c= f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- (EVEN n), ((~ (EVEN m)) /\ (~ (EVEN n))) |- F
T c==> cEVEN f0 c==> c/\ c~ cEVEN f1 c~ cEVEN f0 cF
- (EVEN m), ((~ (EVEN m)) /\ (~ (EVEN n))) |- F
T c==> cEVEN f0 c==> c/\ c~ cEVEN f0 c~ cEVEN f1 cF
