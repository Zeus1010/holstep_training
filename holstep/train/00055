N HAS_COMPLEX_DERIVATIVE_LMUL_AT
C |- (!f. (!f'. (!c. (!x. ((((has_complex_derivative f) f') (at x)) ==> (((has_complex_derivative (\x. ((complex_mul c) (f x)))) ((complex_mul c) f')) (at x)))))))
T c==> chas_complex_derivative f0 f1 cat f2 chas_complex_derivative / ccomplex_mul f3 * f0 b0 ccomplex_mul f3 f1 cat f2
D TRUTH
A |- T
T cT
D WITHIN_UNIV
A |- (!x. (((within (at x)) UNIV) = (at x)))
T c= cwithin cat f0 part cUNIV cat f0
D HAS_COMPLEX_DERIVATIVE_LMUL_WITHIN
A |- (!f. (!f'. (!c. (!x. (!s. ((((has_complex_derivative f) f') ((within (at x)) s)) ==> (((has_complex_derivative (\x. ((complex_mul c) (f x)))) ((complex_mul c) f')) ((within (at x)) s))))))))
T c==> chas_complex_derivative f0 f1 cwithin cat f2 f3 chas_complex_derivative / ccomplex_mul f4 * f0 b0 ccomplex_mul f4 f1 cwithin cat f2 f3
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!x. t) = t)
T c= ! f0 f0
