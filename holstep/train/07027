N MAXIMAL_INDEPENDENT_SUBSET
C |- (!v. (?b. ((b SUBSET v) /\ ((independent b) /\ (v SUBSET (span b))))))
T c? / c/\ cSUBSET b0 f0 c/\ cindependent b0 cSUBSET f0 * part cspan b0
D TRUTH
A |- T
T cT
D EMPTY_SUBSET
A |- (!s. (EMPTY SUBSET s))
T cSUBSET part cEMPTY f0
D INDEPENDENT_EMPTY
A |- (independent EMPTY)
T cindependent part cEMPTY
D MAXIMAL_INDEPENDENT_SUBSET_EXTEND
A |- (!s. (!v. (((s SUBSET v) /\ (independent s)) ==> (?b. ((s SUBSET b) /\ ((b SUBSET v) /\ ((independent b) /\ (v SUBSET (span b)))))))))
T c==> c/\ cSUBSET f0 f1 cindependent f0 c? / c/\ cSUBSET f0 b0 c/\ cSUBSET b0 f1 c/\ cindependent b0 cSUBSET f1 * part cspan b0
+ |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((!v. t) = t)
T c= ! f0 f0
- |- ((?b. t) = t)
T c= c? / f0 f0
+ |- ((T ==> t) = t)
T c= c==> cT f0 f0
+ |- (((!s. (EMPTY SUBSET s)) ==> ((\s. (EMPTY SUBSET s)) s)) = ((\s. (EMPTY SUBSET s)) s))
T c= c==> ! cSUBSET part cEMPTY b0 * / cSUBSET part cEMPTY b0 f0 * / cSUBSET part cEMPTY b0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
