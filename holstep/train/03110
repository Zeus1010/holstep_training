N CNJ_CEXP
C |- (!z. ((cnj (cexp z)) = (cexp (cnj z))))
T c= ccnj ccexp f0 ccexp ccnj f0
D TRUTH
A |- T
T cT
D SERIES_UNIQUE
A |- (!f. (!l. (!l'. (!s. (((((sums f) l) s) /\ (((sums f) l') s)) ==> (l = l'))))))
T c==> c/\ csums f0 f1 f2 csums f0 f3 f2 c= f1 f3
D CNJ_CX
A |- (!x. ((cnj (Cx x)) = (Cx x)))
T c= ccnj cCx f0 cCx f0
D CNJ_DIV
A |- (!w. (!z. ((cnj ((complex_div w) z)) = ((complex_div (cnj w)) (cnj z)))))
T c= ccnj ccomplex_div f0 f1 ccomplex_div ccnj f0 ccnj f1
D CNJ_POW
A |- (!z. (!n. ((cnj ((complex_pow z) n)) = ((complex_pow (cnj z)) n))))
T c= ccnj ccomplex_pow f0 f1 ccomplex_pow ccnj f0 f1
D SUMS_CNJ
A |- (!net. (!f. (!l. ((((sums (\x. (cnj (f x)))) (cnj l)) net) = (((sums f) l) net)))))
T c= csums / ccnj * f0 b0 ccnj f1 f2 csums f0 f1 f2
D CEXP_CONVERGES
A |- (!z. (((sums (\n. ((complex_div ((complex_pow z) n)) (Cx (real_of_num (FACT n)))))) (cexp z)) (from (NUMERAL _0))))
T csums / ccomplex_div ccomplex_pow f0 b0 cCx creal_of_num cFACT b0 ccexp f0 * part cfrom cNUMERAL c_0
+ ((\s. ((((sums f) l) s) /\ (((sums f) l') s))) s) |- ((((sums f) l) s) /\ (((sums f) l') s))
T c==> * / c/\ csums f0 f1 b0 csums f0 f2 b0 f3 c/\ csums f0 f1 f3 csums f0 f2 f3
- (?s. ((((sums f) l) s) /\ (((sums f) l') s))) |- ((?s. ((((sums f) l) s) /\ (((sums f) l') s))) /\ ((!f. (!l. (!l'. (!s. (((((sums f) l) s) /\ (((sums f) l') s)) ==> (l = l')))))) ==> (l = l')))
T c==> c? / c/\ csums f0 f1 b0 csums f0 f2 b0 c/\ c? / c/\ csums f0 f1 b0 csums f0 f2 b0 c==> ! ! ! ! c==> c/\ csums b0 b1 b3 csums b0 b2 b3 c= b1 b2 c= f1 f2
