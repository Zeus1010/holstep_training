N CLOSED_SING
C |- (!a. (closed (a INSERT EMPTY)))
T cclosed * * part cINSERT f0 part cEMPTY
D TRUTH
A |- T
T cT
D CLOSED_LIMPT
A |- (!s. ((closed s) = (!x. (((limit_point_of x) s) ==> (x IN s)))))
T c= cclosed f0 ! c==> climit_point_of b0 f0 cIN b0 f0
D LIMPT_EMPTY
A |- (!x. (~ ((limit_point_of x) EMPTY)))
T c~ climit_point_of f0 part cEMPTY
D LIMPT_INSERT
A |- (!s. (!x. (!y. (((limit_point_of x) (y INSERT s)) = ((limit_point_of x) s)))))
T c= climit_point_of f0 * * part cINSERT f1 f2 climit_point_of f0 f2
+ |- (((!s. ((closed s) = (!x. (((limit_point_of x) s) ==> (x IN s))))) ==> ((\s. ((closed s) = (!x. (((limit_point_of x) s) ==> (x IN s))))) s)) = ((\s. ((closed s) = (!x. (((limit_point_of x) s) ==> (x IN s))))) s))
T c= c==> ! c= cclosed b0 ! c==> climit_point_of b1 b0 cIN b1 b0 * / c= cclosed b0 ! c==> climit_point_of b1 b0 cIN b1 b0 f0 * / c= cclosed b0 ! c==> climit_point_of b1 b0 cIN b1 b0 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
