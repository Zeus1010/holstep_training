N INT_LT_IMP_NE
C |- (!x. (!y. (((int_lt x) y) ==> (~ (x = y)))))
T c==> cint_lt f0 f1 c~ c= f0 f1
D REAL_LT_IMP_NE
A |- (!x. (!y. (((real_lt x) y) ==> (~ (x = y)))))
T c==> creal_lt f0 f1 c~ c= f0 f1
D int_eq
A |- (!x. (!y. ((x = y) = ((real_of_int x) = (real_of_int y)))))
T c= c= f0 f1 c= creal_of_int f0 creal_of_int f1
D int_lt
A |- (!x. (!y. (((int_lt x) y) = ((real_lt (real_of_int x)) (real_of_int y)))))
T c= cint_lt f0 f1 creal_lt creal_of_int f0 creal_of_int f1
