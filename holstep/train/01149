+ |- ((!s. (!t. (((t <=_c UNIV) /\ (s <=_c t)) ==> (s <=_c UNIV)))) = (!s. (!t. (((COUNTABLE t) /\ (s <=_c t)) ==> (COUNTABLE s)))))
