N MEASURE_INTERIOR
C |- (!s. (((bounded s) /\ (negligible (frontier s))) ==> ((measure (interior s)) = (measure s))))
T c==> c/\ cbounded f0 cnegligible * part cfrontier f0 c= cmeasure * part cinterior f0 cmeasure f0
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D I_THM
A |- (!x. ((I x) = x))
T c= cI f0 f0
D IN
A |- (!P. (!x. ((x IN P) = (P x))))
T c= cIN f0 f1 * f1 f0
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D IN_UNION
A |- (!s. (!t. (!x. ((x IN (s UNION t)) = ((x IN s) \/ (x IN t))))))
T c= cIN f0 * * part cUNION f1 f2 c\/ cIN f0 f1 cIN f0 f2
D IN_DIFF
A |- (!s. (!t. (!x. ((x IN (s DIFF t)) = ((x IN s) /\ (~ (x IN t)))))))
T c= cIN f0 * * part cDIFF f1 f2 c/\ cIN f0 f1 c~ cIN f0 f2
D INTERIOR_SUBSET
A |- (!s. ((interior s) SUBSET s))
T cSUBSET * part cinterior f0 f0
D CLOSURE_SUBSET
A |- (!s. (s SUBSET (closure s)))
T cSUBSET f0 * part cclosure f0
D frontier
A |- (!s. ((frontier s) = ((closure s) DIFF (interior s))))
T c= * part cfrontier f0 * * part cDIFF * part cclosure f0 * part cinterior f0
D NEGLIGIBLE_SUBSET
A |- (!s. (!t. (((negligible s) /\ (t SUBSET s)) ==> (negligible t))))
T c==> c/\ cnegligible f0 cSUBSET f1 f0 cnegligible f1
D MEASURE_NEGLIGIBLE_SYMDIFF
