N REAL_CLOSED_INTER
C |- (!s. (!t. (((real_closed s) /\ (real_closed t)) ==> (real_closed (s INTER t)))))
T c==> c/\ creal_closed f0 creal_closed f1 creal_closed * * part cINTER f0 f1
D TRUTH
A |- T
T cT
D CLOSED_IN_INTER
A |- (!top. (!s. (!t. ((((closed_in top) s) /\ ((closed_in top) t)) ==> ((closed_in top) (s INTER t))))))
T c==> c/\ cclosed_in f0 f1 cclosed_in f0 f2 cclosed_in f0 * * part cINTER f1 f2
D REAL_CLOSED_IN
A |- (!s. ((real_closed s) = ((closed_in euclideanreal) s)))
T c= creal_closed f0 cclosed_in ceuclideanreal f0
+ |- (((((closed_in top) s) /\ ((closed_in top) t)) ==> ((closed_in top) (s INTER t))) = T)
T c= c==> c/\ cclosed_in f0 f1 cclosed_in f0 f2 cclosed_in f0 * * part cINTER f1 f2 cT
+ |- (((!top. (!s. (!t. ((((closed_in top) s) /\ ((closed_in top) t)) ==> ((closed_in top) (s INTER t)))))) ==> ((\top. (!s. (!t. ((((closed_in top) s) /\ ((closed_in top) t)) ==> ((closed_in top) (s INTER t)))))) top)) = ((\top. (!s. (!t. ((((closed_in top) s) /\ ((closed_in top) t)) ==> ((closed_in top) (s INTER t)))))) top))
T c= c==> ! ! ! c==> c/\ cclosed_in b0 b1 cclosed_in b0 b2 cclosed_in b0 * * part cINTER b1 b2 * / ! ! c==> c/\ cclosed_in b0 b1 cclosed_in b0 b2 cclosed_in b0 * * part cINTER b1 b2 f0 * / ! ! c==> c/\ cclosed_in b0 b1 cclosed_in b0 b2 cclosed_in b0 * * part cINTER b1 b2 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
