N INDEPENDENT_IMP_FINITE
C |- (!s. ((independent s) ==> (FINITE s)))
T c==> cindependent f0 cFINITE f0
D TRUTH
A |- T
T cT
D INDEPENDENT_BOUND
A |- (!s. ((independent s) ==> ((FINITE s) /\ ((CARD s) <= (dimindex UNIV)))))
T c==> cindependent f0 c/\ cFINITE f0 c<= cCARD f0 cdimindex part cUNIV
+ |- ((\s. ((independent s) ==> ((FINITE s) /\ ((CARD s) <= (dimindex UNIV))))) s)
T * / c==> cindependent b0 c/\ cFINITE b0 c<= cCARD b0 cdimindex part cUNIV f0
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (independent s) |- ((CARD s) <= (dimindex UNIV))
T c==> cindependent f0 c<= cCARD f0 cdimindex part cUNIV
+ |- ((!s. t) = t)
T c= ! f0 f0
+ |- ((\p'. (!q'. (((independent s) = p') ==> ((p' ==> ((FINITE s) = q')) ==> (((independent s) ==> (FINITE s)) = (p' ==> q')))))) p')
T * / ! c==> c= cindependent f0 b0 c==> c==> b0 c= cFINITE f0 b1 c= c==> cindependent f0 cFINITE f0 c==> b0 b1 f1
+ |- ((!q'. ((p = p') ==> ((p' ==> (q = q')) ==> ((p ==> q) = (p' ==> q'))))) = T)
T c= ! c==> c= f0 f1 c==> c==> f1 c= f2 b0 c= c==> f0 f2 c==> f1 b0 cT
- |- ((independent s) ==> (((CARD s) <= (dimindex UNIV)) = T))
T c==> cindependent f0 c= c<= cCARD f0 cdimindex part cUNIV cT
+ |- ((\q'. (((independent s) = p') ==> ((p' ==> ((FINITE s) = q')) ==> (((independent s) ==> (FINITE s)) = (p' ==> q'))))) q')
T * / c==> c= cindependent f0 f1 c==> c==> f1 c= cFINITE f0 b0 c= c==> cindependent f0 cFINITE f0 c==> f1 b0 f2
- (independent s) |- (T = (independent s))
T c==> cindependent f0 c= cT cindependent f0
- (independent s) |- ((independent s) /\ (((CARD s) <= (dimindex UNIV)) = T))
T c==> cindependent f0 c/\ cindependent f0 c= c<= cCARD f0 cdimindex part cUNIV cT
- (independent s) |- ((independent s) = T)
T c==> cindependent f0 c= cindependent f0 cT
