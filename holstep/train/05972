N Hypermap_iso.image_set_inj_gen
C |- (!u. (!f. ((!x. (!y. (((x IN u) /\ ((y IN u) /\ ((f x) = (f y)))) ==> (x = y)))) ==> (!s. (!t. (((s SUBSET u) /\ ((t SUBSET u) /\ (((IMAGE f) s) = ((IMAGE f) t)))) ==> (s = t)))))))
T c==> ! ! c==> c/\ cIN b0 f0 c/\ cIN b1 f0 c= * f1 b0 * f1 b1 c= b0 b1 ! ! c==> c/\ cSUBSET b0 f0 c/\ cSUBSET b1 f0 c= * * part cIMAGE f1 b0 * * part cIMAGE f1 b1 c= b0 b1
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D EXTENSION
A |- (!s. (!t. ((s = t) = (!x. ((x IN s) = (x IN t))))))
T c= c= f0 f1 ! c= cIN b0 f0 cIN b0 f1
D SUBSET
