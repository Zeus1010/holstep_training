N FINITE_PRODUCT
C |- (!s. (!t. (((FINITE s) /\ (FINITE t)) ==> (FINITE (GSPEC (\GEN%PVAR%92. (?x. (?y. (((SETSPEC GEN%PVAR%92) ((x IN s) /\ (y IN t))) (x , y))))))))))
T c==> c/\ cFINITE f0 cFINITE f1 cFINITE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f1 c, b1 b2
D TRUTH
A |- T
T cT
D FINITE_PRODUCT_DEPENDENT
A |- (!f. (!s. (!t. (((FINITE s) /\ (!x. ((x IN s) ==> (FINITE (t x))))) ==> (FINITE (GSPEC (\GEN%PVAR%91. (?x. (?y. (((SETSPEC GEN%PVAR%91) ((x IN s) /\ (y IN (t x)))) ((f x) y)))))))))))
T c==> c/\ cFINITE f0 ! c==> cIN b0 f0 cFINITE * f1 b0 cFINITE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 * f1 b1 * * f2 b1 b2
+ |- ((\q'. (((x IN s) = p') ==> ((p' ==> ((FINITE t) = q')) ==> (((x IN s) ==> (FINITE t)) = (p' ==> q'))))) q')
T * / c==> c= cIN f0 f1 f2 c==> c==> f2 c= cFINITE f3 b0 c= c==> cIN f0 f1 cFINITE f3 c==> f2 b0 f4
+ |- (((FINITE s) /\ (!x. ((x IN s) ==> (FINITE (t x))))) ==> ((FINITE (GSPEC (\GEN%PVAR%92. (?x. (?y. (((SETSPEC GEN%PVAR%92) ((x IN s) /\ (y IN (t x)))) ((f x) y))))))) = T))
T c==> c/\ cFINITE f0 ! c==> cIN b0 f0 cFINITE * f1 b0 c= cFINITE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 * f1 b1 * * f2 b1 b2 cT
+ |- (((FINITE s) /\ (FINITE t)) ==> ((FINITE (GSPEC (\GEN%PVAR%92. (?x. (?y. (((SETSPEC GEN%PVAR%92) ((x IN s) /\ (y IN t))) (x , y))))))) = T))
T c==> c/\ cFINITE f0 cFINITE f1 c= cFINITE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f1 c, b1 b2 cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (x IN s) |- ((x IN s) = T)
T c==> cIN f0 f1 c= cIN f0 f1 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
