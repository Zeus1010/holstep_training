T creal_le creal_of_num cNUMERAL c_0 cvector_norm f0
D LIFT_DROP
A |- ((!x. ((lift (drop x)) = x)) /\ (!x. ((drop (lift x)) = x)))
T c/\ ! c= clift cdrop b0 b0 ! c= cdrop clift b0 b0
D DROP_VSUM
A |- (!k. (!x. ((drop ((vsum k) x)) = ((sum k) (drop o x)))))
T c= cdrop cvsum f0 f1 csum f0 * * part co part cdrop f1
D division_of
A |- (!s. (!i. (((division_of s) i) = ((FINITE s) /\ ((!k. ((k IN s) ==> ((k SUBSET i) /\ ((~ (k = EMPTY)) /\ (?a. (?b. (k = (closed_interval ((CONS (a , b)) NIL))))))))) /\ ((!k1. (!k2. (((k1 IN s) /\ ((k2 IN s) /\ (~ (k1 = k2)))) ==> (((interior k1) INTER (interior k2)) = EMPTY)))) /\ ((UNIONS s) = i)))))))
