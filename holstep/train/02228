N ABSOLUTELY_INTEGRABLE_BOUNDED_SETVARIATION
C |- (!f. (!s. (((absolutely_integrable_on f) s) ==> ((has_bounded_setvariation_on (\k. ((integral k) f))) s))))
T c==> cabsolutely_integrable_on f0 f1 chas_bounded_setvariation_on / cintegral b0 f0 f1
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D EXCLUDED_MIDDLE
A |- (!t. (t \/ (~ t)))
T c\/ f0 c~ f0
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D NOT_EXISTS_THM
A |- (!P. ((~ (?x. (P x))) = (!x. (~ (P x)))))
T c= c~ c? / * f0 b0 ! c~ * f0 b0
D NOT_FORALL_THM
A |- (!P. ((~ (!x. (P x))) = (?x. (~ (P x)))))
T c= c~ ! * f0 b0 c? / c~ * f0 b0
D RIGHT_OR_FORALL_THM
A |- (!P. (!Q. ((P \/ (!x. (Q x))) = (!x. (P \/ (Q x))))))
T c= c\/ f0 ! * f1 b0 ! c\/ f0 * f1 b0
D SKOLEM_THM
A |- (!P. ((!x. (?y. ((P x) y))) = (?y. (!x. ((P x) (y x))))))
T c= ! c? / * * f0 b0 b1 c? / ! * * f0 b1 * b0 b1
D o_THM
A |- (!f. (!g. (!x. (((f o g) x) = (f (g x))))))
T c= co f0 f1 f2 * f0 * f1 f2
D REAL_LE_TRANS
A |- (!x. (!y. (!z. ((((real_le x) y) /\ ((real_le y) z)) ==> ((real_le x) z)))))
T c==> c/\ creal_le f0 f1 creal_le f1 f2 creal_le f0 f2
D REAL_EQ_IMP_LE
A |- (!x. (!y. ((x = y) ==> ((real_le x) y))))
T c==> c= f0 f1 creal_le f0 f1
D SUBSET_TRANS
A |- (!s. (!t. (!u. (((s SUBSET t) /\ (t SUBSET u)) ==> (s SUBSET u)))))
T c==> c/\ cSUBSET f0 f1 cSUBSET f1 f2 cSUBSET f0 f2
D SUM_LE
A |- (!f. (!g. (!s. (((FINITE s) /\ (!x. ((x IN s) ==> ((real_le (f x)) (g x))))) ==> ((real_le ((sum s) f)) ((sum s) g))))))
T c==> c/\ cFINITE f0 ! c==> cIN b0 f0 creal_le * f1 b0 * f2 b0 creal_le csum f0 f1 csum f0 f2
D NORM_POS_LE
