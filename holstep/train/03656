N COMPACT_LINEAR_IMAGE
C |- (!f. (!s. (((compact s) /\ (linear f)) ==> (compact ((IMAGE f) s)))))
T c==> c/\ ccompact f0 clinear f1 ccompact * * part cIMAGE f1 f0
D TRUTH
A |- T
T cT
D LINEAR_CONTINUOUS_ON
A |- (!f. (!s. ((linear f) ==> ((continuous_on f) s))))
T c==> clinear f0 ccontinuous_on f0 f1
D COMPACT_CONTINUOUS_IMAGE
A |- (!f. (!s. ((((continuous_on f) s) /\ (compact s)) ==> (compact ((IMAGE f) s)))))
T c==> c/\ ccontinuous_on f0 f1 ccompact f1 ccompact * * part cIMAGE f0 f1
+ (((continuous_on f) s) /\ (compact s)) |- ((((continuous_on f) s) /\ (compact s)) /\ ((compact ((IMAGE f) s)) = T))
T c==> c/\ ccontinuous_on f0 f1 ccompact f1 c/\ c/\ ccontinuous_on f0 f1 ccompact f1 c= ccompact * * part cIMAGE f0 f1 cT
+ |- ((\p'. (!q'. ((((compact s) /\ (linear f)) = p') ==> ((p' ==> ((compact ((IMAGE f) s)) = q')) ==> ((((compact s) /\ (linear f)) ==> (compact ((IMAGE f) s))) = (p' ==> q')))))) p')
T * / ! c==> c= c/\ ccompact f0 clinear f1 b0 c==> c==> b0 c= ccompact * * part cIMAGE f1 f0 b1 c= c==> c/\ ccompact f0 clinear f1 ccompact * * part cIMAGE f1 f0 c==> b0 b1 f2
+ |- (((!s. ((linear f) ==> ((continuous_on f) s))) ==> ((\s. ((linear f) ==> ((continuous_on f) s))) s)) = ((\s. ((linear f) ==> ((continuous_on f) s))) s))
T c= c==> ! c==> clinear f0 ccontinuous_on f0 b0 * / c==> clinear f0 ccontinuous_on f0 b0 f1 * / c==> clinear f0 ccontinuous_on f0 b0 f1
+ |- ((\f. (!s. ((linear f) ==> ((continuous_on f) s)))) f)
T * / ! c==> clinear b0 ccontinuous_on b0 b1 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((compact s) /\ (linear f)) |- ((linear f) = T)
T c==> c/\ ccompact f0 clinear f1 c= clinear f1 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- ((compact s) /\ (linear f)) |- (T = (linear f))
T c==> c/\ ccompact f0 clinear f1 c= cT clinear f1
