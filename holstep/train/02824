N SC_INV
C |- ((SC (INV R)) = (INV (SC R)))
T c= * part cSC * part cINV f0 * part cINV * part cSC f0
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D SC_EXPLICIT
A |- (!R. ((((SC R) x) y) = (((R x) y) \/ ((R y) x))))
T c= cSC f0 f1 f2 c\/ * * f0 f1 f2 * * f0 f2 f1
D INV
A |- (!R. (!y. (!x. ((((INV R) x) y) = ((R y) x)))))
T c= cINV f0 f1 f2 * * f0 f2 f1
+ |- (T = ((SC (INV R)) = (INV (SC R))))
T c= cT c= * part cSC * part cINV f0 * part cINV * part cSC f0
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
