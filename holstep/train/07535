N SUBPATH_TRIVIAL
C |- (!g. ((((subpath (vec (NUMERAL _0))) (vec (NUMERAL (BIT1 _0)))) g) = g))
T c= * * * part csubpath cvec cNUMERAL c_0 cvec cNUMERAL cBIT1 c_0 f0 f0
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D VECTOR_ADD_LID
A |- (!x. (((vector_add (vec (NUMERAL _0))) x) = x))
T c= cvector_add cvec cNUMERAL c_0 f0 f0
D VECTOR_MUL_LID
A |- (!x. (((% (real_of_num (NUMERAL (BIT1 _0)))) x) = x))
T c= c% creal_of_num cNUMERAL cBIT1 c_0 f0 f0
D VECTOR_SUB_RZERO
A |- (((vector_sub x) (vec (NUMERAL _0))) = x)
T c= cvector_sub f0 cvec cNUMERAL c_0 f0
D DROP_VEC
A |- (!n. ((drop (vec n)) = (real_of_num n)))
T c= cdrop cvec f0 creal_of_num f0
D subpath
A |- (!g. (!v. (!u. ((((subpath u) v) g) = (\x. (g ((vector_add u) ((% (drop ((vector_sub v) u))) x))))))))
T c= * * * part csubpath f0 f1 f2 / * f2 cvector_add f0 c% cdrop cvector_sub f1 f0 b0
