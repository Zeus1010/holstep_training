N EXISTS_IN_CROSS
C |- (!P. (!s. (!t. ((?z. ((z IN (s CROSS t)) /\ (P z))) = (?x. (?y. ((x IN s) /\ ((y IN t) /\ (P (x , y))))))))))
T c= c? / c/\ cIN b0 * * part cCROSS f0 f1 * f2 b0 c? / c? / c/\ cIN b0 f0 c/\ cIN b1 f1 * f2 c, b0 b1
D TRUTH
A |- T
T cT
D EXISTS_PAIR_THM
A |- (!P. ((?p. (P p)) = (?p1. (?p2. (P (p1 , p2))))))
T c= c? / * f0 b0 c? / c? / * f0 c, b0 b1
D IN_CROSS
A |- (!x. (!y. (!s. (!t. (((x , y) IN (s CROSS t)) = ((x IN s) /\ (y IN t)))))))
T c= cIN c, f0 f1 * * part cCROSS f2 f3 c/\ cIN f0 f2 cIN f1 f3
+ |- (((!P. ((?p. (P p)) = (?p1. (?p2. (P (p1 , p2)))))) ==> ((\P. ((?p. (P p)) = (?p1. (?p2. (P (p1 , p2)))))) P)) = ((\P. ((?p. (P p)) = (?p1. (?p2. (P (p1 , p2)))))) P))
T c= c==> ! c= c? / * b0 b1 c? / c? / * b0 c, b1 b2 * / c= c? / * b0 b1 c? / c? / * b0 c, b1 b2 f0 * / c= c? / * b0 b1 c? / c? / * b0 c, b1 b2 f0
+ ((t1 /\ (t2 /\ t3)) = ((t1 /\ t2) /\ t3)) |- (((t1 /\ t2) /\ t3) = (t1 /\ (t2 /\ t3)))
T c==> c= c/\ f0 c/\ f1 f2 c/\ c/\ f0 f1 f2 c= c/\ c/\ f0 f1 f2 c/\ f0 c/\ f1 f2
- |- ((?y. t) = t)
T c= c? / f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
