N REAL_CONTINUOUS_ON_CONST
C |- (!s. (!c. ((real_continuous_on (\x. c)) s)))
T creal_continuous_on / f0 f1
D TRUTH
A |- T
T cT
D REAL_CONTINUOUS_CONST
A |- (!net. (!c. ((real_continuous (\x. c)) net)))
T creal_continuous / f0 f1
D REAL_CONTINUOUS_ON_EQ_CONTINUOUS_WITHIN
A |- (!f. (!s. (((real_continuous_on f) s) = (!x. ((x IN s) ==> ((real_continuous f) ((within (atreal x)) s)))))))
T c= creal_continuous_on f0 f1 ! c==> cIN b0 f1 creal_continuous f0 cwithin catreal b0 f1
+ |- (((!p'. (!q'. (((x IN s) = p') ==> ((p' ==> (((real_continuous (\x. c)) ((within (atreal x)) s)) = q')) ==> (((x IN s) ==> ((real_continuous (\x. c)) ((within (atreal x)) s))) = (p' ==> q')))))) ==> ((\p'. (!q'. (((x IN s) = p') ==> ((p' ==> (((real_continuous (\x. c)) ((within (atreal x)) s)) = q')) ==> (((x IN s) ==> ((real_continuous (\x. c)) ((within (atreal x)) s))) = (p' ==> q')))))) p')) = ((\p'. (!q'. (((x IN s) = p') ==> ((p' ==> (((real_continuous (\x. c)) ((within (atreal x)) s)) = q')) ==> (((x IN s) ==> ((real_continuous (\x. c)) ((within (atreal x)) s))) = (p' ==> q')))))) p'))
T c= c==> ! ! c==> c= cIN f0 f1 b0 c==> c==> b0 c= creal_continuous / f2 cwithin catreal f0 f1 b1 c= c==> cIN f0 f1 creal_continuous / f2 cwithin catreal f0 f1 c==> b0 b1 * / ! c==> c= cIN f0 f1 b0 c==> c==> b0 c= creal_continuous / f2 cwithin catreal f0 f1 b1 c= c==> cIN f0 f1 creal_continuous / f2 cwithin catreal f0 f1 c==> b0 b1 f3 * / ! c==> c= cIN f0 f1 b0 c==> c==> b0 c= creal_continuous / f2 cwithin catreal f0 f1 b1 c= c==> cIN f0 f1 creal_continuous / f2 cwithin catreal f0 f1 c==> b0 b1 f3
+ |- (((x IN s) ==> (((real_continuous (\x. c)) ((within (atreal x)) s)) = q')) ==> (((x IN s) ==> ((real_continuous (\x. c)) ((within (atreal x)) s))) = ((x IN s) ==> q')))
T c==> c==> cIN f0 f1 c= creal_continuous / f2 cwithin catreal f0 f1 f3 c= c==> cIN f0 f1 creal_continuous / f2 cwithin catreal f0 f1 c==> cIN f0 f1 f3
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (x IN s) |- ((x IN s) = T)
T c==> cIN f0 f1 c= cIN f0 f1 cT
