N HAUSDIST_UNION_LE
C |- (!s. (!t. (!u. (((bounded s) /\ ((bounded t) /\ ((bounded u) /\ ((~ (t = EMPTY)) /\ (~ (u = EMPTY)))))) ==> ((real_le (hausdist ((s UNION t) , (s UNION u)))) (hausdist (t , u)))))))
