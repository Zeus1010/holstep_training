N REALLIM_1_OVER_N
C |- (((---> (\n. (real_inv (real_of_num n)))) (real_of_num (NUMERAL _0))) sequentially)
T c---> / creal_inv creal_of_num b0 creal_of_num cNUMERAL c_0 csequentially
D TRUTH
A |- T
T cT
D o_DEF
A |- (!f. (!g. ((f o g) = (\x. (f (g x))))))
T c= * * part co f0 f1 / * f0 * f1 b0
D CX_INV
A |- (!x. ((Cx (real_inv x)) = (complex_inv (Cx x))))
T c= cCx creal_inv f0 ccomplex_inv cCx f0
D LIM_INV_N
A |- (((--> (\n. (complex_inv (Cx (real_of_num n))))) (Cx (real_of_num (NUMERAL _0)))) sequentially)
T c--> / ccomplex_inv cCx creal_of_num b0 cCx creal_of_num cNUMERAL c_0 csequentially
D REALLIM_COMPLEX
A |- (((---> s) l) = ((--> (Cx o s)) (Cx l)))
T c= * * part c---> f0 f1 * * part c--> * * part co part cCx f0 cCx f1
+ |- (((!f. (!g. ((f o g) = (\x. (f (g x)))))) ==> ((\f. (!g. ((f o g) = (\x. (f (g x)))))) f)) = ((\f. (!g. ((f o g) = (\x. (f (g x)))))) f))
T c= c==> ! ! c= * * part co b0 b1 / * b0 * b1 b2 * / ! c= * * part co b0 b1 / * b0 * b1 b2 f0 * / ! c= * * part co b0 b1 / * b0 * b1 b2 f0
- |- (((\n. (f n)) y) = (f y))
T c= * / * f0 b0 f1 * f0 f1
