N LOCALLY_COMPACT_CLOSED_IN
C |- (!s. (!t. ((((closed_in ((subtopology euclidean) s)) t) /\ ((locally compact) s)) ==> ((locally compact) t))))
T c==> c/\ cclosed_in csubtopology ceuclidean f0 f1 clocally part ccompact f0 clocally part ccompact f1
D TRUTH
A |- T
T cT
D CLOSED_IN_CLOSED
A |- (!s. (!u. (((closed_in ((subtopology euclidean) u)) s) = (?t. ((closed t) /\ (s = (u INTER t)))))))
T c= cclosed_in csubtopology ceuclidean f0 f1 c? / c/\ cclosed b0 c= f1 * * part cINTER f0 b0
D CLOSED_IMP_LOCALLY_COMPACT
A |- (!s. ((closed s) ==> ((locally compact) s)))
T c==> cclosed f0 clocally part ccompact f0
D LOCALLY_COMPACT_INTER
A |- (!s. (!t. ((((locally compact) s) /\ ((locally compact) t)) ==> ((locally compact) (s INTER t)))))
T c==> c/\ clocally part ccompact f0 clocally part ccompact f1 clocally part ccompact * * part cINTER f0 f1
+ |- ((\s. ((closed s) ==> ((locally compact) s))) s)
T * / c==> cclosed b0 clocally part ccompact b0 f0
+ (closed t'), ((locally compact) s) |- (T = (((locally compact) s) /\ ((locally compact) t')))
T c==> cclosed f0 c==> clocally part ccompact f1 c= cT c/\ clocally part ccompact f1 clocally part ccompact f0
+ (closed t'), (t = (s INTER t')), ((locally compact) s) |- ((locally compact) t)
T c==> cclosed f0 c==> c= f1 * * part cINTER f2 f0 c==> clocally part ccompact f2 clocally part ccompact f1
+ |- ((\s. (!t. ((((locally compact) s) /\ ((locally compact) t)) ==> ((locally compact) (s INTER t))))) s)
T * / ! c==> c/\ clocally part ccompact b0 clocally part ccompact b1 clocally part ccompact * * part cINTER b0 b1 f0
+ ((locally compact) s) |- (((closed t') /\ (t = (s INTER t'))) ==> ((locally compact) t))
T c==> clocally part ccompact f0 c==> c/\ cclosed f1 c= f2 * * part cINTER f0 f1 clocally part ccompact f2
+ ((closed t') /\ (t = (s INTER t'))), ((locally compact) s) |- ((locally compact) t)
T c==> c/\ cclosed f0 c= f1 * * part cINTER f2 f0 c==> clocally part ccompact f2 clocally part ccompact f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((((locally compact) s) /\ ((locally compact) t)) ==> (((locally compact) (s INTER t)) = T))
T c==> c/\ clocally part ccompact f0 clocally part ccompact f1 c= clocally part ccompact * * part cINTER f0 f1 cT
+ (closed t') |- (((locally compact) t') = T)
T c==> cclosed f0 c= clocally part ccompact f0 cT
- |- ((!t. t) = t)
T c= ! f0 f0
- |- ((?t'. t) = t)
T c= c? / f0 f0
- (t = (s INTER t')) |- ((closed (s INTER t')) = (closed t))
T c==> c= f0 * * part cINTER f1 f2 c= cclosed * * part cINTER f1 f2 cclosed f0
- |- ((x = x) = T)
T c= c= f0 f0 cT
- (closed t') |- ((closed t') = T)
T c==> cclosed f0 c= cclosed f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- (closed t') |- (T = (closed t'))
T c==> cclosed f0 c= cT cclosed f0
