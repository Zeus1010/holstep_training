+ |- (!p1. (!p2. (!p1'. (!p2'. (!p1''. ((((hreal_add p1) p2') = ((hreal_add p1') p2)) ==> (((hreal_add ((hreal_add p1) p1'')) p2') = ((hreal_add ((hreal_add p1') p1'')) p2))))))))
T c==> c= chreal_add f0 f1 chreal_add f2 f3 c= chreal_add chreal_add f0 f4 f1 chreal_add chreal_add f2 f4 f3
- |- (((x = x) ==> p) = p)
T c= c==> c= f0 f0 f1 f1
- |- ((x = x) = T)
T c= c= f0 f0 cT
