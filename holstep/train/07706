N TREAL_ADD_WELLDEFR
C |- (!x1. (!x2. (!y. ((x1 treal_eq x2) ==> ((x1 treal_add y) treal_eq (x2 treal_add y))))))
T c==> ctreal_eq f0 f1 ctreal_eq ctreal_add f0 f2 ctreal_add f1 f2
D TRUTH
A |- T
T cT
D FORALL_PAIR_THM
A |- (!P. ((!p. (P p)) = (!p1. (!p2. (P (p1 , p2))))))
T c= ! * f0 b0 ! ! * f0 c, b0 b1
D HREAL_ADD_SYM
A |- (!x. (!y. (((hreal_add x) y) = ((hreal_add y) x))))
T c= chreal_add f0 f1 chreal_add f1 f0
D HREAL_ADD_ASSOC
A |- (!x. (!y. (!z. (((hreal_add x) ((hreal_add y) z)) = ((hreal_add ((hreal_add x) y)) z)))))
T c= chreal_add f0 chreal_add f1 f2 chreal_add chreal_add f0 f1 f2
D HREAL_EQ_ADD_RCANCEL
A |- (!m. (!n. (!p. ((((hreal_add m) p) = ((hreal_add n) p)) = (m = n)))))
T c= c= chreal_add f0 f1 chreal_add f2 f1 c= f0 f2
D treal_add
A |- (!x1. (!x2. (!y1. (!y2. (((x1 , y1) treal_add (x2 , y2)) = (((hreal_add x1) x2) , ((hreal_add y1) y2)))))))
T c= ctreal_add c, f0 f1 c, f2 f3 c, chreal_add f0 f2 chreal_add f1 f3
D treal_eq
A |- (!x1. (!y2. (!x2. (!y1. (((x1 , y1) treal_eq (x2 , y2)) = (((hreal_add x1) y2) = ((hreal_add x2) y1)))))))
T c= ctreal_eq c, f0 f1 c, f2 f3 c= chreal_add f0 f3 chreal_add f2 f1
+ |- ((!p1. (!p2. (!p1'. (!p2'. (!p1''. ((((hreal_add p2') p1) = ((hreal_add p2) p1')) ==> (((hreal_add p2') ((hreal_add p1) p1'')) = ((hreal_add p2) ((hreal_add p1') p1''))))))))) = (!p1. (!p2. (!p1'. (!p2'. (!p1''. ((((hreal_add p1) p2') = ((hreal_add p1') p2)) ==> (((hreal_add ((hreal_add p1) p1'')) p2') = ((hreal_add ((hreal_add p1') p1'')) p2)))))))))
