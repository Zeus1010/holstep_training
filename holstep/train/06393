N FRONTIER_SING
C |- (!a. ((frontier (a INSERT EMPTY)) = (a INSERT EMPTY)))
T c= * part cfrontier * * part cINSERT f0 part cEMPTY * * part cINSERT f0 part cEMPTY
D TRUTH
A |- T
T cT
D DIFF_EMPTY
A |- (!s. ((s DIFF EMPTY) = s))
T c= * * part cDIFF f0 part cEMPTY f0
D INTERIOR_SING
A |- (!a. ((interior (a INSERT EMPTY)) = EMPTY))
T c= * part cinterior * * part cINSERT f0 part cEMPTY part cEMPTY
D frontier
A |- (!s. ((frontier s) = ((closure s) DIFF (interior s))))
T c= * part cfrontier f0 * * part cDIFF * part cclosure f0 * part cinterior f0
D CLOSURE_SING
A |- (!x. ((closure (x INSERT EMPTY)) = (x INSERT EMPTY)))
T c= * part cclosure * * part cINSERT f0 part cEMPTY * * part cINSERT f0 part cEMPTY
