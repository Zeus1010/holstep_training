N ISOMORPHISM_EXPAND
C |- (!f. (!g. ((((f o g) = I) /\ ((g o f) = I)) = ((!x. ((f (g x)) = x)) /\ (!x. ((g (f x)) = x))))))
T c= c/\ c= * * part co f0 f1 part cI c= * * part co f1 f0 part cI c/\ ! c= * f0 * f1 b0 b0 ! c= * f1 * f0 b0 b0
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D o_THM
A |- (!f. (!g. (!x. (((f o g) x) = (f (g x))))))
T c= co f0 f1 f2 * f0 * f1 f2
D I_THM
A |- (!x. ((I x) = x))
T c= cI f0 f0
+ |- (((!x. ((I x) = x)) ==> ((\x. ((I x) = x)) x)) = ((\x. ((I x) = x)) x))
T c= c==> ! c= cI b0 b0 * / c= cI b0 b0 f0 * / c= cI b0 b0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
