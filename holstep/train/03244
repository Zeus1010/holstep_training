N TREAL_LE_ANTISYM
C |- (!x. (!y. (((x treal_le y) /\ (y treal_le x)) = (x treal_eq y))))
T c= c/\ ctreal_le f0 f1 ctreal_le f1 f0 ctreal_eq f0 f1
D TRUTH
A |- T
T cT
D FORALL_PAIR_THM
A |- (!P. ((!p. (P p)) = (!p1. (!p2. (P (p1 , p2))))))
T c= ! * f0 b0 ! ! * f0 c, b0 b1
D HREAL_LE_ANTISYM
A |- (!x. (!y. ((((hreal_le x) y) /\ ((hreal_le y) x)) = (x = y))))
T c= c/\ chreal_le f0 f1 chreal_le f1 f0 c= f0 f1
D treal_le
A |- (!x1. (!y2. (!x2. (!y1. (((x1 , y1) treal_le (x2 , y2)) = ((hreal_le ((hreal_add x1) y2)) ((hreal_add x2) y1)))))))
T c= ctreal_le c, f0 f1 c, f2 f3 chreal_le chreal_add f0 f3 chreal_add f2 f1
D treal_eq
A |- (!x1. (!y2. (!x2. (!y1. (((x1 , y1) treal_eq (x2 , y2)) = (((hreal_add x1) y2) = ((hreal_add x2) y1)))))))
T c= ctreal_eq c, f0 f1 c, f2 f3 c= chreal_add f0 f3 chreal_add f2 f1
+ |- (((!x. (!y. ((((hreal_le x) y) /\ ((hreal_le y) x)) = (x = y)))) ==> ((\x. (!y. ((((hreal_le x) y) /\ ((hreal_le y) x)) = (x = y)))) x)) = ((\x. (!y. ((((hreal_le x) y) /\ ((hreal_le y) x)) = (x = y)))) x))
T c= c==> ! ! c= c/\ chreal_le b0 b1 chreal_le b1 b0 c= b0 b1 * / ! c= c/\ chreal_le b0 b1 chreal_le b1 b0 c= b0 b1 f0 * / ! c= c/\ chreal_le b0 b1 chreal_le b1 b0 c= b0 b1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
