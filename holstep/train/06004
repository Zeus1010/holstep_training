N Planarity.FINITE_FACE_FAN
C |- (!x. (!V. (!E. (!ds. (((FAN (x , (V , E))) /\ (ds IN (face_set (hypermap1_of_fanx (x , (V , E)))))) ==> (FINITE ds))))))
T c==> c/\ cFAN c, f0 c, f1 f2 cIN f3 * part cface_set chypermap1_of_fanx c, f0 c, f1 f2 cFINITE f3
D TRUTH
A |- T
T cT
D FINITE_SUBSET
A |- (!s. (!t. (((FINITE t) /\ (s SUBSET t)) ==> (FINITE s))))
T c==> c/\ cFINITE f0 cSUBSET f1 f0 cFINITE f1
D Fan.finite_d_fan
A |- (!x. (!V. (!E. ((FAN (x , (V , E))) ==> (FINITE (d_fan (x , (V , E))))))))
T c==> cFAN c, f0 c, f1 f2 cFINITE * part cd_fan c, f0 c, f1 f2
D Fan.face_subset_dart_fan
A |- (!x. (!V. (!E. (!ds. (((FAN (x , (V , E))) /\ (ds IN (face_set (hypermap1_of_fanx (x , (V , E)))))) ==> (ds SUBSET (d_fan (x , (V , E)))))))))
T c==> c/\ cFAN c, f0 c, f1 f2 cIN f3 * part cface_set chypermap1_of_fanx c, f0 c, f1 f2 cSUBSET f3 * part cd_fan c, f0 c, f1 f2
+ (FAN (x , (V , E))), (ds SUBSET (d_fan (x , (V , E)))) |- (((FAN (x , (V , E))) ==> (FINITE (d_fan (x , (V , E))))) ==> (FINITE ds))
T c==> cFAN c, f0 c, f1 f2 c==> cSUBSET f3 * part cd_fan c, f0 c, f1 f2 c==> c==> cFAN c, f0 c, f1 f2 cFINITE * part cd_fan c, f0 c, f1 f2 cFINITE f3
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
