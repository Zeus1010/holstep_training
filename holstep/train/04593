N LOCALLY_COMPACT_OPEN_UNION
C |- (!s. (!t. (!u. ((((locally compact) s) /\ (((locally compact) t) /\ (((open_in ((subtopology euclidean) u)) s) /\ ((open_in ((subtopology euclidean) u)) t)))) ==> ((locally compact) (s UNION t))))))
T c==> c/\ clocally part ccompact f0 c/\ clocally part ccompact f1 c/\ copen_in csubtopology ceuclidean f2 f0 copen_in csubtopology ceuclidean f2 f1 clocally part ccompact * * part cUNION f0 f1
D TRUTH
A |- T
T cT
D NOT_IN_EMPTY
A |- (!x. (~ (x IN EMPTY)))
T c~ cIN f0 part cEMPTY
D FORALL_IN_INSERT
A |- (!P. (!a. (!s. ((!x. ((x IN (a INSERT s)) ==> (P x))) = ((P a) /\ (!x. ((x IN s) ==> (P x))))))))
T c= ! c==> cIN b0 * * part cINSERT f0 f1 * f2 b0 c/\ * f2 f0 ! c==> cIN b0 f1 * f2 b0
D UNIONS_2
A |- ((UNIONS (s INSERT (t INSERT EMPTY))) = (s UNION t))
T c= * part cUNIONS * * part cINSERT f0 * * part cINSERT f1 part cEMPTY * * part cUNION f0 f1
D LOCALLY_COMPACT_OPEN_UNIONS
A |- (!f. (!u. (((!c. ((c IN f) ==> ((locally compact) c))) /\ (!c. ((c IN f) ==> ((open_in ((subtopology euclidean) u)) c)))) ==> ((locally compact) (UNIONS f)))))
T c==> c/\ ! c==> cIN b0 f0 clocally part ccompact b0 ! c==> cIN b0 f0 copen_in csubtopology ceuclidean f1 b0 clocally part ccompact * part cUNIONS f0
+ (((locally compact) s) /\ (((locally compact) t) /\ (((open_in ((subtopology euclidean) u)) s) /\ ((open_in ((subtopology euclidean) u)) t)))), ((locally compact) s) |- ((locally compact) (s UNION t))
T c==> c/\ clocally part ccompact f0 c/\ clocally part ccompact f1 c/\ copen_in csubtopology ceuclidean f2 f0 copen_in csubtopology ceuclidean f2 f1 c==> clocally part ccompact f0 clocally part ccompact * * part cUNION f0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
