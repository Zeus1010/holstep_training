N Hypermap_iso.permutes_imp_bij
C |- (!s. (!p. (((permutes p) s) ==> (((BIJ p) s) s))))
T c==> cpermutes f0 f1 cBIJ f0 f1 f1
D INJ
A |- (!t. (!s. (!f. ((((INJ f) s) t) = ((!x. ((x IN s) ==> ((f x) IN t))) /\ (!x. (!y. (((x IN s) /\ ((y IN s) /\ ((f x) = (f y)))) ==> (x = y)))))))))
T c= cINJ f0 f1 f2 c/\ ! c==> cIN b0 f1 cIN * f0 b0 f2 ! ! c==> c/\ cIN b0 f1 c/\ cIN b1 f1 c= * f0 b0 * f0 b1 c= b0 b1
D SURJ
A |- (!t. (!s. (!f. ((((SURJ f) s) t) = ((!x. ((x IN s) ==> ((f x) IN t))) /\ (!x. ((x IN t) ==> (?y. ((y IN s) /\ ((f y) = x))))))))))
T c= cSURJ f0 f1 f2 c/\ ! c==> cIN b0 f1 cIN * f0 b0 f2 ! c==> cIN b0 f2 c? / c/\ cIN b1 f1 c= * f0 b1 b0
D BIJ
A |- (!f. (!s. (!t. ((((BIJ f) s) t) = ((((INJ f) s) t) /\ (((SURJ f) s) t))))))
T c= cBIJ f0 f1 f2 c/\ cINJ f0 f1 f2 cSURJ f0 f1 f2
D PERMUTES_IN_IMAGE
A |- (!p. (!s. (!x. (((permutes p) s) ==> (((p x) IN s) = (x IN s))))))
T c==> cpermutes f0 f1 c= cIN * f0 f2 f1 cIN f2 f1
D PERMUTES_INJECTIVE
A |- (!p. (!s. (((permutes p) s) ==> (!x. (!y. (((p x) = (p y)) = (x = y)))))))
T c==> cpermutes f0 f1 ! ! c= c= * f0 b0 * f0 b1 c= b0 b1
D PERMUTES_SURJECTIVE
A |- (!p. (!s. (((permutes p) s) ==> (!y. (?x. ((p x) = y))))))
T c==> cpermutes f0 f1 ! c? / c= * f0 b1 b0
+ |- ((!p. (((permutes p) s) ==> (((BIJ p) s) s))) = T)
T c= ! c==> cpermutes b0 f0 cBIJ b0 f0 f0 cT
- (FINITE s), (FINITE t), ((CARD s) = (CARD t)) |- ((FINITE s) /\ (!f. ((((SURJ f) s) t) = (((BIJ f) s) t))))
T c==> cFINITE f0 c==> cFINITE f1 c==> c= cCARD f0 cCARD f1 c/\ cFINITE f0 ! c= cSURJ b0 f0 f1 cBIJ b0 f0 f1
