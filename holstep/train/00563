N Planarity.ball_eq_normball
C |- (!x. (!r. ((ball (x , r)) = ((normball x) r))))
T c= * part cball c, f0 f1 * * part cnormball f0 f1
D DIST_SYM
A |- (!x. (!y. ((distance (x , y)) = (distance (y , x)))))
T c= cdistance c, f0 f1 cdistance c, f1 f0
D ball
A |- (!x. (!e. ((ball (x , e)) = (GSPEC (\GEN%PVAR%553. (?y. (((SETSPEC GEN%PVAR%553) ((real_lt (distance (x , y))) e)) y)))))))
T c= * part cball c, f0 f1 * part cGSPEC / c? / cSETSPEC b0 creal_lt cdistance c, f0 b1 f1 b1
D normball
A |- (!x. (!r. (((normball x) r) = (GSPEC (\GEN%PVAR%5172. (?y. (((SETSPEC GEN%PVAR%5172) ((real_lt (distance (y , x))) r)) y)))))))
T c= * * part cnormball f0 f1 * part cGSPEC / c? / cSETSPEC b0 creal_lt cdistance c, b1 f0 f1 b1
+ |- ((\y. ((distance (x , y)) = (distance (y , x)))) y)
T * / c= cdistance c, f0 b0 cdistance c, b0 f0 f1
+ |- ((\x. (!y. ((distance (x , y)) = (distance (y , x))))) x)
T * / ! c= cdistance c, b0 b1 cdistance c, b1 b0 f0
- |- ((?y. t) = t)
T c= c? / f0 f0
- |- (T = ((ball (x , r)) = ((normball x) r)))
T c= cT c= * part cball c, f0 f1 * * part cnormball f0 f1
