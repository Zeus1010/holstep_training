N ZIP
C |- ((((ZIP NIL) NIL) = NIL) /\ (((ZIP ((CONS h1) t1)) ((CONS h2) t2)) = ((CONS (h1 , h2)) ((ZIP t1) t2))))
T c/\ c= cZIP cNIL cNIL cNIL c= cZIP cCONS f0 f1 cCONS f2 f3 cCONS c, f0 f2 cZIP f1 f3
D TRUTH
A |- T
T cT
D HD
A |- ((HD ((CONS h) t)) = h)
T c= cHD cCONS f0 f1 f0
D TL
A |- ((TL ((CONS h) t)) = t)
T c= cTL cCONS f0 f1 f1
D ZIP_DEF
A |- ((((ZIP NIL) l2) = NIL) /\ (((ZIP ((CONS h1) t1)) l2) = ((CONS (h1 , (HD l2))) ((ZIP t1) (TL l2)))))
T c/\ c= cZIP cNIL f0 cNIL c= cZIP cCONS f1 f2 f0 cCONS c, f1 cHD f0 cZIP f2 cTL f0
+ |- (((ZIP NIL) l2) = NIL)
T c= cZIP cNIL f0 cNIL
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
