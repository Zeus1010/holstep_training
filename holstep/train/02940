N Reduction4.EL_SUC_CONS
C |- (!i. (!h. (!t. (((EL (SUC i)) ((CONS h) t)) = ((EL i) t)))))
T c= cEL cSUC f0 cCONS f1 f2 cEL f0 f2
D TRUTH
A |- T
T cT
D TL
A |- ((TL ((CONS h) t)) = t)
T c= cTL cCONS f0 f1 f1
D EL
A |- ((((EL (NUMERAL _0)) l) = (HD l)) /\ (((EL (SUC n)) l) = ((EL n) (TL l))))
T c/\ c= cEL cNUMERAL c_0 f0 cHD f0 c= cEL cSUC f1 f0 cEL f1 cTL f0
+ |- ((!t. t) = t)
T c= ! f0 f0
- |- (((EL (NUMERAL _0)) l) = (HD l))
T c= cEL cNUMERAL c_0 f0 cHD f0
