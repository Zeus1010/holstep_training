D Trigonometry.BKMUSOX1
A |- (!x. (!y. (((cross x) y) = (vector_neg ((cross y) x)))))
T c= ccross f0 f1 cvector_neg ccross f1 f0
D CROSS_REFL
A |- (!x. (((cross x) x) = (vec (NUMERAL _0))))
T c= ccross f0 f0 cvec cNUMERAL c_0
D CROSS_LADD
A |- (!x. (!y. (!z. (((cross ((vector_add x) y)) z) = ((vector_add ((cross x) z)) ((cross y) z))))))
T c= ccross cvector_add f0 f1 f2 cvector_add ccross f0 f2 ccross f1 f2
D CROSS_RADD
A |- (!x. (!y. (!z. (((cross x) ((vector_add y) z)) = ((vector_add ((cross x) y)) ((cross x) z))))))
T c= ccross f0 cvector_add f1 f2 cvector_add ccross f0 f1 ccross f0 f2
D CROSS_LMUL
A |- (!c. (!x. (!y. (((cross ((% c) x)) y) = ((% c) ((cross x) y))))))
T c= ccross c% f0 f1 f2 c% f0 ccross f1 f2
D CROSS_RMUL
A |- (!c. (!x. (!y. (((cross x) ((% c) y)) = ((% c) ((cross x) y))))))
T c= ccross f0 c% f1 f2 c% f1 ccross f0 f2
D DOT_CROSS_SELF
