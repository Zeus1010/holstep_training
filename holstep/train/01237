N CX_ABS
C |- (!x. ((Cx (real_abs x)) = (Cx (vector_norm (Cx x)))))
T c= cCx creal_abs f0 cCx cvector_norm cCx f0
D TRUTH
A |- T
T cT
D REAL_ADD_RID
A |- (!x. (((real_add x) (real_of_num (NUMERAL _0))) = x))
T c= creal_add f0 creal_of_num cNUMERAL c_0 f0
D Real_ext.REAL_SV_LMUL_0
A |- (!x. (((real_mul (real_of_num (NUMERAL _0))) x) = (real_of_num (NUMERAL _0))))
T c= creal_mul creal_of_num cNUMERAL c_0 f0 creal_of_num cNUMERAL c_0
D Float.POW_2
A |- (!x. (((real_pow x) (NUMERAL (BIT0 (BIT1 _0)))) = ((real_mul x) x)))
T c= creal_pow f0 cNUMERAL cBIT0 cBIT1 c_0 creal_mul f0 f0
D POW_2_SQRT_ABS
A |- (!x. ((sqrt ((real_pow x) (NUMERAL (BIT0 (BIT1 _0))))) = (real_abs x)))
T c= csqrt creal_pow f0 cNUMERAL cBIT0 cBIT1 c_0 creal_abs f0
D CX_DEF
A |- (!a. ((Cx a) = (complex (a , (real_of_num (NUMERAL _0))))))
T c= cCx f0 ccomplex c, f0 creal_of_num cNUMERAL c_0
D RE
A |- ((Re (complex (x , y))) = x)
T c= cRe ccomplex c, f0 f1 f0
D IM
A |- ((Im (complex (x , y))) = y)
T c= cIm ccomplex c, f0 f1 f1
D COMPLEX_EQ
A |- (!w. (!z. ((w = z) = (((Re w) = (Re z)) /\ ((Im w) = (Im z))))))
T c= c= f0 f1 c/\ c= cRe f0 cRe f1 c= cIm f0 cIm f1
D complex_norm
A |- ((vector_norm z) = (sqrt ((real_add ((real_pow (Re z)) (NUMERAL (BIT0 (BIT1 _0))))) ((real_pow (Im z)) (NUMERAL (BIT0 (BIT1 _0)))))))
T c= cvector_norm f0 csqrt creal_add creal_pow cRe f0 cNUMERAL cBIT0 cBIT1 c_0 creal_pow cIm f0 cNUMERAL cBIT0 cBIT1 c_0
+ |- ((t /\ T) = t)
T c= c/\ f0 cT f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
