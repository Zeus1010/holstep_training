N MAP2
C |- (((((MAP2 f) NIL) NIL) = NIL) /\ ((((MAP2 f) ((CONS h1) t1)) ((CONS h2) t2)) = ((CONS ((f h1) h2)) (((MAP2 f) t1) t2))))
T c/\ c= cMAP2 f0 cNIL cNIL cNIL c= cMAP2 f0 cCONS f1 f2 cCONS f3 f4 cCONS * * f0 f1 f3 cMAP2 f0 f2 f4
D TRUTH
A |- T
T cT
D HD
A |- ((HD ((CONS h) t)) = h)
T c= cHD cCONS f0 f1 f0
D TL
A |- ((TL ((CONS h) t)) = t)
T c= cTL cCONS f0 f1 f1
D MAP2_DEF
A |- (((((MAP2 f) NIL) l) = NIL) /\ ((((MAP2 f) ((CONS h1) t1)) l) = ((CONS ((f h1) (HD l))) (((MAP2 f) t1) (TL l)))))
T c/\ c= cMAP2 f0 cNIL f1 cNIL c= cMAP2 f0 cCONS f2 f3 f1 cCONS * * f0 f2 cHD f1 cMAP2 f0 f3 cTL f1
+ |- ((x = x) = T)
T c= c= f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
