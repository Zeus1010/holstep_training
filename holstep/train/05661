N REAL_OPEN
C |- (!s. ((real_open s) = (open ((IMAGE lift) s))))
T c= creal_open f0 copen * * part cIMAGE part clift f0
D TRUTH
A |- T
T cT
D FORALL_IN_IMAGE
A |- (!f. (!s. ((!y. ((y IN ((IMAGE f) s)) ==> (P y))) = (!x. ((x IN s) ==> (P (f x)))))))
T c= ! c==> cIN b0 * * part cIMAGE f0 f1 * f2 b0 ! c==> cIN b0 f1 * f2 * f0 b0
D FORALL_LIFT
A |- ((!x. (P x)) = (!x. (P (lift x))))
T c= ! * f0 b0 ! * f0 clift b0
D LIFT_IN_IMAGE_LIFT
A |- (!x. (!s. (((lift x) IN ((IMAGE lift) s)) = (x IN s))))
T c= cIN clift f0 * * part cIMAGE part clift f1 cIN f0 f1
D DIST_LIFT
A |- (!x. (!y. ((distance ((lift x) , (lift y))) = (real_abs ((real_sub x) y)))))
T c= cdistance c, clift f0 clift f1 creal_abs creal_sub f0 f1
D open_def
A |- (!s. ((open s) = (!x. ((x IN s) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!x'. (((real_lt (distance (x' , x))) e) ==> (x' IN s)))))))))
T c= copen f0 ! c==> cIN b0 f0 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b1 ! c==> creal_lt cdistance c, b2 b0 b1 cIN b2 f0
D real_open
A |- (!s. ((real_open s) = (!x. ((x IN s) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!x'. (((real_lt (real_abs ((real_sub x') x))) e) ==> (x' IN s)))))))))
T c= creal_open f0 ! c==> cIN b0 f0 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b1 ! c==> creal_lt creal_abs creal_sub b2 b0 b1 cIN b2 f0
+ |- ((\x. (!y. ((distance ((lift x) , (lift y))) = (real_abs ((real_sub x) y))))) x)
T * / ! c= cdistance c, clift b0 clift b1 creal_abs creal_sub b0 b1 f0
+ |- ((\s. ((open s) = (!x. ((x IN s) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!x'. (((real_lt (distance (x' , x))) e) ==> (x' IN s))))))))) s)
T * / c= copen b0 ! c==> cIN b1 b0 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b2 ! c==> creal_lt cdistance c, b3 b1 b2 cIN b3 b0 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\y. ((distance ((lift x) , (lift y))) = (real_abs ((real_sub x) y)))) y)
T * / c= cdistance c, clift f0 clift b0 creal_abs creal_sub f0 b0 f1
- |- ((?e. t) = t)
T c= c? / f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
