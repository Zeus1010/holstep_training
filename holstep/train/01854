N REALLIM_RMUL
C |- (!f. (!l. (!c. ((((---> f) l) net) ==> (((---> (\x. ((real_mul (f x)) c))) ((real_mul l) c)) net)))))
T c==> c---> f0 f1 f2 c---> / creal_mul * f0 b0 f3 creal_mul f1 f3 f2
D TRUTH
A |- T
T cT
D REAL_MUL_SYM
A |- (!x. (!y. (((real_mul x) y) = ((real_mul y) x))))
T c= creal_mul f0 f1 creal_mul f1 f0
D REALLIM_LMUL
A |- (!f. (!l. (!c. ((((---> f) l) net) ==> (((---> (\x. ((real_mul c) (f x)))) ((real_mul c) l)) net)))))
T c==> c---> f0 f1 f2 c---> / creal_mul f3 * f0 b0 creal_mul f3 f1 f2
+ |- ((\f. (!l. (!c. ((((---> f) l) net) ==> (((---> (\x. ((real_mul c) (f x)))) ((real_mul c) l)) net))))) f)
T * / ! ! c==> c---> b0 b1 f0 c---> / creal_mul b2 * b0 b3 creal_mul b2 b1 f0 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
