+ |- ((\p'. (!q'. (((negligible s) = p') ==> ((p' ==> (((absolutely_integrable_on f) s) = q')) ==> (((negligible s) ==> ((absolutely_integrable_on f) s)) = (p' ==> q')))))) p')
T * / ! c==> c= cnegligible f0 b0 c==> c==> b0 c= cabsolutely_integrable_on f1 f0 b1 c= c==> cnegligible f0 cabsolutely_integrable_on f1 f0 c==> b0 b1 f2
+ |- (((!q'. (((negligible s) = p') ==> ((p' ==> (((absolutely_integrable_on f) s) = q')) ==> (((negligible s) ==> ((absolutely_integrable_on f) s)) = (p' ==> q'))))) ==> ((\q'. (((negligible s) = p') ==> ((p' ==> (((absolutely_integrable_on f) s) = q')) ==> (((negligible s) ==> ((absolutely_integrable_on f) s)) = (p' ==> q'))))) q')) = ((\q'. (((negligible s) = p') ==> ((p' ==> (((absolutely_integrable_on f) s) = q')) ==> (((negligible s) ==> ((absolutely_integrable_on f) s)) = (p' ==> q'))))) q'))
T c= c==> ! c==> c= cnegligible f0 f1 c==> c==> f1 c= cabsolutely_integrable_on f2 f0 b0 c= c==> cnegligible f0 cabsolutely_integrable_on f2 f0 c==> f1 b0 * / c==> c= cnegligible f0 f1 c==> c==> f1 c= cabsolutely_integrable_on f2 f0 b0 c= c==> cnegligible f0 cabsolutely_integrable_on f2 f0 c==> f1 b0 f3 * / c==> c= cnegligible f0 f1 c==> c==> f1 c= cabsolutely_integrable_on f2 f0 b0 c= c==> cnegligible f0 cabsolutely_integrable_on f2 f0 c==> f1 b0 f3
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- (negligible s) |- (T = (negligible s))
T c==> cnegligible f0 c= cT cnegligible f0
- (negligible s) |- ((negligible s) = T)
T c==> cnegligible f0 c= cnegligible f0 cT
