N IM_DIV_CX
C |- (!z. (!x. ((Im ((complex_div z) (Cx x))) = ((real_div (Im z)) x))))
T c= cIm ccomplex_div f0 cCx f1 creal_div cIm f0 f1
D TRUTH
A |- T
T cT
D real_div
A |- (!x. (!y. (((real_div x) y) = ((real_mul x) (real_inv y)))))
T c= creal_div f0 f1 creal_mul f0 creal_inv f1
D complex_div
A |- (!w. (!z. (((complex_div w) z) = ((complex_mul w) (complex_inv z)))))
T c= ccomplex_div f0 f1 ccomplex_mul f0 ccomplex_inv f1
D CX_INV
A |- (!x. ((Cx (real_inv x)) = (complex_inv (Cx x))))
T c= cCx creal_inv f0 ccomplex_inv cCx f0
D IM_MUL_CX
A |- (!x. (!z. (((Im ((complex_mul (Cx x)) z)) = ((real_mul x) (Im z))) /\ ((Im ((complex_mul z) (Cx x))) = ((real_mul (Im z)) x)))))
T c/\ c= cIm ccomplex_mul cCx f0 f1 creal_mul f0 cIm f1 c= cIm ccomplex_mul f1 cCx f0 creal_mul cIm f1 f0
+ |- ((\x. (!z. (((Im ((complex_mul (Cx x)) z)) = ((real_mul x) (Im z))) /\ ((Im ((complex_mul z) (Cx x))) = ((real_mul (Im z)) x))))) x)
T * / ! c/\ c= cIm ccomplex_mul cCx b0 b1 creal_mul b0 cIm b1 c= cIm ccomplex_mul b1 cCx b0 creal_mul cIm b1 b0 f0
- |- ((Im ((complex_mul (Cx x)) z)) = ((real_mul x) (Im z)))
T c= cIm ccomplex_mul cCx f0 f1 creal_mul f0 cIm f1
