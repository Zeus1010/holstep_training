N FACET_OF_POLYHEDRON
C |- (!s. (!c. (((polyhedron s) /\ ((facet_of c) s)) ==> (?a. (?b. ((~ (a = (vec (NUMERAL _0)))) /\ ((s SUBSET (GSPEC (\GEN%PVAR%2331. (?x. (((SETSPEC GEN%PVAR%2331) ((real_le ((dot a) x)) b)) x))))) /\ (c = (s INTER (GSPEC (\GEN%PVAR%2332. (?x. (((SETSPEC GEN%PVAR%2332) (((dot a) x) = b)) x)))))))))))))
