N COMPACT_TRANSLATION
C |- (!s. (!a. ((compact s) ==> (compact ((IMAGE (\x. ((vector_add a) x))) s)))))
T c==> ccompact f0 ccompact * * part cIMAGE / cvector_add f1 b0 f0
D TRUTH
A |- T
T cT
D CONTINUOUS_ON_CONST
A |- (!s. (!c. ((continuous_on (\x. c)) s)))
T ccontinuous_on / f0 f1
D CONTINUOUS_ON_ADD
A |- (!f. (!g. (!s. ((((continuous_on f) s) /\ ((continuous_on g) s)) ==> ((continuous_on (\x. ((vector_add (f x)) (g x)))) s)))))
T c==> c/\ ccontinuous_on f0 f1 ccontinuous_on f2 f1 ccontinuous_on / cvector_add * f0 b0 * f2 b0 f1
D CONTINUOUS_ON_ID
A |- (!s. ((continuous_on (\x. x)) s))
T ccontinuous_on / b0 f0
D COMPACT_CONTINUOUS_IMAGE
A |- (!f. (!s. ((((continuous_on f) s) /\ (compact s)) ==> (compact ((IMAGE f) s)))))
T c==> c/\ ccontinuous_on f0 f1 ccompact f1 ccompact * * part cIMAGE f0 f1
+ (((continuous_on f) s) /\ (compact s)) |- ((((continuous_on f) s) /\ (compact s)) /\ ((compact ((IMAGE f) s)) = T))
T c==> c/\ ccontinuous_on f0 f1 ccompact f1 c/\ c/\ ccontinuous_on f0 f1 ccompact f1 c= ccompact * * part cIMAGE f0 f1 cT
+ |- ((\f. (!g. (!s. ((((continuous_on f) s) /\ ((continuous_on g) s)) ==> ((continuous_on (\x. ((vector_add (f x)) (g x)))) s))))) f)
T * / ! ! c==> c/\ ccontinuous_on b0 b2 ccontinuous_on b1 b2 ccontinuous_on / cvector_add * b0 b3 * b1 b3 b2 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
