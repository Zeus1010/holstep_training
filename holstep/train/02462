N HULL_UNION_SUBSET
C |- (!P. (!s. (!t. ((((hull P) s) UNION ((hull P) t)) SUBSET ((hull P) (s UNION t))))))
T cSUBSET * * part cUNION * * part chull f0 f1 * * part chull f0 f2 * * part chull f0 * * part cUNION f1 f2
D TRUTH
A |- T
T cT
D SUBSET_UNION
A |- ((!s. (!t. (s SUBSET (s UNION t)))) /\ (!s. (!t. (s SUBSET (t UNION s)))))
T c/\ ! ! cSUBSET b0 * * part cUNION b0 b1 ! ! cSUBSET b0 * * part cUNION b1 b0
D UNION_SUBSET
A |- (!s. (!t. (!u. (((s UNION t) SUBSET u) = ((s SUBSET u) /\ (t SUBSET u))))))
T c= cSUBSET * * part cUNION f0 f1 f2 c/\ cSUBSET f0 f2 cSUBSET f1 f2
D HULL_MONO
A |- (!P. (!s. (!t. ((s SUBSET t) ==> (((hull P) s) SUBSET ((hull P) t))))))
T c==> cSUBSET f0 f1 cSUBSET * * part chull f2 f0 * * part chull f2 f1
+ |- ((\P. (!s. (!t. ((s SUBSET t) ==> (((hull P) s) SUBSET ((hull P) t)))))) P)
T * / ! ! c==> cSUBSET b1 b2 cSUBSET * * part chull b0 b1 * * part chull b0 b2 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
