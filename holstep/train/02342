N LEFT_FORALL_OR_THM
C |- (!P. (!Q. ((!x. ((P x) \/ Q)) = ((!x. (P x)) \/ Q))))
T c= ! c\/ * f0 b0 f1 c\/ ! * f0 b0 f1
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D DE_MORGAN_THM
A |- (!t1. (!t2. (((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2))) /\ ((~ (t1 \/ t2)) = ((~ t1) /\ (~ t2))))))
T c/\ c= c~ c/\ f0 f1 c\/ c~ f0 c~ f1 c= c~ c\/ f0 f1 c/\ c~ f0 c~ f1
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D NOT_FORALL_THM
A |- (!P. ((~ (!x. (P x))) = (?x. (~ (P x)))))
T c= c~ ! * f0 b0 c? / c~ * f0 b0
+ |- ((b = (~ (~ b))) = ((T = b) = ((~ T) = (~ b))))
T c= c= f0 c~ c~ f0 c= c= cT f0 c= c~ cT c~ f0
+ |- (((!x. ((P x) \/ Q)) = ((!x. (P x)) \/ Q)) = T)
T c= c= ! c\/ * f0 b0 f1 c\/ ! * f0 b0 f1 cT
+ |- ((~ (!x. ((P x) \/ Q))) = (~ ((!x. (P x)) \/ Q)))
T c= c~ ! c\/ * f0 b0 f1 c~ c\/ ! * f0 b0 f1
- |- ((?x. t) = t)
T c= c? / f0 f0
- |- ((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2)))
T c= c~ c/\ f0 f1 c\/ c~ f0 c~ f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
