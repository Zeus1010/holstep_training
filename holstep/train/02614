N AR_SING
C |- (!a. (AR (a INSERT EMPTY)))
T cAR * * part cINSERT f0 part cEMPTY
D TRUTH
A |- T
T cT
D NOT_INSERT_EMPTY
A |- (!x. (!s. (~ ((x INSERT s) = EMPTY))))
T c~ c= * * part cINSERT f0 f1 part cEMPTY
D CONVEX_SING
A |- (!a. (convex (a INSERT EMPTY)))
T cconvex * * part cINSERT f0 part cEMPTY
D CONVEX_IMP_AR
A |- (!s. (((convex s) /\ (~ (s = EMPTY))) ==> (AR s)))
T c==> c/\ cconvex f0 c~ c= f0 part cEMPTY cAR f0
+ |- (((!a. (convex (a INSERT EMPTY))) ==> ((\a. (convex (a INSERT EMPTY))) a)) = ((\a. (convex (a INSERT EMPTY))) a))
T c= c==> ! cconvex * * part cINSERT b0 part cEMPTY * / cconvex * * part cINSERT b0 part cEMPTY f0 * / cconvex * * part cINSERT b0 part cEMPTY f0
+ |- ((\s. (((convex s) /\ (~ (s = EMPTY))) ==> (AR s))) s)
T * / c==> c/\ cconvex b0 c~ c= b0 part cEMPTY cAR b0 f0
+ |- ((!a. t) = t)
T c= ! f0 f0
+ |- (((convex s) /\ (~ (s = EMPTY))) ==> ((AR s) = T))
T c==> c/\ cconvex f0 c~ c= f0 part cEMPTY c= cAR f0 cT
- ((x INSERT s) = EMPTY) |- (EMPTY = (x INSERT s))
T c==> c= * * part cINSERT f0 f1 part cEMPTY c= part cEMPTY * * part cINSERT f0 f1
- (EMPTY = (x INSERT s)) |- ((x INSERT s) = EMPTY)
T c==> c= part cEMPTY * * part cINSERT f0 f1 c= * * part cINSERT f0 f1 part cEMPTY
+ |- ((convex (a INSERT EMPTY)) = T)
T c= cconvex * * part cINSERT f0 part cEMPTY cT
+ |- (((!s. (~ ((x INSERT s) = EMPTY))) ==> ((\s. (~ ((x INSERT s) = EMPTY))) s)) = ((\s. (~ ((x INSERT s) = EMPTY))) s))
T c= c==> ! c~ c= * * part cINSERT f0 b0 part cEMPTY * / c~ c= * * part cINSERT f0 b0 part cEMPTY f1 * / c~ c= * * part cINSERT f0 b0 part cEMPTY f1
- |- ((EMPTY = (x INSERT s)) = F)
T c= c= part cEMPTY * * part cINSERT f0 f1 cF
- |- (((~ (EMPTY = (x INSERT s))) ==> ((EMPTY = (x INSERT s)) = F)) = ((EMPTY = (x INSERT s)) = F))
T c= c==> c~ c= part cEMPTY * * part cINSERT f0 f1 c= c= part cEMPTY * * part cINSERT f0 f1 cF c= c= part cEMPTY * * part cINSERT f0 f1 cF
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- (~ (EMPTY = (x INSERT s)))
T c~ c= part cEMPTY * * part cINSERT f0 f1
