N CONTRACTIBLE_IMP_PATH_CONNECTED
C |- (!s. ((contractible s) ==> (path_connected s)))
T c==> ccontractible f0 cpath_connected f0
D TRUTH
A |- T
T cT
D SIMPLY_CONNECTED_IMP_PATH_CONNECTED
A |- (!s. ((simply_connected s) ==> (path_connected s)))
T c==> csimply_connected f0 cpath_connected f0
D CONTRACTIBLE_IMP_SIMPLY_CONNECTED
A |- (!s. ((contractible s) ==> (simply_connected s)))
T c==> ccontractible f0 csimply_connected f0
+ |- (T = (!s. ((contractible s) ==> (path_connected s))))
T c= cT ! c==> ccontractible b0 cpath_connected b0
+ |- (((!p'. (!q'. (((contractible s) = p') ==> ((p' ==> ((path_connected s) = q')) ==> (((contractible s) ==> (path_connected s)) = (p' ==> q')))))) ==> ((\p'. (!q'. (((contractible s) = p') ==> ((p' ==> ((path_connected s) = q')) ==> (((contractible s) ==> (path_connected s)) = (p' ==> q')))))) p')) = ((\p'. (!q'. (((contractible s) = p') ==> ((p' ==> ((path_connected s) = q')) ==> (((contractible s) ==> (path_connected s)) = (p' ==> q')))))) p'))
T c= c==> ! ! c==> c= ccontractible f0 b0 c==> c==> b0 c= cpath_connected f0 b1 c= c==> ccontractible f0 cpath_connected f0 c==> b0 b1 * / ! c==> c= ccontractible f0 b0 c==> c==> b0 c= cpath_connected f0 b1 c= c==> ccontractible f0 cpath_connected f0 c==> b0 b1 f1 * / ! c==> c= ccontractible f0 b0 c==> c==> b0 c= cpath_connected f0 b1 c= c==> ccontractible f0 cpath_connected f0 c==> b0 b1 f1
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((simply_connected s) ==> ((path_connected s) = T))
T c==> csimply_connected f0 c= cpath_connected f0 cT
+ |- ((\p'. (!q'. (((contractible s) = p') ==> ((p' ==> ((path_connected s) = q')) ==> (((contractible s) ==> (path_connected s)) = (p' ==> q')))))) p')
T * / ! c==> c= ccontractible f0 b0 c==> c==> b0 c= cpath_connected f0 b1 c= c==> ccontractible f0 cpath_connected f0 c==> b0 b1 f1
+ |- ((!s. t) = t)
T c= ! f0 f0
- (contractible s) |- (T = (simply_connected s))
T c==> ccontractible f0 c= cT csimply_connected f0
- (contractible s) |- ((contractible s) = T)
T c==> ccontractible f0 c= ccontractible f0 cT
- |- ((contractible s) ==> ((simply_connected s) = T))
T c==> ccontractible f0 c= csimply_connected f0 cT
- (contractible s) |- ((contractible s) /\ ((simply_connected s) = T))
T c==> ccontractible f0 c/\ ccontractible f0 c= csimply_connected f0 cT
- (contractible s) |- (T = (contractible s))
T c==> ccontractible f0 c= cT ccontractible f0
