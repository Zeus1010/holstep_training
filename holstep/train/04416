N LEBESGUE_MEASURABLE_OPEN
C |- (!s. ((open s) ==> (lebesgue_measurable s)))
T c==> copen f0 clebesgue_measurable f0
D TRUTH
A |- T
T cT
D OPEN_CLOSED
A |- (!s. ((open s) = (closed (UNIV DIFF s))))
T c= copen f0 cclosed * * part cDIFF part cUNIV f0
D LEBESGUE_MEASURABLE_COMPL
A |- (!s. ((lebesgue_measurable (UNIV DIFF s)) = (lebesgue_measurable s)))
T c= clebesgue_measurable * * part cDIFF part cUNIV f0 clebesgue_measurable f0
D LEBESGUE_MEASURABLE_CLOSED
A |- (!s. ((closed s) ==> (lebesgue_measurable s)))
T c==> cclosed f0 clebesgue_measurable f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!s. ((closed (UNIV DIFF s)) ==> (lebesgue_measurable s))) = (!s. ((open s) ==> (lebesgue_measurable s))))
T c= ! c==> cclosed * * part cDIFF part cUNIV b0 clebesgue_measurable b0 ! c==> copen b0 clebesgue_measurable b0
+ |- ((\s. ((open s) = (closed (UNIV DIFF s)))) s)
T * / c= copen b0 cclosed * * part cDIFF part cUNIV b0 f0
+ ((lebesgue_measurable s) = (lebesgue_measurable (UNIV DIFF s))) |- ((lebesgue_measurable (UNIV DIFF s)) = (lebesgue_measurable s))
T c==> c= clebesgue_measurable f0 clebesgue_measurable * * part cDIFF part cUNIV f0 c= clebesgue_measurable * * part cDIFF part cUNIV f0 clebesgue_measurable f0
+ |- (((!s. ((closed s) ==> (lebesgue_measurable s))) ==> ((\s. ((closed s) ==> (lebesgue_measurable s))) s)) = ((\s. ((closed s) ==> (lebesgue_measurable s))) s))
T c= c==> ! c==> cclosed b0 clebesgue_measurable b0 * / c==> cclosed b0 clebesgue_measurable b0 f0 * / c==> cclosed b0 clebesgue_measurable b0 f0
- |- ((!s. t) = t)
T c= ! f0 f0
- (closed (UNIV DIFF s)) |- (T = (closed (UNIV DIFF s)))
T c==> cclosed * * part cDIFF part cUNIV f0 c= cT cclosed * * part cDIFF part cUNIV f0
- (closed (UNIV DIFF s)) |- ((closed (UNIV DIFF s)) = T)
T c==> cclosed * * part cDIFF part cUNIV f0 c= cclosed * * part cDIFF part cUNIV f0 cT
