N LEBESGUE_MEASURABLE_CLOSED
C |- (!s. ((closed s) ==> (lebesgue_measurable s)))
T c==> cclosed f0 clebesgue_measurable f0
D TRUTH
A |- T
T cT
D CLOSED_INTER_COMPACT
A |- (!s. (!t. (((closed s) /\ (compact t)) ==> (compact (s INTER t)))))
T c==> c/\ cclosed f0 ccompact f1 ccompact * * part cINTER f0 f1
D COMPACT_INTERVAL
A |- (!a. (!b. (compact (closed_interval ((CONS (a , b)) NIL)))))
T ccompact * part cclosed_interval cCONS c, f0 f1 cNIL
D LEBESGUE_MEASURABLE_COMPACT
A |- (!s. ((compact s) ==> (lebesgue_measurable s)))
T c==> ccompact f0 clebesgue_measurable f0
D LEBESGUE_MEASURABLE_ON_SUBINTERVALS
A |- (!s. ((lebesgue_measurable s) = (!a. (!b. (lebesgue_measurable (s INTER (closed_interval ((CONS (a , b)) NIL))))))))
T c= clebesgue_measurable f0 ! ! clebesgue_measurable * * part cINTER f0 * part cclosed_interval cCONS c, b0 b1 cNIL
+ (closed s) |- ((compact (s INTER (closed_interval ((CONS (a , b)) NIL)))) = T)
T c==> cclosed f0 c= ccompact * * part cINTER f0 * part cclosed_interval cCONS c, f1 f2 cNIL cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
