N REAL_INTERVAL_NE_EMPTY
C |- ((!a. (!b. ((~ ((closed_real_interval ((CONS (a , b)) NIL)) = EMPTY)) = ((real_le a) b)))) /\ (!a. (!b. ((~ ((open_real_interval (a , b)) = EMPTY)) = ((real_lt a) b)))))
T c/\ ! ! c= c~ c= * part cclosed_real_interval cCONS c, b0 b1 cNIL part cEMPTY creal_le b0 b1 ! ! c= c~ c= * part copen_real_interval c, b0 b1 part cEMPTY creal_lt b0 b1
D TRUTH
A |- T
T cT
D REAL_NOT_LT
A |- (!x. (!y. ((~ ((real_lt x) y)) = ((real_le y) x))))
T c= c~ creal_lt f0 f1 creal_le f1 f0
D REAL_NOT_LE
A |- (!x. (!y. ((~ ((real_le x) y)) = ((real_lt y) x))))
T c= c~ creal_le f0 f1 creal_lt f1 f0
D REAL_INTERVAL_EQ_EMPTY
A |- ((!a. (!b. (((closed_real_interval ((CONS (a , b)) NIL)) = EMPTY) = ((real_lt b) a)))) /\ (!a. (!b. (((open_real_interval (a , b)) = EMPTY) = ((real_le b) a)))))
T c/\ ! ! c= c= * part cclosed_real_interval cCONS c, b0 b1 cNIL part cEMPTY creal_lt b1 b0 ! ! c= c= * part copen_real_interval c, b0 b1 part cEMPTY creal_le b1 b0
+ |- ((\a. (!b. (((open_real_interval (a , b)) = EMPTY) = ((real_le b) a)))) a)
T * / ! c= c= * part copen_real_interval c, b0 b1 part cEMPTY creal_le b1 b0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
