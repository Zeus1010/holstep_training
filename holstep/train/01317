N Ssrnat.muln_subl
C |- (!m. (!n. (!p. (((m - n) * p) = ((m * p) - (n * p))))))
T c= c* c- f0 f1 f2 c- c* f0 f2 c* f1 f2
D MULT_SYM
A |- (!m. (!n. ((m * n) = (n * m))))
T c= c* f0 f1 c* f1 f0
D Ssrnat.muln_subr
A |- (!m. (!n. (!p. ((m * (n - p)) = ((m * n) - (m * p))))))
T c= c* f0 c- f1 f2 c- c* f0 f1 c* f0 f2
+ |- ((\n. (!p. ((m * (n - p)) = ((m * n) - (m * p))))) n)
T * / ! c= c* f0 c- b0 b1 c- c* f0 b0 c* f0 b1 f1
+ |- ((!p. ((p * (m - n)) = ((p * m) - (p * n)))) = T)
T c= ! c= c* b0 c- f0 f1 c- c* b0 f0 c* b0 f1 cT
- |- ((!p. t) = t)
T c= ! f0 f0
- |- ((x = x) = T)
T c= c= f0 f0 cT
