N Float.TWOPOW_ABS
C |- (!a. ((real_abs (twopow a)) = (twopow a)))
T c= creal_abs ctwopow f0 ctwopow f0
D TRUTH
A |- T
T cT
D REAL_ABS_NUM
A |- (!n. ((real_abs (real_of_num n)) = (real_of_num n)))
T c= creal_abs creal_of_num f0 creal_of_num f0
D REAL_ABS_POW
A |- (!x. (!n. ((real_abs ((real_pow x) n)) = ((real_pow (real_abs x)) n))))
T c= creal_abs creal_pow f0 f1 creal_pow creal_abs f0 f1
D REAL_ABS_INV
A |- (!x. ((real_abs (real_inv x)) = (real_inv (real_abs x))))
T c= creal_abs creal_inv f0 creal_inv creal_abs f0
D Num_ext_nabs.INT_REP2
A |- (!a. (?n. ((a = (int_of_num n)) \/ (a = (int_neg (int_of_num n))))))
T c? / c\/ c= f0 cint_of_num b0 c= f0 cint_neg cint_of_num b0
D Float.TWOPOW_POS
A |- (!n. ((twopow (int_of_num n)) = ((real_pow (real_of_num (NUMERAL (BIT0 (BIT1 _0))))) n)))
T c= ctwopow cint_of_num f0 creal_pow creal_of_num cNUMERAL cBIT0 cBIT1 c_0 f0
D Float.TWOPOW_NEG
A |- (!n. ((twopow (int_neg (int_of_num n))) = (real_inv ((real_pow (real_of_num (NUMERAL (BIT0 (BIT1 _0))))) n))))
T c= ctwopow cint_neg cint_of_num f0 creal_inv creal_pow creal_of_num cNUMERAL cBIT0 cBIT1 c_0 f0
