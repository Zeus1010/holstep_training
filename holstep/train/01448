N COLLINEAR_DIST_IN_CLOSED_SEGMENT
C |- (!a. (!b. (!x. (((collinear (x INSERT (a INSERT (b INSERT EMPTY)))) /\ (((real_le (distance (x , a))) (distance (a , b))) /\ ((real_le (distance (x , b))) (distance (a , b))))) ==> (x IN (closed_segment ((CONS (a , b)) NIL)))))))
T c==> c/\ ccollinear * * part cINSERT f0 * * part cINSERT f1 * * part cINSERT f2 part cEMPTY c/\ creal_le cdistance c, f0 f1 cdistance c, f1 f2 creal_le cdistance c, f0 f2 cdistance c, f1 f2 cIN f0 * part cclosed_segment cCONS c, f1 f2 cNIL
D TRUTH
A |- T
T cT
D COLLINEAR_DIST_BETWEEN
A |- (!a. (!b. (!x. (((collinear (x INSERT (a INSERT (b INSERT EMPTY)))) /\ (((real_le (distance (x , a))) (distance (a , b))) /\ ((real_le (distance (x , b))) (distance (a , b))))) ==> ((between x) (a , b))))))
T c==> c/\ ccollinear * * part cINSERT f0 * * part cINSERT f1 * * part cINSERT f2 part cEMPTY c/\ creal_le cdistance c, f0 f1 cdistance c, f1 f2 creal_le cdistance c, f0 f2 cdistance c, f1 f2 cbetween f0 c, f1 f2
D BETWEEN_IN_SEGMENT
A |- (!x. (!a. (!b. (((between x) (a , b)) = (x IN (closed_segment ((CONS (a , b)) NIL)))))))
T c= cbetween f0 c, f1 f2 cIN f0 * part cclosed_segment cCONS c, f1 f2 cNIL
+ ((x IN (closed_segment ((CONS (a , b)) NIL))) = ((between x) (a , b))) |- (((between x) (a , b)) = (x IN (closed_segment ((CONS (a , b)) NIL))))
T c==> c= cIN f0 * part cclosed_segment cCONS c, f1 f2 cNIL cbetween f0 c, f1 f2 c= cbetween f0 c, f1 f2 cIN f0 * part cclosed_segment cCONS c, f1 f2 cNIL
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!x. t) = t)
T c= ! f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
