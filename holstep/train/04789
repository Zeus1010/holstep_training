+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\q'. (((x' IN ((mball m) (x , r))) = p') ==> ((p' ==> ((x' IN (mspace m)) = q')) ==> (((x' IN ((mball m) (x , r))) ==> (x' IN (mspace m))) = (p' ==> q'))))) q')
