N HAS_SIZE_UNION
C |- (!s. (!t. (!m. (!n. (((s HAS_SIZE m) /\ ((t HAS_SIZE n) /\ ((DISJOINT s) t))) ==> ((s UNION t) HAS_SIZE (m + n)))))))
T c==> c/\ cHAS_SIZE f0 f1 c/\ cHAS_SIZE f2 f3 cDISJOINT f0 f2 cHAS_SIZE * * part cUNION f0 f2 c+ f1 f3
D TRUTH
A |- T
T cT
D DISJOINT
A |- (!s. (!t. (((DISJOINT s) t) = ((s INTER t) = EMPTY))))
T c= cDISJOINT f0 f1 c= * * part cINTER f0 f1 part cEMPTY
D FINITE_UNION
A |- (!s. (!t. ((FINITE (s UNION t)) = ((FINITE s) /\ (FINITE t)))))
T c= cFINITE * * part cUNION f0 f1 c/\ cFINITE f0 cFINITE f1
D CARD_UNION
A |- (!s. (!t. (((FINITE s) /\ ((FINITE t) /\ ((s INTER t) = EMPTY))) ==> ((CARD (s UNION t)) = ((CARD s) + (CARD t))))))
T c==> c/\ cFINITE f0 c/\ cFINITE f1 c= * * part cINTER f0 f1 part cEMPTY c= cCARD * * part cUNION f0 f1 c+ cCARD f0 cCARD f1
D HAS_SIZE
A |- (!s. (!n. ((s HAS_SIZE n) = ((FINITE s) /\ ((CARD s) = n)))))
T c= cHAS_SIZE f0 f1 c/\ cFINITE f0 c= cCARD f0 f1
+ (((FINITE s) /\ ((CARD s) = m)) /\ (((FINITE t) /\ ((CARD t) = n)) /\ ((s INTER t) = EMPTY))) |- ((CARD s) = m)
T c==> c/\ c/\ cFINITE f0 c= cCARD f0 f1 c/\ c/\ cFINITE f2 c= cCARD f2 f3 c= * * part cINTER f0 f2 part cEMPTY c= cCARD f0 f1
+ |- ((((s HAS_SIZE m) /\ ((t HAS_SIZE n) /\ ((DISJOINT s) t))) = p') ==> ((p' ==> (((s UNION t) HAS_SIZE (m + n)) = q')) ==> ((((s HAS_SIZE m) /\ ((t HAS_SIZE n) /\ ((DISJOINT s) t))) ==> ((s UNION t) HAS_SIZE (m + n))) = (p' ==> q'))))
T c==> c= c/\ cHAS_SIZE f0 f1 c/\ cHAS_SIZE f2 f3 cDISJOINT f0 f2 f4 c==> c==> f4 c= cHAS_SIZE * * part cUNION f0 f2 c+ f1 f3 f5 c= c==> c/\ cHAS_SIZE f0 f1 c/\ cHAS_SIZE f2 f3 cDISJOINT f0 f2 cHAS_SIZE * * part cUNION f0 f2 c+ f1 f3 c==> f4 f5
+ |- (((!s. (!t. (((DISJOINT s) t) = ((s INTER t) = EMPTY)))) ==> ((\s. (!t. (((DISJOINT s) t) = ((s INTER t) = EMPTY)))) s)) = ((\s. (!t. (((DISJOINT s) t) = ((s INTER t) = EMPTY)))) s))
T c= c==> ! ! c= cDISJOINT b0 b1 c= * * part cINTER b0 b1 part cEMPTY * / ! c= cDISJOINT b0 b1 c= * * part cINTER b0 b1 part cEMPTY f0 * / ! c= cDISJOINT b0 b1 c= * * part cINTER b0 b1 part cEMPTY f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((FINITE s) /\ ((FINITE t) /\ ((s INTER t) = EMPTY))) |- (((FINITE s) /\ ((FINITE t) /\ ((s INTER t) = EMPTY))) /\ ((CARD (s UNION t)) = ((CARD s) + (CARD t))))
T c==> c/\ cFINITE f0 c/\ cFINITE f1 c= * * part cINTER f0 f1 part cEMPTY c/\ c/\ cFINITE f0 c/\ cFINITE f1 c= * * part cINTER f0 f1 part cEMPTY c= cCARD * * part cUNION f0 f1 c+ cCARD f0 cCARD f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
