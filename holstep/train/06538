N APPEND_EQ_NIL
C |- (!l. (!m. ((((APPEND l) m) = NIL) = ((l = NIL) /\ (m = NIL)))))
T c= c= cAPPEND f0 f1 cNIL c/\ c= f0 cNIL c= f1 cNIL
D TRUTH
A |- T
T cT
D Ssrnat.addn_eq0
A |- (!m. (!n. (((m + n) = (NUMERAL _0)) = ((m = (NUMERAL _0)) /\ (n = (NUMERAL _0))))))
T c= c= c+ f0 f1 cNUMERAL c_0 c/\ c= f0 cNUMERAL c_0 c= f1 cNUMERAL c_0
D LENGTH_APPEND
A |- (!l. (!m. ((LENGTH ((APPEND l) m)) = ((LENGTH l) + (LENGTH m)))))
T c= cLENGTH cAPPEND f0 f1 c+ cLENGTH f0 cLENGTH f1
D LENGTH_EQ_NIL
A |- (!l. (((LENGTH l) = (NUMERAL _0)) = (l = NIL)))
T c= c= cLENGTH f0 cNUMERAL c_0 c= f0 cNIL
+ ((l = NIL) = ((LENGTH l) = (NUMERAL _0))) |- (((LENGTH l) = (NUMERAL _0)) = (l = NIL))
T c==> c= c= f0 cNIL c= cLENGTH f0 cNUMERAL c_0 c= c= cLENGTH f0 cNUMERAL c_0 c= f0 cNIL
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
