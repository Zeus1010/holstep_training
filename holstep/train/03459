N REAL_CONTINUOUS_ON_ID
C |- (!s. ((real_continuous_on (\x. x)) s))
T creal_continuous_on / b0 f0
D TRUTH
A |- T
T cT
D REAL_CONTINUOUS_WITHIN_ID
A |- (!x. (!s. ((real_continuous (\x. x)) ((within (atreal x)) s))))
T creal_continuous / b0 cwithin catreal f0 f1
D REAL_CONTINUOUS_ON_EQ_CONTINUOUS_WITHIN
A |- (!f. (!s. (((real_continuous_on f) s) = (!x. ((x IN s) ==> ((real_continuous f) ((within (atreal x)) s)))))))
T c= creal_continuous_on f0 f1 ! c==> cIN b0 f1 creal_continuous f0 cwithin catreal b0 f1
+ |- (((!x. (!s. ((real_continuous (\x. x)) ((within (atreal x)) s)))) ==> ((\x. (!s. ((real_continuous (\x. x)) ((within (atreal x)) s)))) x)) = ((\x. (!s. ((real_continuous (\x. x)) ((within (atreal x)) s)))) x))
T c= c==> ! ! creal_continuous / b2 cwithin catreal b0 b1 * / ! creal_continuous / b2 cwithin catreal b0 b1 f0 * / ! creal_continuous / b2 cwithin catreal b0 b1 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
