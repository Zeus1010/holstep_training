A |- (!f. (!g. (!s. (!x. (!d. ((((real_lt (real_of_num (NUMERAL _0))) d) /\ ((x IN s) /\ ((!x'. (((x' IN s) /\ ((real_lt (distance (x' , x))) d)) ==> ((f x') = (g x')))) /\ ((continuous f) ((within (at x)) s))))) ==> ((continuous g) ((within (at x)) s))))))))
