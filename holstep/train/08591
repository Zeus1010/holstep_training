+ |- (!n. ((((SUC (NUMERAL _0)) - (SUC n)) = ((NUMERAL _0) - n)) ==> (((SUC (NUMERAL _0)) - (SUC (SUC n))) = ((NUMERAL _0) - (SUC n)))))
