N AFF_GE_MONO_LEFT
C |- (!s. (!s'. (!t. ((s SUBSET s') ==> (((aff_ge s) t) SUBSET ((aff_ge s') t))))))
T c==> cSUBSET f0 f1 cSUBSET * * part caff_ge f0 f2 * * part caff_ge f1 f2
D TRUTH
A |- T
T cT
D aff_ge_def
A |- (aff_ge = (affsign sgn_ge))
T c= part caff_ge * part caffsign part csgn_ge
D AFFSIGN_MONO_LEFT
A |- (!sgn. (!s. (!s'. (!t. ((s SUBSET s') ==> ((((affsign sgn) s) t) SUBSET (((affsign sgn) s') t)))))))
T c==> cSUBSET f0 f1 cSUBSET * * * part caffsign f2 f0 f3 * * * part caffsign f2 f1 f3
+ |- ((\s. (!s'. (!t. ((s SUBSET s') ==> ((((affsign sgn) s) t) SUBSET (((affsign sgn) s') t)))))) s)
T * / ! ! c==> cSUBSET b0 b1 cSUBSET * * * part caffsign f0 b0 b2 * * * part caffsign f0 b1 b2 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
