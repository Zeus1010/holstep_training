N OPEN_SUBOPEN
C |- (!s. ((open s) = (!x. ((x IN s) ==> (?t. ((open t) /\ ((x IN t) /\ (t SUBSET s))))))))
T c= copen f0 ! c==> cIN b0 f0 c? / c/\ copen b1 c/\ cIN b0 b1 cSUBSET b1 f0
D TRUTH
A |- T
T cT
D OPEN_IN_SUBOPEN
A |- (!top. (!s. (((open_in top) s) = (!x. ((x IN s) ==> (?t. (((open_in top) t) /\ ((x IN t) /\ (t SUBSET s)))))))))
T c= copen_in f0 f1 ! c==> cIN b0 f1 c? / c/\ copen_in f0 b1 c/\ cIN b0 b1 cSUBSET b1 f1
D OPEN_IN
A |- (!s. ((open s) = ((open_in euclidean) s)))
T c= copen f0 copen_in ceuclidean f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\s. ((!x. ((x IN s) ==> (?t. (((open_in top) t) /\ ((x IN t) /\ (t SUBSET s)))))) = ((open_in top) s))) s)
T * / c= ! c==> cIN b1 b0 c? / c/\ copen_in f0 b2 c/\ cIN b1 b2 cSUBSET b2 b0 copen_in f0 b0 f1
- |- ((?t. t) = t)
T c= c? / f0 f0
+ |- ((\s. ((open s) = ((open_in euclidean) s))) s)
T * / c= copen b0 copen_in ceuclidean b0 f0
+ (((open_in top) s) = (!x. ((x IN s) ==> (?t. (((open_in top) t) /\ ((x IN t) /\ (t SUBSET s))))))) |- ((!x. ((x IN s) ==> (?t. (((open_in top) t) /\ ((x IN t) /\ (t SUBSET s)))))) = ((open_in top) s))
T c==> c= copen_in f0 f1 ! c==> cIN b0 f1 c? / c/\ copen_in f0 b1 c/\ cIN b0 b1 cSUBSET b1 f1 c= ! c==> cIN b0 f1 c? / c/\ copen_in f0 b1 c/\ cIN b0 b1 cSUBSET b1 f1 copen_in f0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
