N COND_ELIM_THM
C |- ((P (((COND c) x) y)) = ((c ==> (P x)) /\ ((~ c) ==> (P y))))
T c= * f0 cCOND f1 f2 f3 c/\ c==> f1 * f0 f2 c==> c~ f1 * f0 f3
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D COND_CLAUSES
A |- (!t1. (!t2. (((((COND T) t1) t2) = t1) /\ ((((COND F) t1) t2) = t2))))
T c/\ c= cCOND cT f0 f1 f0 c= cCOND cF f0 f1 f1
+ |- ((t /\ T) = t)
T c= c/\ f0 cT f0
+ |- (T = ((P (((COND T) x) y)) = ((T ==> (P x)) /\ ((~ T) ==> (P y)))))
T c= cT c= * f0 cCOND cT f1 f2 c/\ c==> cT * f0 f1 c==> c~ cT * f0 f2
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
