N REAL_EXP_MONO_LE
C |- (!x. (!y. (((real_le (exp x)) (exp y)) = ((real_le x) y))))
T c= creal_le cexp f0 cexp f1 creal_le f0 f1
D TRUTH
A |- T
T cT
D REAL_NOT_LT
A |- (!x. (!y. ((~ ((real_lt x) y)) = ((real_le y) x))))
T c= c~ creal_lt f0 f1 creal_le f1 f0
D REAL_EXP_MONO_LT
A |- (!x. (!y. (((real_lt (exp x)) (exp y)) = ((real_lt x) y))))
T c= creal_lt cexp f0 cexp f1 creal_lt f0 f1
