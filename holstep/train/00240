N FACE_OF_INTER_SUBFACE
C |- (!c1. (!c2. (!d1. (!d2. ((((face_of (c1 INTER c2)) c1) /\ (((face_of (c1 INTER c2)) c2) /\ (((face_of d1) c1) /\ ((face_of d2) c2)))) ==> (((face_of (d1 INTER d2)) d1) /\ ((face_of (d1 INTER d2)) d2)))))))
T c==> c/\ cface_of * * part cINTER f0 f1 f0 c/\ cface_of * * part cINTER f0 f1 f1 c/\ cface_of f2 f0 cface_of f3 f1 c/\ cface_of * * part cINTER f2 f3 f2 cface_of * * part cINTER f2 f3 f3
D TRUTH
A |- T
T cT
D INTER_SUBSET
A |- ((!s. (!t. ((s INTER t) SUBSET s))) /\ (!s. (!t. ((t INTER s) SUBSET s))))
T c/\ ! ! cSUBSET * * part cINTER b0 b1 b0 ! ! cSUBSET * * part cINTER b1 b0 b0
D FACE_OF_TRANS
A |- (!s. (!t. (!u. ((((face_of s) t) /\ ((face_of t) u)) ==> ((face_of s) u)))))
T c==> c/\ cface_of f0 f1 cface_of f1 f2 cface_of f0 f2
D FACE_OF_SUBSET
A |- (!f. (!s. (!t. ((((face_of f) s) /\ ((f SUBSET t) /\ (t SUBSET s))) ==> ((face_of f) t)))))
T c==> c/\ cface_of f0 f1 c/\ cSUBSET f0 f2 cSUBSET f2 f1 cface_of f0 f2
D FACE_OF_INTER_INTER
A |- (!f. (!t. (!f'. (!t'. ((((face_of f) t) /\ ((face_of f') t')) ==> ((face_of (f INTER f')) (t INTER t')))))))
T c==> c/\ cface_of f0 f1 cface_of f2 f3 cface_of * * part cINTER f0 f2 * * part cINTER f1 f3
D FACE_OF_IMP_SUBSET
A |- (!s. (!t. (((face_of t) s) ==> (t SUBSET s))))
T c==> cface_of f0 f1 cSUBSET f0 f1
+ (((face_of d1) c1) /\ ((face_of d2) c2)), ((face_of d1) c1), ((face_of (c1 INTER c2)) c1), ((face_of (c1 INTER c2)) c2) |- (((face_of (d1 INTER d2)) d1) /\ ((face_of (d1 INTER d2)) d2))
T c==> c/\ cface_of f0 f1 cface_of f2 f3 c==> cface_of f0 f1 c==> cface_of * * part cINTER f1 f3 f1 c==> cface_of * * part cINTER f1 f3 f3 c/\ cface_of * * part cINTER f0 f2 f0 cface_of * * part cINTER f0 f2 f2
+ (!f. (!s. (!t. ((((face_of f) s) /\ ((f SUBSET t) /\ (t SUBSET s))) ==> ((face_of f) t))))) |- ((\t. ((((face_of f) s) /\ ((f SUBSET t) /\ (t SUBSET s))) ==> ((face_of f) t))) t)
T c==> ! ! ! c==> c/\ cface_of b0 b1 c/\ cSUBSET b0 b2 cSUBSET b2 b1 cface_of b0 b2 * / c==> c/\ cface_of f0 f1 c/\ cSUBSET f0 b0 cSUBSET b0 f1 cface_of f0 b0 f2
- ((face_of d2) c2) |- (T = ((face_of d2) c2))
T c==> cface_of f0 f1 c= cT cface_of f0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
