N GAUGE_INTER
C |- (!d1. (!d2. (((gauge d1) /\ (gauge d2)) ==> (gauge (\x. ((d1 x) INTER (d2 x)))))))
T c==> c/\ cgauge f0 cgauge f1 cgauge / * * part cINTER * f0 b0 * f1 b0
D TRUTH
A |- T
T cT
D IN_INTER
A |- (!s. (!t. (!x. ((x IN (s INTER t)) = ((x IN s) /\ (x IN t))))))
T c= cIN f0 * * part cINTER f1 f2 c/\ cIN f0 f1 cIN f0 f2
D OPEN_INTER
A |- (!s. (!t. (((open s) /\ (open t)) ==> (open (s INTER t)))))
T c==> c/\ copen f0 copen f1 copen * * part cINTER f0 f1
D gauge
A |- (!d. ((gauge d) = (!x. ((x IN (d x)) /\ (open (d x))))))
T c= cgauge f0 ! c/\ cIN b0 * f0 b0 copen * f0 b0
+ ((!x. ((x IN (d1 x)) /\ (open (d1 x)))) /\ (!x. ((x IN (d2 x)) /\ (open (d2 x))))) |- ((\x. ((x IN (d1 x)) /\ (open (d1 x)))) x)
T c==> c/\ ! c/\ cIN b0 * f0 b0 copen * f0 b0 ! c/\ cIN b0 * f1 b0 copen * f1 b0 * / c/\ cIN b0 * f0 b0 copen * f0 b0 f2
+ |- ((\s. (!t. (((open s) /\ (open t)) ==> (open (s INTER t))))) s)
T * / ! c==> c/\ copen b0 copen b1 copen * * part cINTER b0 b1 f0
+ |- (((!d. ((gauge d) = (!x. ((x IN (d x)) /\ (open (d x)))))) ==> ((\d. ((gauge d) = (!x. ((x IN (d x)) /\ (open (d x)))))) d)) = ((\d. ((gauge d) = (!x. ((x IN (d x)) /\ (open (d x)))))) d))
T c= c==> ! c= cgauge b0 ! c/\ cIN b1 * b0 b1 copen * b0 b1 * / c= cgauge b0 ! c/\ cIN b1 * b0 b1 copen * b0 b1 f0 * / c= cgauge b0 ! c/\ cIN b1 * b0 b1 copen * b0 b1 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- (((\x. (f x)) y) = (f y))
T c= * / * f0 b0 f1 * f0 f1
