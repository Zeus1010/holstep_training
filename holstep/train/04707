N Leaf_cell.CONNECTED_SEGMENT_NOT_COVERED
C |- (!A. (!B. (!a. (!b. (((open A) /\ ((open B) /\ ((a IN A) /\ ((b IN B) /\ ((A INTER B) = EMPTY))))) ==> (?x. ((x IN (closed_segment ((CONS (a , b)) NIL))) /\ ((~ (x IN A)) /\ (~ (x IN B))))))))))
