N CONVERGENT_EQ_CAUCHY
C |- (!s. ((?l. (((--> s) l) sequentially)) = (cauchy s)))
T c= c? / c--> f0 b0 csequentially ccauchy f0
D TRUTH
A |- T
T cT
D IN_UNIV
A |- (!x. (x IN UNIV))
T cIN f0 part cUNIV
D complete
A |- (!s. ((complete s) = (!f. (((!n. ((f n) IN s)) /\ (cauchy f)) ==> (?l. ((l IN s) /\ (((--> f) l) sequentially)))))))
T c= ccomplete f0 ! c==> c/\ ! cIN * b0 b1 f0 ccauchy b0 c? / c/\ cIN b1 f0 c--> b0 b1 csequentially
D CONVERGENT_IMP_CAUCHY
A |- (!s. (!l. ((((--> s) l) sequentially) ==> (cauchy s))))
T c==> c--> f0 f1 csequentially ccauchy f0
D COMPLETE_UNIV
A |- (complete UNIV)
T ccomplete part cUNIV
+ |- ((\l. ((((--> s) l) sequentially) ==> (cauchy s))) l)
T * / c==> c--> f0 b0 csequentially ccauchy f0 f1
+ |- ((\x. (x IN UNIV)) x)
T * / cIN b0 part cUNIV f0
+ |- ((cauchy s) ==> (?l. (((--> s) l) sequentially)))
T c==> ccauchy f0 c? / c--> f0 b0 csequentially
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((?l. t) = t)
T c= c? / f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
