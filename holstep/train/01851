N PAIRED_EXT
C |- (!l. (!m. ((!x. (!y. ((l (x , y)) = (m (x , y))))) = (l = m))))
T c= ! ! c= * f0 c, b0 b1 * f1 c, b0 b1 c= f0 f1
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D PAIR
A |- (!x. (((FST x) , (SND x)) = x))
T c= c, cFST f0 cSND f0 f0
