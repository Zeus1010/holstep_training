+ |- (((x IN (mspace m)) /\ ((x' IN (mspace m)) /\ ((real_lt ((mdist m) (x , x'))) r))) ==> ((x' IN ((mcball m) (x , r))) = T))
T c==> c/\ cIN f0 * part cmspace f1 c/\ cIN f2 * part cmspace f1 creal_lt cmdist f1 c, f0 f2 f3 c= cIN f2 * * part cmcball f1 c, f0 f3 cT
+ |- (((!q'. (((x' IN ((mball m) (x , r))) = p') ==> ((p' ==> ((x' IN ((mcball m) (x , r))) = q')) ==> (((x' IN ((mball m) (x , r))) ==> (x' IN ((mcball m) (x , r)))) = (p' ==> q'))))) ==> ((\q'. (((x' IN ((mball m) (x , r))) = p') ==> ((p' ==> ((x' IN ((mcball m) (x , r))) = q')) ==> (((x' IN ((mball m) (x , r))) ==> (x' IN ((mcball m) (x , r)))) = (p' ==> q'))))) q')) = ((\q'. (((x' IN ((mball m) (x , r))) = p') ==> ((p' ==> ((x' IN ((mcball m) (x , r))) = q')) ==> (((x' IN ((mball m) (x , r))) ==> (x' IN ((mcball m) (x , r)))) = (p' ==> q'))))) q'))
