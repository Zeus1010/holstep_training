N HOMOTOPIC_PATHS_LOOP_PARTS
C |- (!s. (!p. (!q. (!a. (((((homotopic_loops s) ((++ p) (reversepath q))) (linepath (a , a))) /\ (path q)) ==> (((homotopic_paths s) p) q))))))
T c==> c/\ chomotopic_loops f0 * * part c++ f1 * part creversepath f2 * part clinepath c, f3 f3 cpath f2 chomotopic_paths f0 f1 f2
D TRUTH
A |- T
T cT
D EXCLUDED_MIDDLE
A |- (!t. (t \/ (~ t)))
T c\/ f0 c~ f0
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D SING_SUBSET
A |- (!s. (!x. (((x INSERT EMPTY) SUBSET s) = (x IN s))))
T c= cSUBSET * * part cINSERT f0 part cEMPTY f1 cIN f0 f1
D UNION_SUBSET
A |- (!s. (!t. (!u. (((s UNION t) SUBSET u) = ((s SUBSET u) /\ (t SUBSET u))))))
T c= cSUBSET * * part cUNION f0 f1 f2 c/\ cSUBSET f0 f2 cSUBSET f1 f2
D SEGMENT_REFL
A |- ((!a. ((closed_segment ((CONS (a , a)) NIL)) = (a INSERT EMPTY))) /\ (!a. ((open_segment (a , a)) = EMPTY)))
T c/\ ! c= * part cclosed_segment cCONS c, b0 b0 cNIL * * part cINSERT b0 part cEMPTY ! c= * part copen_segment c, b0 b0 part cEMPTY
D PATHSTART_REVERSEPATH
A |- ((pathstart (reversepath g)) = (pathfinish g))
T c= cpathstart * part creversepath f0 cpathfinish f0
D PATHFINISH_REVERSEPATH
A |- ((pathfinish (reversepath g)) = (pathstart g))
T c= cpathfinish * part creversepath f0 cpathstart f0
D PATHSTART_JOIN
A |- (!g1. (!g2. ((pathstart ((++ g1) g2)) = (pathstart g1))))
T c= cpathstart * * part c++ f0 f1 cpathstart f0
D PATHFINISH_JOIN
A |- (!g1. (!g2. ((pathfinish ((++ g1) g2)) = (pathfinish g2))))
T c= cpathfinish * * part c++ f0 f1 cpathfinish f1
D PATH_IMAGE_REVERSEPATH
A |- (!g. ((path_image (reversepath g)) = (path_image g)))
T c= * part cpath_image * part creversepath f0 * part cpath_image f0
D PATH_REVERSEPATH
A |- (!g. ((path (reversepath g)) = (path g)))
T c= cpath * part creversepath f0 cpath f0
D PATH_JOIN
A |- (!g1. (!g2. (((pathfinish g1) = (pathstart g2)) ==> ((path ((++ g1) g2)) = ((path g1) /\ (path g2))))))
T c==> c= cpathfinish f0 cpathstart f1 c= cpath * * part c++ f0 f1 c/\ cpath f0 cpath f1
D PATH_IMAGE_JOIN
A |- (!g1. (!g2. (((pathfinish g1) = (pathstart g2)) ==> ((path_image ((++ g1) g2)) = ((path_image g1) UNION (path_image g2))))))
T c==> c= cpathfinish f0 cpathstart f1 c= * part cpath_image * * part c++ f0 f1 * * part cUNION * part cpath_image f0 * part cpath_image f1
D PATH_JOIN_PATH_ENDS
A |- (!g1. (!g2. (((path g2) /\ (path ((++ g1) g2))) ==> ((pathfinish g1) = (pathstart g2)))))
T c==> c/\ cpath f0 cpath * * part c++ f1 f0 c= cpathfinish f1 cpathstart f0
D PATHFINISH_LINEPATH
A |- (!a. (!b. ((pathfinish (linepath (a , b))) = b)))
T c= cpathfinish * part clinepath c, f0 f1 f1
D PATH_IMAGE_LINEPATH
A |- (!a. (!b. ((path_image (linepath (a , b))) = (closed_segment ((CONS (a , b)) NIL)))))
T c= * part cpath_image * part clinepath c, f0 f1 * part cclosed_segment cCONS c, f0 f1 cNIL
D HOMOTOPIC_PATHS_IMP_PATHFINISH
A |- (!s. (!p. (!q. ((((homotopic_paths s) p) q) ==> ((pathfinish p) = (pathfinish q))))))
T c==> chomotopic_paths f0 f1 f2 c= cpathfinish f1 cpathfinish f2
D HOMOTOPIC_PATHS_IMP_PATH
A |- (!s. (!p. (!q. ((((homotopic_paths s) p) q) ==> ((path p) /\ (path q))))))
T c==> chomotopic_paths f0 f1 f2 c/\ cpath f1 cpath f2
D HOMOTOPIC_PATHS_IMP_SUBSET
A |- (!s. (!p. (!q. ((((homotopic_paths s) p) q) ==> (((path_image p) SUBSET s) /\ ((path_image q) SUBSET s))))))
