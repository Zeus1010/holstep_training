N LEBESGUE_MEASURABLE_UNION
C |- (!s. (!t. (((lebesgue_measurable s) /\ (lebesgue_measurable t)) ==> (lebesgue_measurable (s UNION t)))))
T c==> c/\ clebesgue_measurable f0 clebesgue_measurable f1 clebesgue_measurable * * part cUNION f0 f1
D TRUTH
A |- T
T cT
D indicator
A |- (!s. ((indicator s) = (\x. (((COND (x IN s)) (vec (NUMERAL (BIT1 _0)))) (vec (NUMERAL _0))))))
T c= * part cindicator f0 / cCOND cIN b0 f0 cvec cNUMERAL cBIT1 c_0 cvec cNUMERAL c_0
D MEASURABLE_ON_UNIV
A |- (((measurable_on (\x. (((COND (x IN s)) (f x)) (vec (NUMERAL _0))))) UNIV) = ((measurable_on f) s))
T c= cmeasurable_on / cCOND cIN b0 f0 * f1 b0 cvec cNUMERAL c_0 part cUNIV cmeasurable_on f1 f0
D lebesgue_measurable
A |- (!s. ((lebesgue_measurable s) = ((measurable_on (indicator s)) UNIV)))
T c= clebesgue_measurable f0 cmeasurable_on * part cindicator f0 part cUNIV
D MEASURABLE_ON_UNION
A |- (!f. (!s. (!t. ((((measurable_on f) s) /\ ((measurable_on f) t)) ==> ((measurable_on f) (s UNION t))))))
T c==> c/\ cmeasurable_on f0 f1 cmeasurable_on f0 f2 cmeasurable_on f0 * * part cUNION f1 f2
+ |- ((\s. ((indicator s) = (\x. (((COND (x IN s)) (vec (NUMERAL (BIT1 _0)))) (vec (NUMERAL _0)))))) s)
T * / c= * part cindicator b0 / cCOND cIN b1 b0 cvec cNUMERAL cBIT1 c_0 cvec cNUMERAL c_0 f0
+ |- (((!t. ((((measurable_on f) s) /\ ((measurable_on f) t)) ==> ((measurable_on f) (s UNION t)))) ==> ((\t. ((((measurable_on f) s) /\ ((measurable_on f) t)) ==> ((measurable_on f) (s UNION t)))) t)) = ((\t. ((((measurable_on f) s) /\ ((measurable_on f) t)) ==> ((measurable_on f) (s UNION t)))) t))
T c= c==> ! c==> c/\ cmeasurable_on f0 f1 cmeasurable_on f0 b0 cmeasurable_on f0 * * part cUNION f1 b0 * / c==> c/\ cmeasurable_on f0 f1 cmeasurable_on f0 b0 cmeasurable_on f0 * * part cUNION f1 b0 f2 * / c==> c/\ cmeasurable_on f0 f1 cmeasurable_on f0 b0 cmeasurable_on f0 * * part cUNION f1 b0 f2
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
