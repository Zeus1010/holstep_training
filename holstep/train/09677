N EXTREME_POINT_OF_EMPTY
C |- (!x. (~ ((extreme_point_of x) EMPTY)))
T c~ cextreme_point_of f0 part cEMPTY
D TRUTH
A |- T
T cT
D NOT_IN_EMPTY
A |- (!x. (~ (x IN EMPTY)))
T c~ cIN f0 part cEMPTY
D extreme_point_of
A |- (!s. (!x. (((extreme_point_of x) s) = ((x IN s) /\ (!a. (!b. (((a IN s) /\ (b IN s)) ==> (~ (x IN (open_segment (a , b)))))))))))
T c= cextreme_point_of f0 f1 c/\ cIN f0 f1 ! ! c==> c/\ cIN b0 f1 cIN b1 f1 c~ cIN f0 * part copen_segment c, b0 b1
+ |- (T = (!x. (~ ((extreme_point_of x) EMPTY))))
T c= cT ! c~ cextreme_point_of b0 part cEMPTY
+ |- ((\s. (!x. (((extreme_point_of x) s) = ((x IN s) /\ (!a. (!b. (((a IN s) /\ (b IN s)) ==> (~ (x IN (open_segment (a , b))))))))))) s)
T * / ! c= cextreme_point_of b1 b0 c/\ cIN b1 b0 ! ! c==> c/\ cIN b2 b0 cIN b3 b0 c~ cIN b1 * part copen_segment c, b2 b3 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
