N CONNECTED_COMPONENT_OVERLAP
C |- (!s. (!a. (!b. ((~ ((((connected_component s) a) INTER ((connected_component s) b)) = EMPTY)) = ((a IN s) /\ ((b IN s) /\ (((connected_component s) a) = ((connected_component s) b))))))))
T c= c~ c= * * part cINTER * * part cconnected_component f0 f1 * * part cconnected_component f0 f2 part cEMPTY c/\ cIN f1 f0 c/\ cIN f2 f0 c= * * part cconnected_component f0 f1 * * part cconnected_component f0 f2
D TRUTH
A |- T
T cT
D DE_MORGAN_THM
A |- (!t1. (!t2. (((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2))) /\ ((~ (t1 \/ t2)) = ((~ t1) /\ (~ t2))))))
T c/\ c= c~ c/\ f0 f1 c\/ c~ f0 c~ f1 c= c~ c\/ f0 f1 c/\ c~ f0 c~ f1
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D CONNECTED_COMPONENT_NONOVERLAP
A |- (!s. (!a. (!b. (((((connected_component s) a) INTER ((connected_component s) b)) = EMPTY) = ((~ (a IN s)) \/ ((~ (b IN s)) \/ (~ (((connected_component s) a) = ((connected_component s) b)))))))))
T c= c= * * part cINTER * * part cconnected_component f0 f1 * * part cconnected_component f0 f2 part cEMPTY c\/ c~ cIN f1 f0 c\/ c~ cIN f2 f0 c~ c= * * part cconnected_component f0 f1 * * part cconnected_component f0 f2
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
+ |- ((!b. t) = t)
T c= ! f0 f0
+ |- ((\b. (((((connected_component s) a) INTER ((connected_component s) b)) = EMPTY) = ((~ (a IN s)) \/ ((~ (b IN s)) \/ (~ (((connected_component s) a) = ((connected_component s) b))))))) b)
T * / c= c= * * part cINTER * * part cconnected_component f0 f1 * * part cconnected_component f0 b0 part cEMPTY c\/ c~ cIN f1 f0 c\/ c~ cIN b0 f0 c~ c= * * part cconnected_component f0 f1 * * part cconnected_component f0 b0 f2
+ |- (((!t2. (((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2))) /\ ((~ (t1 \/ t2)) = ((~ t1) /\ (~ t2))))) ==> ((\t2. (((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2))) /\ ((~ (t1 \/ t2)) = ((~ t1) /\ (~ t2))))) t2)) = ((\t2. (((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2))) /\ ((~ (t1 \/ t2)) = ((~ t1) /\ (~ t2))))) t2))
T c= c==> ! c/\ c= c~ c/\ f0 b0 c\/ c~ f0 c~ b0 c= c~ c\/ f0 b0 c/\ c~ f0 c~ b0 * / c/\ c= c~ c/\ f0 b0 c\/ c~ f0 c~ b0 c= c~ c\/ f0 b0 c/\ c~ f0 c~ b0 f1 * / c/\ c= c~ c/\ f0 b0 c\/ c~ f0 c~ b0 c= c~ c\/ f0 b0 c/\ c~ f0 c~ b0 f1
- |- ((~ (t1 /\ t2)) = ((~ t1) \/ (~ t2)))
T c= c~ c/\ f0 f1 c\/ c~ f0 c~ f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
