N NEGLIGIBLE_DIFF
C |- (!s. (!t. ((negligible s) ==> (negligible (s DIFF t)))))
T c==> cnegligible f0 cnegligible * * part cDIFF f0 f1
D TRUTH
A |- T
T cT
D SUBSET_DIFF
A |- (!s. (!t. ((s DIFF t) SUBSET s)))
T cSUBSET * * part cDIFF f0 f1 f0
D NEGLIGIBLE_SUBSET
A |- (!s. (!t. (((negligible s) /\ (t SUBSET s)) ==> (negligible t))))
T c==> c/\ cnegligible f0 cSUBSET f1 f0 cnegligible f1
+ (negligible s) |- (T = ((negligible s) /\ ((s DIFF t) SUBSET s)))
T c==> cnegligible f0 c= cT c/\ cnegligible f0 cSUBSET * * part cDIFF f0 f1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
