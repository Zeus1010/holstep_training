N EXTREME_POINT_OF_FACE
C |- (!f. (!s. (!v. (((face_of f) s) ==> (((extreme_point_of v) f) = (((extreme_point_of v) s) /\ (v IN f)))))))
T c==> cface_of f0 f1 c= cextreme_point_of f2 f0 c/\ cextreme_point_of f2 f1 cIN f2 f0
D TRUTH
A |- T
T cT
D SING_SUBSET
A |- (!s. (!x. (((x INSERT EMPTY) SUBSET s) = (x IN s))))
T c= cSUBSET * * part cINSERT f0 part cEMPTY f1 cIN f0 f1
D FACE_OF_FACE
A |- (!f. (!s. (!t. (((face_of t) s) ==> (((face_of f) t) = (((face_of f) s) /\ (f SUBSET t)))))))
T c==> cface_of f0 f1 c= cface_of f2 f0 c/\ cface_of f2 f1 cSUBSET f2 f0
D FACE_OF_SING
A |- (!x. (!s. (((face_of (x INSERT EMPTY)) s) = ((extreme_point_of x) s))))
T c= cface_of * * part cINSERT f0 part cEMPTY f1 cextreme_point_of f0 f1
+ |- (((!x. (!s. (((extreme_point_of x) s) = ((face_of (x INSERT EMPTY)) s)))) ==> ((\x. (!s. (((extreme_point_of x) s) = ((face_of (x INSERT EMPTY)) s)))) x)) = ((\x. (!s. (((extreme_point_of x) s) = ((face_of (x INSERT EMPTY)) s)))) x))
T c= c==> ! ! c= cextreme_point_of b0 b1 cface_of * * part cINSERT b0 part cEMPTY b1 * / ! c= cextreme_point_of b0 b1 cface_of * * part cINSERT b0 part cEMPTY b1 f0 * / ! c= cextreme_point_of b0 b1 cface_of * * part cINSERT b0 part cEMPTY b1 f0
+ |- ((\s. (!x. ((x IN s) = ((x INSERT EMPTY) SUBSET s)))) s)
T * / ! c= cIN b1 b0 cSUBSET * * part cINSERT b1 part cEMPTY b0 f0
+ |- ((((face_of t) s) ==> (((face_of f) t) = (((face_of f) s) /\ (f SUBSET t)))) = T)
T c= c==> cface_of f0 f1 c= cface_of f2 f0 c/\ cface_of f2 f1 cSUBSET f2 f0 cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((x = x) = T)
T c= c= f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
