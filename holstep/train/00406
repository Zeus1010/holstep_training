N IS_REALINTERVAL_CONNECTED
C |- (!s. ((is_realinterval s) = (connected ((IMAGE lift) s))))
T c= cis_realinterval f0 cconnected * * part cIMAGE part clift f0
D TRUTH
A |- T
T cT
D IS_INTERVAL_CONNECTED_1
A |- (!s. ((is_interval s) = (connected s)))
T c= cis_interval f0 cconnected f0
D IS_REALINTERVAL_IS_INTERVAL
A |- (!s. ((is_realinterval s) = (is_interval ((IMAGE lift) s))))
T c= cis_realinterval f0 cis_interval * * part cIMAGE part clift f0
