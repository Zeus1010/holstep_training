N CLOSED_DIFF
C |- (!s. (!t. (((closed s) /\ (open t)) ==> (closed (s DIFF t)))))
T c==> c/\ cclosed f0 copen f1 cclosed * * part cDIFF f0 f1
D TRUTH
A |- T
T cT
D CLOSED_IN_DIFF
A |- (!top. (!s. (!t. ((((closed_in top) s) /\ ((open_in top) t)) ==> ((closed_in top) (s DIFF t))))))
T c==> c/\ cclosed_in f0 f1 copen_in f0 f2 cclosed_in f0 * * part cDIFF f1 f2
D OPEN_IN
A |- (!s. ((open s) = ((open_in euclidean) s)))
T c= copen f0 copen_in ceuclidean f0
D CLOSED_IN
A |- (!s. ((closed s) = ((closed_in euclidean) s)))
T c= cclosed f0 cclosed_in ceuclidean f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((!t. t) = t)
T c= ! f0 f0
+ |- (((!top. (!s. (!t. ((((closed_in top) s) /\ ((open_in top) t)) ==> ((closed_in top) (s DIFF t)))))) ==> ((\top. (!s. (!t. ((((closed_in top) s) /\ ((open_in top) t)) ==> ((closed_in top) (s DIFF t)))))) top)) = ((\top. (!s. (!t. ((((closed_in top) s) /\ ((open_in top) t)) ==> ((closed_in top) (s DIFF t)))))) top))
T c= c==> ! ! ! c==> c/\ cclosed_in b0 b1 copen_in b0 b2 cclosed_in b0 * * part cDIFF b1 b2 * / ! ! c==> c/\ cclosed_in b0 b1 copen_in b0 b2 cclosed_in b0 * * part cDIFF b1 b2 f0 * / ! ! c==> c/\ cclosed_in b0 b1 copen_in b0 b2 cclosed_in b0 * * part cDIFF b1 b2 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
