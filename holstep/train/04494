+ |- (((!a0'. (!a1'. ((((CONS a0) a1) = ((CONS a0') a1')) = ((a0 = a0') /\ (a1 = a1'))))) ==> ((\a0'. (!a1'. ((((CONS a0) a1) = ((CONS a0') a1')) = ((a0 = a0') /\ (a1 = a1'))))) a0')) = ((\a0'. (!a1'. ((((CONS a0) a1) = ((CONS a0') a1')) = ((a0 = a0') /\ (a1 = a1'))))) a0'))
