N HAS_MEASURE_ELEMENTARY
C |- (!d. (!s. (((division_of d) s) ==> ((has_measure s) ((sum d) content)))))
T c==> cdivision_of f0 f1 chas_measure f1 csum f0 part ccontent
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D EXCLUDED_MIDDLE
A |- (!t. (t \/ (~ t)))
T c\/ f0 c~ f0
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D NOT_CLAUSES
A |- ((!t. ((~ (~ t)) = t)) /\ (((~ T) = F) /\ ((~ F) = T)))
T c/\ ! c= c~ c~ b0 b0 c/\ c= c~ cT cF c= c~ cF cT
D NOT_EXISTS_THM
A |- (!P. ((~ (?x. (P x))) = (!x. (~ (P x)))))
T c= c~ c? / * f0 b0 ! c~ * f0 b0
D NOT_FORALL_THM
A |- (!P. ((~ (!x. (P x))) = (?x. (~ (P x)))))
T c= c~ ! * f0 b0 c? / c~ * f0 b0
D RIGHT_OR_FORALL_THM
A |- (!P. (!Q. ((P \/ (!x. (Q x))) = (!x. (P \/ (Q x))))))
T c= c\/ f0 ! * f1 b0 ! c\/ f0 * f1 b0
D SKOLEM_THM
A |- (!P. ((!x. (?y. ((P x) y))) = (?y. (!x. ((P x) (y x))))))
T c= ! c? / * * f0 b0 b1 c? / ! * * f0 b1 * b0 b1
D o_THM
A |- (!f. (!g. (!x. (((f o g) x) = (f (g x))))))
T c= co f0 f1 f2 * f0 * f1 f2
D LIFT_SUM
A |- (!k. (!x. ((lift ((sum k) x)) = ((vsum k) (lift o x)))))
T c= clift csum f0 f1 cvsum f0 * * part co part clift f1
D division_of
A |- (!s. (!i. (((division_of s) i) = ((FINITE s) /\ ((!k. ((k IN s) ==> ((k SUBSET i) /\ ((~ (k = EMPTY)) /\ (?a. (?b. (k = (closed_interval ((CONS (a , b)) NIL))))))))) /\ ((!k1. (!k2. (((k1 IN s) /\ ((k2 IN s) /\ (~ (k1 = k2)))) ==> (((interior k1) INTER (interior k2)) = EMPTY)))) /\ ((UNIONS s) = i)))))))
