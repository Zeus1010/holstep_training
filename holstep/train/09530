N OPEN_IMP_GDELTA
C |- (!s. ((open s) ==> (gdelta s)))
T c==> copen f0 cgdelta f0
D TRUTH
A |- T
T cT
D NOT_IN_EMPTY
A |- (!x. (~ (x IN EMPTY)))
T c~ cIN f0 part cEMPTY
D FORALL_IN_INSERT
A |- (!P. (!a. (!s. ((!x. ((x IN (a INSERT s)) ==> (P x))) = ((P a) /\ (!x. ((x IN s) ==> (P x))))))))
T c= ! c==> cIN b0 * * part cINSERT f0 f1 * f2 b0 c/\ * f2 f0 ! c==> cIN b0 f1 * f2 b0
D INTERS_1
A |- ((INTERS (s INSERT EMPTY)) = s)
T c= * part cINTERS * * part cINSERT f0 part cEMPTY f0
D COUNTABLE_SING
A |- (!x. (COUNTABLE (x INSERT EMPTY)))
T cCOUNTABLE * * part cINSERT f0 part cEMPTY
D gdelta
A |- (!s. ((gdelta s) = (?g. ((COUNTABLE g) /\ ((!u. ((u IN g) ==> (open u))) /\ ((INTERS g) = s))))))
T c= cgdelta f0 c? / c/\ cCOUNTABLE b0 c/\ ! c==> cIN b1 b0 copen b1 c= * part cINTERS b0 f0
+ |- ((t /\ T) = t)
T c= c/\ f0 cT f0
+ |- (T = (!u. ((u IN EMPTY) ==> (open u))))
T c= cT ! c==> cIN b0 part cEMPTY copen b0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- (((!s. ((gdelta s) = (?g. ((COUNTABLE g) /\ ((!u. ((u IN g) ==> (open u))) /\ ((INTERS g) = s)))))) ==> ((\s. ((gdelta s) = (?g. ((COUNTABLE g) /\ ((!u. ((u IN g) ==> (open u))) /\ ((INTERS g) = s)))))) s)) = ((\s. ((gdelta s) = (?g. ((COUNTABLE g) /\ ((!u. ((u IN g) ==> (open u))) /\ ((INTERS g) = s)))))) s))
T c= c==> ! c= cgdelta b0 c? / c/\ cCOUNTABLE b1 c/\ ! c==> cIN b2 b1 copen b2 c= * part cINTERS b1 b0 * / c= cgdelta b0 c? / c/\ cCOUNTABLE b1 c/\ ! c==> cIN b2 b1 copen b2 c= * part cINTERS b1 b0 f0 * / c= cgdelta b0 c? / c/\ cCOUNTABLE b1 c/\ ! c==> cIN b2 b1 copen b2 c= * part cINTERS b1 b0 f0
- |- ((?g. t) = t)
T c= c? / f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
