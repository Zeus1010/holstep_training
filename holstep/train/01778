N CONTINUOUS_ON_CX_LIFT
C |- (!f. (!s. (((continuous_on (\x. (Cx (f x)))) s) = ((continuous_on (\x. (lift (f x)))) s))))
T c= ccontinuous_on / cCx * f0 b0 f1 ccontinuous_on / clift * f0 b0 f1
D TRUTH
A |- T
T cT
D CONTINUOUS_ON_EQ_CONTINUOUS_WITHIN
A |- (!f. (!s. (((continuous_on f) s) = (!x. ((x IN s) ==> ((continuous f) ((within (at x)) s)))))))
T c= ccontinuous_on f0 f1 ! c==> cIN b0 f1 ccontinuous f0 cwithin cat b0 f1
D CONTINUOUS_CX_LIFT
A |- (!f. (((continuous (\x. (Cx (f x)))) net) = ((continuous (\x. (lift (f x)))) net)))
T c= ccontinuous / cCx * f0 b0 f1 ccontinuous / clift * f0 b0 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- (((!f. (((continuous (\x. (Cx (f x)))) net) = ((continuous (\x. (lift (f x)))) net))) ==> ((\f. (((continuous (\x. (Cx (f x)))) net) = ((continuous (\x. (lift (f x)))) net))) f)) = ((\f. (((continuous (\x. (Cx (f x)))) net) = ((continuous (\x. (lift (f x)))) net))) f))
T c= c==> ! c= ccontinuous / cCx * b0 b1 f0 ccontinuous / clift * b0 b1 f0 * / c= ccontinuous / cCx * b0 b1 f0 ccontinuous / clift * b0 b1 f0 f1 * / c= ccontinuous / cCx * b0 b1 f0 ccontinuous / clift * b0 b1 f0 f1
