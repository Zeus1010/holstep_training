T c= creal_neg f0 creal_mul creal_neg creal_of_num cNUMERAL cBIT1 c_0 f0
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D IN_DIFF
A |- (!s. (!t. (!x. ((x IN (s DIFF t)) = ((x IN s) /\ (~ (x IN t)))))))
T c= cIN f0 * * part cDIFF f1 f2 c/\ cIN f0 f1 c~ cIN f0 f2
D SUBSET_REFL
A |- (!s. (s SUBSET s))
T cSUBSET f0 f0
D VECTOR_SUB_RZERO
A |- (((vector_sub x) (vec (NUMERAL _0))) = x)
T c= cvector_sub f0 cvec cNUMERAL c_0 f0
D dist
A |- (!x. (!y. ((distance (x , y)) = (vector_norm ((vector_sub x) y)))))
T c= cdistance c, f0 f1 cvector_norm cvector_sub f0 f1
D LIFT_DROP
A |- ((!x. ((lift (drop x)) = x)) /\ (!x. ((drop (lift x)) = x)))
T c/\ ! c= clift cdrop b0 b0 ! c= cdrop clift b0 b0
D DROP_EQ
A |- (!x. (!y. (((drop x) = (drop y)) = (x = y))))
T c= c= cdrop f0 cdrop f1 c= f0 f1
D LIFT_NUM
A |- (!n. ((lift (real_of_num n)) = (vec n)))
T c= clift creal_of_num f0 cvec f0
D LIFT_SUB
A |- (!x. (!y. ((lift ((real_sub x) y)) = ((vector_sub (lift x)) (lift y)))))
T c= clift creal_sub f0 f1 cvector_sub clift f0 clift f1
D NORM_LIFT
A |- (!x. ((vector_norm (lift x)) = (real_abs x)))
T c= cvector_norm clift f0 creal_abs f0
D SEGMENT_SYM
A |- ((!a. (!b. ((closed_segment ((CONS (a , b)) NIL)) = (closed_segment ((CONS (b , a)) NIL))))) /\ (!a. (!b. ((open_segment (a , b)) = (open_segment (b , a))))))
