N RC_INV
C |- ((RC (INV R)) = (INV (RC R)))
T c= * part cRC * part cINV f0 * part cINV * part cRC f0
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D RC_EXPLICIT
A |- (!R. (!x. (!y. ((((RC R) x) y) = (((R x) y) \/ (x = y))))))
T c= cRC f0 f1 f2 c\/ * * f0 f1 f2 c= f1 f2
D INV
A |- (!R. (!y. (!x. ((((INV R) x) y) = ((R y) x)))))
T c= cINV f0 f1 f2 * * f0 f2 f1
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
+ |- ((!x'. t) = t)
T c= ! f0 f0
