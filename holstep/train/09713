N TREAL_EQ_IMP_LE
C |- (!x. (!y. ((x treal_eq y) ==> (x treal_le y))))
T c==> ctreal_eq f0 f1 ctreal_le f0 f1
D TRUTH
A |- T
T cT
D FORALL_PAIR_THM
A |- (!P. ((!p. (P p)) = (!p1. (!p2. (P (p1 , p2))))))
T c= ! * f0 b0 ! ! * f0 c, b0 b1
D HREAL_LE_REFL
A |- (!x. ((hreal_le x) x))
T chreal_le f0 f0
D treal_le
A |- (!x1. (!y2. (!x2. (!y1. (((x1 , y1) treal_le (x2 , y2)) = ((hreal_le ((hreal_add x1) y2)) ((hreal_add x2) y1)))))))
T c= ctreal_le c, f0 f1 c, f2 f3 chreal_le chreal_add f0 f3 chreal_add f2 f1
D treal_eq
A |- (!x1. (!y2. (!x2. (!y1. (((x1 , y1) treal_eq (x2 , y2)) = (((hreal_add x1) y2) = ((hreal_add x2) y1)))))))
T c= ctreal_eq c, f0 f1 c, f2 f3 c= chreal_add f0 f3 chreal_add f2 f1
+ |- ((\y2. (!x2. (!y1. (((x1 , y1) treal_eq (x2 , y2)) = (((hreal_add x1) y2) = ((hreal_add x2) y1)))))) y2)
T * / ! ! c= ctreal_eq c, f0 b2 c, b1 b0 c= chreal_add f0 b0 chreal_add b1 b2 f1
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
+ |- ((\y2. (!x2. (!y1. (((x1 , y1) treal_le (x2 , y2)) = ((hreal_le ((hreal_add x1) y2)) ((hreal_add x2) y1)))))) y2)
T * / ! ! c= ctreal_le c, f0 b2 c, b1 b0 chreal_le chreal_add f0 b0 chreal_add b1 b2 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- (((x = x) ==> p) = p)
T c= c==> c= f0 f0 f1 f1
- |- ((x = x) = T)
T c= c= f0 f0 cT
