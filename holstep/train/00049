N CONTINUOUS_ON
C |- (!f. (!s. (((continuous_on f) s) = (!x. ((x IN s) ==> (((--> f) (f x)) ((within (at x)) s)))))))
T c= ccontinuous_on f0 f1 ! c==> cIN b0 f1 c--> f0 * f0 b0 cwithin cat b0 f1
D TRUTH
A |- T
T cT
D CONTINUOUS_WITHIN
A |- (!f. (!x. (((continuous f) ((within (at x)) s)) = (((--> f) (f x)) ((within (at x)) s)))))
T c= ccontinuous f0 cwithin cat f1 f2 c--> f0 * f0 f1 cwithin cat f1 f2
D CONTINUOUS_ON_EQ_CONTINUOUS_WITHIN
A |- (!f. (!s. (((continuous_on f) s) = (!x. ((x IN s) ==> ((continuous f) ((within (at x)) s)))))))
T c= ccontinuous_on f0 f1 ! c==> cIN b0 f1 ccontinuous f0 cwithin cat b0 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- (T = (!f. (!s. (((continuous_on f) s) = (!x. ((x IN s) ==> (((--> f) (f x)) ((within (at x)) s))))))))
T c= cT ! ! c= ccontinuous_on b0 b1 ! c==> cIN b2 b1 c--> b0 * b0 b2 cwithin cat b2 b1
