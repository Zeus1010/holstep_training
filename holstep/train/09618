N MEASURE_EMPTY
C |- ((measure EMPTY) = (real_of_num (NUMERAL _0)))
T c= cmeasure part cEMPTY creal_of_num cNUMERAL c_0
D TRUTH
A |- T
T cT
D NEGLIGIBLE_EMPTY
A |- (negligible EMPTY)
T cnegligible part cEMPTY
D MEASURE_EQ_0
A |- (!s. ((negligible s) ==> ((measure s) = (real_of_num (NUMERAL _0)))))
T c==> cnegligible f0 c= cmeasure f0 creal_of_num cNUMERAL c_0
+ |- (T = (negligible EMPTY))
T c= cT cnegligible part cEMPTY
- |- ((x = x) = T)
T c= c= f0 f0 cT
+ |- (((!s. ((negligible s) ==> ((measure s) = (real_of_num (NUMERAL _0))))) ==> ((\s. ((negligible s) ==> ((measure s) = (real_of_num (NUMERAL _0))))) s)) = ((\s. ((negligible s) ==> ((measure s) = (real_of_num (NUMERAL _0))))) s))
T c= c==> ! c==> cnegligible b0 c= cmeasure b0 creal_of_num cNUMERAL c_0 * / c==> cnegligible b0 c= cmeasure b0 creal_of_num cNUMERAL c_0 f0 * / c==> cnegligible b0 c= cmeasure b0 creal_of_num cNUMERAL c_0 f0
- (negligible s) |- ((negligible s) /\ ((measure s) = (real_of_num (NUMERAL _0))))
T c==> cnegligible f0 c/\ cnegligible f0 c= cmeasure f0 creal_of_num cNUMERAL c_0
