N REAL_EXP
C |- (!z. ((real z) ==> (real (cexp z))))
T c==> creal f0 creal ccexp f0
D TRUTH
A |- T
T cT
D REAL_CNJ
A |- (!z. ((real z) = ((cnj z) = z)))
T c= creal f0 c= ccnj f0 f0
D CNJ_CEXP
A |- (!z. ((cnj (cexp z)) = (cexp (cnj z))))
T c= ccnj ccexp f0 ccexp ccnj f0
+ |- ((\z. ((cnj (cexp z)) = (cexp (cnj z)))) z)
T * / c= ccnj ccexp b0 ccexp ccnj b0 f0
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- (((x = x) ==> p) = p)
T c= c==> c= f0 f0 f1 f1
