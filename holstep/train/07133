N HAS_SIZE_PRODUCT
C |- (!s. (!m. (!t. (!n. (((s HAS_SIZE m) /\ (t HAS_SIZE n)) ==> ((GSPEC (\GEN%PVAR%94. (?x. (?y. (((SETSPEC GEN%PVAR%94) ((x IN s) /\ (y IN t))) (x , y)))))) HAS_SIZE (m * n)))))))
T c==> c/\ cHAS_SIZE f0 f1 cHAS_SIZE f2 f3 cHAS_SIZE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f2 c, b1 b2 c* f1 f3
D TRUTH
A |- T
T cT
D HAS_SIZE
A |- (!s. (!n. ((s HAS_SIZE n) = ((FINITE s) /\ ((CARD s) = n)))))
T c= cHAS_SIZE f0 f1 c/\ cFINITE f0 c= cCARD f0 f1
D FINITE_PRODUCT
A |- (!s. (!t. (((FINITE s) /\ (FINITE t)) ==> (FINITE (GSPEC (\GEN%PVAR%92. (?x. (?y. (((SETSPEC GEN%PVAR%92) ((x IN s) /\ (y IN t))) (x , y))))))))))
T c==> c/\ cFINITE f0 cFINITE f1 cFINITE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f1 c, b1 b2
D CARD_PRODUCT
A |- (!s. (!t. (((FINITE s) /\ (FINITE t)) ==> ((CARD (GSPEC (\GEN%PVAR%93. (?x. (?y. (((SETSPEC GEN%PVAR%93) ((x IN s) /\ (y IN t))) (x , y))))))) = ((CARD s) * (CARD t))))))
T c==> c/\ cFINITE f0 cFINITE f1 c= cCARD * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f1 c, b1 b2 c* cCARD f0 cCARD f1
+ (((FINITE s) /\ ((CARD s) = m)) /\ ((FINITE t) /\ ((CARD t) = n))) |- (FINITE t)
T c==> c/\ c/\ cFINITE f0 c= cCARD f0 f1 c/\ cFINITE f2 c= cCARD f2 f3 cFINITE f2
+ |- ((\t. (((FINITE s) /\ (FINITE t)) ==> ((CARD (GSPEC (\GEN%PVAR%93. (?x. (?y. (((SETSPEC GEN%PVAR%93) ((x IN s) /\ (y IN t))) (x , y))))))) = ((CARD s) * (CARD t))))) t)
T * / c==> c/\ cFINITE f0 cFINITE b0 c= cCARD * part cGSPEC / c? / c? / cSETSPEC b1 c/\ cIN b2 f0 cIN b3 b0 c, b2 b3 c* cCARD f0 cCARD b0 f1
+ |- ((((s HAS_SIZE m) /\ (t HAS_SIZE n)) ==> ((GSPEC (\GEN%PVAR%94. (?x. (?y. (((SETSPEC GEN%PVAR%94) ((x IN s) /\ (y IN t))) (x , y)))))) HAS_SIZE (m * n))) = ((((FINITE s) /\ ((CARD s) = m)) /\ ((FINITE t) /\ ((CARD t) = n))) ==> T))
T c= c==> c/\ cHAS_SIZE f0 f1 cHAS_SIZE f2 f3 cHAS_SIZE * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f2 c, b1 b2 c* f1 f3 c==> c/\ c/\ cFINITE f0 c= cCARD f0 f1 c/\ cFINITE f2 c= cCARD f2 f3 cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- ((FINITE s) /\ (FINITE t)) |- (((FINITE s) /\ (FINITE t)) /\ ((CARD (GSPEC (\GEN%PVAR%93. (?x. (?y. (((SETSPEC GEN%PVAR%93) ((x IN s) /\ (y IN t))) (x , y))))))) = ((CARD s) * (CARD t))))
T c==> c/\ cFINITE f0 cFINITE f1 c/\ c/\ cFINITE f0 cFINITE f1 c= cCARD * part cGSPEC / c? / c? / cSETSPEC b0 c/\ cIN b1 f0 cIN b2 f1 c, b1 b2 c* cCARD f0 cCARD f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
