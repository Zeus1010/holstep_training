N REAL_COMPACT_EQ_BOUNDED_CLOSED
C |- (!s. ((real_compact s) = ((real_bounded s) /\ (real_closed s))))
T c= creal_compact f0 c/\ creal_bounded f0 creal_closed f0
D TRUTH
A |- T
T cT
D COMPACT_EQ_BOUNDED_CLOSED
A |- (!s. ((compact s) = ((bounded s) /\ (closed s))))
T c= ccompact f0 c/\ cbounded f0 cclosed f0
D REAL_CLOSED
A |- (!s. ((real_closed s) = (closed ((IMAGE lift) s))))
T c= creal_closed f0 cclosed * * part cIMAGE part clift f0
D REAL_BOUNDED
A |- ((real_bounded s) = (bounded ((IMAGE lift) s)))
T c= creal_bounded f0 cbounded * * part cIMAGE part clift f0
D real_compact
A |- (!s. ((real_compact s) = (compact ((IMAGE lift) s))))
T c= creal_compact f0 ccompact * * part cIMAGE part clift f0
+ |- ((!s. t) = t)
T c= ! f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
