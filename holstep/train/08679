N OPEN_IN_EUCLIDEAN
C |- ((open_in (mtopology euclidean_metric)) = open)
T c= * part copen_in cmtopology ceuclidean_metric part copen
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D SUBSET
A |- (!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t))))))
T c= cSUBSET f0 f1 ! c==> cIN b0 f0 cIN b0 f1
D IN_UNIV
A |- (!x. (x IN UNIV))
T cIN f0 part cUNIV
D DIST_SYM
A |- (!x. (!y. ((distance (x , y)) = (distance (y , x)))))
T c= cdistance c, f0 f1 cdistance c, f1 f0
D IN_MBALL
A |- (!m. (!x. (!y. (!r. ((y IN ((mball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_lt ((mdist m) (x , y))) r))))))))
T c= cIN f0 * * part cmball f1 c, f2 f3 c/\ cIN f2 * part cmspace f1 c/\ cIN f0 * part cmspace f1 creal_lt cmdist f1 c, f2 f0 f3
D OPEN_IN_MTOPOLOGY
A |- (!m. (!u. (((open_in (mtopology m)) u) = ((u SUBSET (mspace m)) /\ (!x. ((x IN u) ==> (?r. (((real_lt (real_of_num (NUMERAL _0))) r) /\ (((mball m) (x , r)) SUBSET u)))))))))
T c= copen_in cmtopology f0 f1 c/\ cSUBSET f1 * part cmspace f0 ! c==> cIN b0 f1 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b1 cSUBSET * * part cmball f0 c, b0 b1 f1
D open_def
A |- (!s. ((open s) = (!x. ((x IN s) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!x'. (((real_lt (distance (x' , x))) e) ==> (x' IN s)))))))))
T c= copen f0 ! c==> cIN b0 f0 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b1 ! c==> creal_lt cdistance c, b2 b0 b1 cIN b2 f0
D EUCLIDEAN_METRIC
A |- (((mdist euclidean_metric) = distance) /\ ((mspace euclidean_metric) = UNIV))
T c/\ c= * part cmdist ceuclidean_metric part cdistance c= * part cmspace ceuclidean_metric part cUNIV
+ |- ((\x. (x IN UNIV)) x)
T * / cIN b0 part cUNIV f0
+ |- (((!s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t)))))) ==> ((\s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t)))))) s)) = ((\s. (!t. ((s SUBSET t) = (!x. ((x IN s) ==> (x IN t)))))) s))
T c= c==> ! ! c= cSUBSET b0 b1 ! c==> cIN b2 b0 cIN b2 b1 * / ! c= cSUBSET b0 b1 ! c==> cIN b2 b0 cIN b2 b1 f0 * / ! c= cSUBSET b0 b1 ! c==> cIN b2 b0 cIN b2 b1 f0
+ |- ((\s. ((open s) = (!x. ((x IN s) ==> (?e. (((real_lt (real_of_num (NUMERAL _0))) e) /\ (!x'. (((real_lt (distance (x' , x))) e) ==> (x' IN s))))))))) s)
T * / c= copen b0 ! c==> cIN b1 b0 c? / c/\ creal_lt creal_of_num cNUMERAL c_0 b2 ! c==> creal_lt cdistance c, b3 b1 b2 cIN b3 b0 f0
+ |- (((!r. ((y IN ((mball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_lt ((mdist m) (x , y))) r))))) ==> ((\r. ((y IN ((mball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_lt ((mdist m) (x , y))) r))))) r)) = ((\r. ((y IN ((mball m) (x , r))) = ((x IN (mspace m)) /\ ((y IN (mspace m)) /\ ((real_lt ((mdist m) (x , y))) r))))) r))
T c= c==> ! c= cIN f0 * * part cmball f1 c, f2 b0 c/\ cIN f2 * part cmspace f1 c/\ cIN f0 * part cmspace f1 creal_lt cmdist f1 c, f2 f0 b0 * / c= cIN f0 * * part cmball f1 c, f2 b0 c/\ cIN f2 * part cmspace f1 c/\ cIN f0 * part cmspace f1 creal_lt cmdist f1 c, f2 f0 b0 f3 * / c= cIN f0 * * part cmball f1 c, f2 b0 c/\ cIN f2 * part cmspace f1 c/\ cIN f0 * part cmspace f1 creal_lt cmdist f1 c, f2 f0 b0 f3
+ |- ((t ==> T) = T)
T c= c==> f0 cT cT
+ |- ((\y. ((distance (x , y)) = (distance (y , x)))) y)
T * / c= cdistance c, f0 b0 cdistance c, b0 f0 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\x. (!y. ((distance (x , y)) = (distance (y , x))))) x)
T * / ! c= cdistance c, b0 b1 cdistance c, b1 b0 f0
- |- (s SUBSET UNIV)
T cSUBSET f0 part cUNIV
- |- ((?e. t) = t)
T c= c? / f0 f0
- |- (((!s. (s SUBSET UNIV)) ==> ((\s. (s SUBSET UNIV)) s)) = ((\s. (s SUBSET UNIV)) s))
T c= c==> ! cSUBSET b0 part cUNIV * / cSUBSET b0 part cUNIV f0 * / cSUBSET b0 part cUNIV f0
- |- ((s SUBSET UNIV) = T)
T c= cSUBSET f0 part cUNIV cT
- |- ((\s. (s SUBSET UNIV)) s)
T * / cSUBSET b0 part cUNIV f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
