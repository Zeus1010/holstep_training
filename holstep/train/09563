+ |- ((\s. ((((differentiable (lift o f)) ((within (at a)) s)) /\ ((differentiable g) ((within (at a)) s))) ==> ((differentiable (\x. ((% (f x)) (g x)))) ((within (at a)) s)))) s)
T * / c==> c/\ cdifferentiable * * part co part clift f0 cwithin cat f1 b0 cdifferentiable f2 cwithin cat f1 b0 cdifferentiable / c% * f0 b1 * f2 b1 cwithin cat f1 b0 f3
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- (((((differentiable (lift o f)) ((within (at a)) s)) /\ ((differentiable g) ((within (at a)) s))) ==> ((differentiable (\x. ((% (f x)) (g x)))) ((within (at a)) s))) = T)
T c= c==> c/\ cdifferentiable * * part co part clift f0 cwithin cat f1 f2 cdifferentiable f3 cwithin cat f1 f2 cdifferentiable / c% * f0 b0 * f3 b0 cwithin cat f1 f2 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
