N COMPACT_INTER
C |- (!s. (!t. (((compact s) /\ (compact t)) ==> (compact (s INTER t)))))
T c==> c/\ ccompact f0 ccompact f1 ccompact * * part cINTER f0 f1
D TRUTH
A |- T
T cT
D CLOSED_INTER
A |- (!s. (!t. (((closed s) /\ (closed t)) ==> (closed (s INTER t)))))
T c==> c/\ cclosed f0 cclosed f1 cclosed * * part cINTER f0 f1
D BOUNDED_INTER
A |- (!s. (!t. (((bounded s) \/ (bounded t)) ==> (bounded (s INTER t)))))
T c==> c\/ cbounded f0 cbounded f1 cbounded * * part cINTER f0 f1
D COMPACT_EQ_BOUNDED_CLOSED
A |- (!s. ((compact s) = ((bounded s) /\ (closed s))))
T c= ccompact f0 c/\ cbounded f0 cclosed f0
+ (((bounded s) /\ (closed s)) /\ ((bounded t) /\ (closed t))) |- ((closed s) = T)
T c==> c/\ c/\ cbounded f0 cclosed f0 c/\ cbounded f1 cclosed f1 c= cclosed f0 cT
+ |- (((!p'. (!q'. ((((compact s) /\ (compact t)) = p') ==> ((p' ==> ((compact (s INTER t)) = q')) ==> ((((compact s) /\ (compact t)) ==> (compact (s INTER t))) = (p' ==> q')))))) ==> ((\p'. (!q'. ((((compact s) /\ (compact t)) = p') ==> ((p' ==> ((compact (s INTER t)) = q')) ==> ((((compact s) /\ (compact t)) ==> (compact (s INTER t))) = (p' ==> q')))))) p')) = ((\p'. (!q'. ((((compact s) /\ (compact t)) = p') ==> ((p' ==> ((compact (s INTER t)) = q')) ==> ((((compact s) /\ (compact t)) ==> (compact (s INTER t))) = (p' ==> q')))))) p'))
T c= c==> ! ! c==> c= c/\ ccompact f0 ccompact f1 b0 c==> c==> b0 c= ccompact * * part cINTER f0 f1 b1 c= c==> c/\ ccompact f0 ccompact f1 ccompact * * part cINTER f0 f1 c==> b0 b1 * / ! c==> c= c/\ ccompact f0 ccompact f1 b0 c==> c==> b0 c= ccompact * * part cINTER f0 f1 b1 c= c==> c/\ ccompact f0 ccompact f1 ccompact * * part cINTER f0 f1 c==> b0 b1 f2 * / ! c==> c= c/\ ccompact f0 ccompact f1 b0 c==> c==> b0 c= ccompact * * part cINTER f0 f1 b1 c= c==> c/\ ccompact f0 ccompact f1 ccompact * * part cINTER f0 f1 c==> b0 b1 f2
+ |- ((((compact s) /\ (compact t)) ==> (compact (s INTER t))) = ((((bounded s) /\ (closed s)) /\ ((bounded t) /\ (closed t))) ==> T))
T c= c==> c/\ ccompact f0 ccompact f1 ccompact * * part cINTER f0 f1 c==> c/\ c/\ cbounded f0 cclosed f0 c/\ cbounded f1 cclosed f1 cT
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
