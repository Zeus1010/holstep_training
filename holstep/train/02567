N DIST_ADD2
C |- (!m. (!n. (!p. (!q. ((dist ((m + n) , (p + q))) <= ((dist (m , p)) + (dist (n , q))))))))
T c<= cdist c, c+ f0 f1 c+ f2 f3 c+ cdist c, f0 f2 cdist c, f1 f3
D TRUTH
A |- T
T cT
D Hypermap.NON_ZERO
A |- (!n. (~ ((SUC n) = (NUMERAL _0))))
T c~ c= cSUC f0 cNUMERAL c_0
D Ssrnat.addnS
A |- (!m. (!n. ((m + (SUC n)) = (SUC (m + n)))))
T c= c+ f0 cSUC f1 cSUC c+ f0 f1
D ADD_AC
A |- (((m + n) = (n + m)) /\ ((((m + n) + p) = (m + (n + p))) /\ ((m + (n + p)) = (n + (m + p)))))
T c/\ c= c+ f0 f1 c+ f1 f0 c/\ c= c+ c+ f0 f1 f2 c+ f0 c+ f1 f2 c= c+ f0 c+ f1 f2 c+ f1 c+ f0 f2
D EQ_ADD_LCANCEL
A |- (!m. (!n. (!p. (((m + n) = (m + p)) = (n = p)))))
T c= c= c+ f0 f1 c+ f0 f2 c= f1 f2
D EQ_ADD_LCANCEL_0
A |- (!m. (!n. (((m + n) = m) = (n = (NUMERAL _0)))))
T c= c= c+ f0 f1 f0 c= f1 cNUMERAL c_0
D NOT_LT
A |- (!m. (!n. ((~ (m < n)) = (n <= m))))
T c= c~ c< f0 f1 c<= f1 f0
D LT_EXISTS
A |- (!m. (!n. ((m < n) = (?d. (n = (m + (SUC d)))))))
T c= c< f0 f1 c? / c= f1 c+ f0 cSUC b0
D DIST_ELIM_THM
A |- ((P (dist (x , y))) = (!d. (((x = (y + d)) ==> (P d)) /\ ((y = (x + d)) ==> (P d)))))
T c= * f0 cdist c, f1 f2 ! c/\ c==> c= f1 c+ f2 b0 * f0 b0 c==> c= f2 c+ f1 b0 * f0 b0
