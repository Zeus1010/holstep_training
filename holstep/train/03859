N COND_ID
C |- (!b. (!t. ((((COND b) t) t) = t)))
T c= cCOND f0 f1 f1 f1
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
D COND_CLAUSES
A |- (!t1. (!t2. (((((COND T) t1) t2) = t1) /\ ((((COND F) t1) t2) = t2))))
T c/\ c= cCOND cT f0 f1 f0 c= cCOND cF f0 f1 f1
+ (b = F) |- (((((COND b) t) t) = t) = ((((COND F) t) t) = t))
T c==> c= f0 cF c= c= cCOND f0 f1 f1 f1 c= cCOND cF f1 f1 f1
+ (b = F) |- (((((COND F) t) t) = t) = ((((COND b) t) t) = t))
T c==> c= f0 cF c= c= cCOND cF f1 f1 f1 c= cCOND f0 f1 f1 f1
+ |- ((!t. ((((COND b) t) t) = t)) = T)
T c= ! c= cCOND f0 b0 b0 b0 cT
- |- (T = ((((COND T) t) t) = t))
T c= cT c= cCOND cT f0 f0 f0
- |- ((x = x) = T)
T c= c= f0 f0 cT
- |- (T = ((((COND F) t) t) = t))
T c= cT c= cCOND cF f0 f0 f0
