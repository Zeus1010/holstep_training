N ARG_NUM
C |- (!n. ((Arg (Cx (real_of_num n))) = (real_of_num (NUMERAL _0))))
T c= cArg cCx creal_of_num f0 creal_of_num cNUMERAL c_0
D TRUTH
A |- T
T cT
D REAL_POS
A |- (!n. ((real_le (real_of_num (NUMERAL _0))) (real_of_num n)))
T creal_le creal_of_num cNUMERAL c_0 creal_of_num f0
D RE_CX
A |- (!x. ((Re (Cx x)) = x))
T c= cRe cCx f0 f0
D REAL_CX
A |- (!x. (real (Cx x)))
T creal cCx f0
D ARG_EQ_0
A |- (!z. (((Arg z) = (real_of_num (NUMERAL _0))) = ((real z) /\ ((real_le (real_of_num (NUMERAL _0))) (Re z)))))
T c= c= cArg f0 creal_of_num cNUMERAL c_0 c/\ creal f0 creal_le creal_of_num cNUMERAL c_0 cRe f0
+ |- ((!n. t) = t)
T c= ! f0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
