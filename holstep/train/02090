N REAL_CONTINUOUS_NEG
C |- (!f. (!net. (((real_continuous f) net) ==> ((real_continuous (\x. (real_neg (f x)))) net))))
T c==> creal_continuous f0 f1 creal_continuous / creal_neg * f0 b0 f1
D TRUTH
A |- T
T cT
D REALLIM_NEG
A |- (!net. (!f. (!l. ((((---> f) l) net) ==> (((---> (\x. (real_neg (f x)))) (real_neg l)) net)))))
T c==> c---> f0 f1 f2 c---> / creal_neg * f0 b0 creal_neg f1 f2
D real_continuous
A |- (!f. (!net. (((real_continuous f) net) = (((---> f) (f (netlimit net))) net))))
T c= creal_continuous f0 f1 c---> f0 * f0 cnetlimit f1 f1
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((\l. ((((---> f) l) net) ==> (((---> (\x. (real_neg (f x)))) (real_neg l)) net))) l)
T * / c==> c---> f0 b0 f1 c---> / creal_neg * f0 b1 creal_neg b0 f1 f2
+ |- ((!net. t) = t)
T c= ! f0 f0
- |- (((\x. (f x)) y) = (f y))
T c= * / * f0 b0 f1 * f0 f1
