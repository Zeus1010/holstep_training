N RE_DIV_CX
C |- (!z. (!x. ((Re ((complex_div z) (Cx x))) = ((real_div (Re z)) x))))
T c= cRe ccomplex_div f0 cCx f1 creal_div cRe f0 f1
D TRUTH
A |- T
T cT
D real_div
A |- (!x. (!y. (((real_div x) y) = ((real_mul x) (real_inv y)))))
T c= creal_div f0 f1 creal_mul f0 creal_inv f1
D complex_div
A |- (!w. (!z. (((complex_div w) z) = ((complex_mul w) (complex_inv z)))))
T c= ccomplex_div f0 f1 ccomplex_mul f0 ccomplex_inv f1
D CX_INV
A |- (!x. ((Cx (real_inv x)) = (complex_inv (Cx x))))
T c= cCx creal_inv f0 ccomplex_inv cCx f0
D RE_MUL_CX
A |- (!x. (!z. (((Re ((complex_mul (Cx x)) z)) = ((real_mul x) (Re z))) /\ ((Re ((complex_mul z) (Cx x))) = ((real_mul (Re z)) x)))))
T c/\ c= cRe ccomplex_mul cCx f0 f1 creal_mul f0 cRe f1 c= cRe ccomplex_mul f1 cCx f0 creal_mul cRe f1 f0
+ |- (T = (!z. (!x. ((Re ((complex_div z) (Cx x))) = ((real_div (Re z)) x)))))
T c= cT ! ! c= cRe ccomplex_div b0 cCx b1 creal_div cRe b0 b1
- |- ((Re ((complex_mul (Cx x)) z)) = ((real_mul x) (Re z)))
T c= cRe ccomplex_mul cCx f0 f1 creal_mul f0 cRe f1
