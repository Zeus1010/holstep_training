N INT_LET_TRANS
C |- (!x. (!y. (!z. ((((int_le x) y) /\ ((int_lt y) z)) ==> ((int_lt x) z)))))
T c==> c/\ cint_le f0 f1 cint_lt f1 f2 cint_lt f0 f2
D REAL_LET_TRANS
A |- (!x. (!y. (!z. ((((real_le x) y) /\ ((real_lt y) z)) ==> ((real_lt x) z)))))
T c==> c/\ creal_le f0 f1 creal_lt f1 f2 creal_lt f0 f2
D int_le
A |- (!x. (!y. (((int_le x) y) = ((real_le (real_of_int x)) (real_of_int y)))))
T c= cint_le f0 f1 creal_le creal_of_int f0 creal_of_int f1
D int_lt
A |- (!x. (!y. (((int_lt x) y) = ((real_lt (real_of_int x)) (real_of_int y)))))
T c= cint_lt f0 f1 creal_lt creal_of_int f0 creal_of_int f1
