N IMAGE_LIFT_REAL_SEGMENT
C |- ((!a. (!b. (((IMAGE lift) (closed_real_segment ((CONS (a , b)) NIL))) = (closed_segment ((CONS ((lift a) , (lift b))) NIL))))) /\ (!a. (!b. (((IMAGE lift) (open_real_segment (a , b))) = (open_segment ((lift a) , (lift b)))))))
T c/\ ! ! c= * * part cIMAGE part clift * part cclosed_real_segment cCONS c, b0 b1 cNIL * part cclosed_segment cCONS c, clift b0 clift b1 cNIL ! ! c= * * part cIMAGE part clift * part copen_real_segment c, b0 b1 * part copen_segment c, clift b0 clift b1
D TRUTH
A |- T
T cT
D LIFT_DROP
A |- ((!x. ((lift (drop x)) = x)) /\ (!x. ((drop (lift x)) = x)))
T c/\ ! c= clift cdrop b0 b0 ! c= cdrop clift b0 b0
D SEGMENT_REAL_SEGMENT
A |- ((!a. (!b. ((closed_segment ((CONS (a , b)) NIL)) = ((IMAGE lift) (closed_real_segment ((CONS ((drop a) , (drop b))) NIL)))))) /\ (!a. (!b. ((open_segment (a , b)) = ((IMAGE lift) (open_real_segment ((drop a) , (drop b))))))))
T c/\ ! ! c= * part cclosed_segment cCONS c, b0 b1 cNIL * * part cIMAGE part clift * part cclosed_real_segment cCONS c, cdrop b0 cdrop b1 cNIL ! ! c= * part copen_segment c, b0 b1 * * part cIMAGE part clift * part copen_real_segment c, cdrop b0 cdrop b1
- |- (((!x. ((lift (drop x)) = x)) ==> ((\x. ((lift (drop x)) = x)) x)) = ((\x. ((lift (drop x)) = x)) x))
T c= c==> ! c= clift cdrop b0 b0 * / c= clift cdrop b0 b0 f0 * / c= clift cdrop b0 b0 f0
+ |- (((!x. ((drop (lift x)) = x)) ==> ((\x. ((drop (lift x)) = x)) x)) = ((\x. ((drop (lift x)) = x)) x))
T c= c==> ! c= cdrop clift b0 b0 * / c= cdrop clift b0 b0 f0 * / c= cdrop clift b0 b0 f0
+ |- ((!b. t) = t)
T c= ! f0 f0
+ |- ((closed_segment ((CONS (a , b)) NIL)) = ((IMAGE lift) (closed_real_segment ((CONS ((drop a) , (drop b))) NIL))))
T c= * part cclosed_segment cCONS c, f0 f1 cNIL * * part cIMAGE part clift * part cclosed_real_segment cCONS c, cdrop f0 cdrop f1 cNIL
+ |- ((\b. ((open_segment (a , b)) = ((IMAGE lift) (open_real_segment ((drop a) , (drop b)))))) b)
T * / c= * part copen_segment c, f0 b0 * * part cIMAGE part clift * part copen_real_segment c, cdrop f0 cdrop b0 f1
- |- ((\x. ((lift (drop x)) = x)) x)
T * / c= clift cdrop b0 b0 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- ((lift (drop x)) = x)
T c= clift cdrop f0 f0
