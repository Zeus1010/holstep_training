N ENR_IMP_GDELTA
C |- (!s. ((ENR s) ==> (gdelta s)))
T c==> cENR f0 cgdelta f0
D TRUTH
A |- T
T cT
D CLOSED_IMP_GDELTA
A |- (!s. ((closed s) ==> (gdelta s)))
T c==> cclosed f0 cgdelta f0
D OPEN_IMP_GDELTA
A |- (!s. ((open s) ==> (gdelta s)))
T c==> copen f0 cgdelta f0
D GDELTA_INTER
A |- (!s. (!t. (((gdelta s) /\ (gdelta t)) ==> (gdelta (s INTER t)))))
T c==> c/\ cgdelta f0 cgdelta f1 cgdelta * * part cINTER f0 f1
D ENR_INTER_CLOSED_OPEN
A |- (!s. ((ENR s) ==> (?t. (?u. ((closed t) /\ ((open u) /\ (s = (t INTER u))))))))
T c==> cENR f0 c? / c? / c/\ cclosed b0 c/\ copen b1 c= f0 * * part cINTER b0 b1
+ |- ((\s. ((ENR s) ==> (?t. (?u. ((closed t) /\ ((open u) /\ (s = (t INTER u)))))))) s)
T * / c==> cENR b0 c? / c? / c/\ cclosed b1 c/\ copen b2 c= b0 * * part cINTER b1 b2 f0
+ (open s) |- ((open s) /\ ((gdelta s) = T))
T c==> copen f0 c/\ copen f0 c= cgdelta f0 cT
+ ((gdelta s) /\ (gdelta t)) |- (((gdelta s) /\ (gdelta t)) /\ ((gdelta (s INTER t)) = T))
T c==> c/\ cgdelta f0 cgdelta f1 c/\ c/\ cgdelta f0 cgdelta f1 c= cgdelta * * part cINTER f0 f1 cT
+ |- ((((\u. ((closed t) /\ ((open u) /\ (s = (t INTER u))))) u) ==> (gdelta s)) = T)
T c= c==> * / c/\ cclosed f0 c/\ copen b0 c= f1 * * part cINTER f0 b0 f2 cgdelta f1 cT
- (open u) |- (T = (open u))
T c==> copen f0 c= cT copen f0
- (?u. ((closed t) /\ ((open u) /\ (s = (t INTER u))))) |- ((?u. ((closed t) /\ ((open u) /\ (s = (t INTER u))))) /\ (gdelta s))
T c==> c? / c/\ cclosed f0 c/\ copen b0 c= f1 * * part cINTER f0 b0 c/\ c? / c/\ cclosed f0 c/\ copen b0 c= f1 * * part cINTER f0 b0 cgdelta f1
+ (open u), (closed t), ((open u) /\ (s = (t INTER u))) |- (gdelta s)
T c==> copen f0 c==> cclosed f1 c==> c/\ copen f0 c= f2 * * part cINTER f1 f0 cgdelta f2
+ (ENR s) |- ((ENR s) /\ (gdelta s))
T c==> cENR f0 c/\ cENR f0 cgdelta f0
+ |- (((!s. ((open s) ==> (gdelta s))) ==> ((\s. ((open s) ==> (gdelta s))) s)) = ((\s. ((open s) ==> (gdelta s))) s))
T c= c==> ! c==> copen b0 cgdelta b0 * / c==> copen b0 cgdelta b0 f0 * / c==> copen b0 cgdelta b0 f0
+ (closed t), ((closed t) /\ ((open u) /\ (s = (t INTER u)))) |- (gdelta s)
T c==> cclosed f0 c==> c/\ cclosed f0 c/\ copen f1 c= f2 * * part cINTER f0 f1 cgdelta f2
+ ((closed t) /\ ((open u) /\ (s = (t INTER u)))) |- (((closed t) /\ ((open u) /\ (s = (t INTER u)))) /\ (gdelta s))
T c==> c/\ cclosed f0 c/\ copen f1 c= f2 * * part cINTER f0 f1 c/\ c/\ cclosed f0 c/\ copen f1 c= f2 * * part cINTER f0 f1 cgdelta f2
- (ENR s) |- ((ENR s) = T)
T c==> cENR f0 c= cENR f0 cT
- (s = (t INTER u)) |- ((open (t INTER u)) = (open s))
T c==> c= f0 * * part cINTER f1 f2 c= copen * * part cINTER f1 f2 copen f0
- (s = (t INTER u)) |- ((closed (t INTER u)) = (closed s))
T c==> c= f0 * * part cINTER f1 f2 c= cclosed * * part cINTER f1 f2 cclosed f0
- (closed t) |- ((closed t) = T)
T c==> cclosed f0 c= cclosed f0 cT
- (open u) |- ((open u) = T)
T c==> copen f0 c= copen f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- (closed t) |- (T = (closed t))
T c==> cclosed f0 c= cT cclosed f0
