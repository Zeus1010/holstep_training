N TREAL_MUL_SYM_EQ
C |- (!x. (!y. ((x treal_mul y) = (y treal_mul x))))
T c= ctreal_mul f0 f1 ctreal_mul f1 f0
D TRUTH
A |- T
T cT
D FORALL_PAIR_THM
A |- (!P. ((!p. (P p)) = (!p1. (!p2. (P (p1 , p2))))))
T c= ! * f0 b0 ! ! * f0 c, b0 b1
D HREAL_ADD_SYM
A |- (!x. (!y. (((hreal_add x) y) = ((hreal_add y) x))))
T c= chreal_add f0 f1 chreal_add f1 f0
D HREAL_MUL_SYM
A |- (!x. (!y. (((hreal_mul x) y) = ((hreal_mul y) x))))
T c= chreal_mul f0 f1 chreal_mul f1 f0
D treal_mul
A |- (!x1. (!y2. (!y1. (!x2. (((x1 , y1) treal_mul (x2 , y2)) = (((hreal_add ((hreal_mul x1) x2)) ((hreal_mul y1) y2)) , ((hreal_add ((hreal_mul x1) y2)) ((hreal_mul y1) x2))))))))
T c= ctreal_mul c, f0 f1 c, f2 f3 c, chreal_add chreal_mul f0 f2 chreal_mul f1 f3 chreal_add chreal_mul f0 f3 chreal_mul f1 f2
