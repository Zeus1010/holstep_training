N MATRIX_INV_MULTIPLE_TRANP_LEFT
C |- (!A. ((matrix_inv A) = ((matrix_mul (transp A)) ((matrix_mul (transp (matrix_inv A))) (matrix_inv A)))))
T c= cmatrix_inv f0 cmatrix_mul ctransp f0 cmatrix_mul ctransp cmatrix_inv f0 cmatrix_inv f0
D TRUTH
A |- T
T cT
D MATRIX_MUL_ASSOC
A |- (!A. (!B. (!C. (((matrix_mul A) ((matrix_mul B) C)) = ((matrix_mul ((matrix_mul A) B)) C)))))
T c= cmatrix_mul f0 cmatrix_mul f1 f2 cmatrix_mul cmatrix_mul f0 f1 f2
D MATRIX_TRANSP_MUL
A |- (!A. (!B. ((transp ((matrix_mul A) B)) = ((matrix_mul (transp B)) (transp A)))))
T c= ctransp cmatrix_mul f0 f1 cmatrix_mul ctransp f1 ctransp f0
D MATRIX_INV_MUL_OUTER
A |- (!A. (((matrix_mul (matrix_inv A)) ((matrix_mul A) (matrix_inv A))) = (matrix_inv A)))
T c= cmatrix_mul cmatrix_inv f0 cmatrix_mul f0 cmatrix_inv f0 cmatrix_inv f0
D SYMMETRIC_MATRIX_INV_LMUL
