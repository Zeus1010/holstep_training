N EVEN_DOUBLE
C |- (!n. (EVEN ((NUMERAL (BIT0 (BIT1 _0))) * n)))
T cEVEN c* cNUMERAL cBIT0 cBIT1 c_0 f0
D TRUTH
A |- T
T cT
D BIT0_THM
A |- (!n. ((NUMERAL (BIT0 n)) = ((NUMERAL n) + (NUMERAL n))))
T c= cNUMERAL cBIT0 f0 c+ cNUMERAL f0 cNUMERAL f0
D BIT1_THM
A |- (!n. ((NUMERAL (BIT1 n)) = (SUC ((NUMERAL n) + (NUMERAL n)))))
T c= cNUMERAL cBIT1 f0 cSUC c+ cNUMERAL f0 cNUMERAL f0
D EVEN_ADD
A |- (!m. (!n. ((EVEN (m + n)) = ((EVEN m) = (EVEN n)))))
T c= cEVEN c+ f0 f1 c= cEVEN f0 cEVEN f1
D EVEN_MULT
A |- (!m. (!n. ((EVEN (m * n)) = ((EVEN m) \/ (EVEN n)))))
T c= cEVEN c* f0 f1 c\/ cEVEN f0 cEVEN f1
+ |- ((\n. ((EVEN (m + n)) = ((EVEN m) = (EVEN n)))) n)
T * / c= cEVEN c+ f0 b0 c= cEVEN f0 cEVEN b0 f1
