N NADD_CAUCHY
C |- (!x. (?B. (!m. (!n. ((dist ((m * ((dest_nadd x) n)) , (n * ((dest_nadd x) m)))) <= (B * (m + n)))))))
T c? / ! ! c<= cdist c, c* b1 cdest_nadd f0 b2 c* b2 cdest_nadd f0 b1 c* b0 c+ b1 b2
D TRUTH
A |- T
T cT
D ETA_AX
A |- (!t. ((\x. (t x)) = t))
T c= / * f0 b0 f0
D is_nadd
A |- (!x. ((is_nadd x) = (?B. (!m. (!n. ((dist ((m * (x n)) , (n * (x m)))) <= (B * (m + n))))))))
T c= cis_nadd f0 c? / ! ! c<= cdist c, c* b1 * f0 b2 c* b2 * f0 b1 c* b0 c+ b1 b2
D nadd_abs
A |- ((mk_nadd (dest_nadd a)) = a)
T c= cmk_nadd * part cdest_nadd f0 f0
D nadd_rep
A |- ((is_nadd r) = ((dest_nadd (mk_nadd r)) = r))
T c= cis_nadd f0 c= * part cdest_nadd cmk_nadd f0 f0
