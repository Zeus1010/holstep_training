N COVERING_SPACE_INESSENTIAL_LOOP_LIFT_IS_LOOP
C |- (!p. (!c. (!s. (!g. (!h. (!a. ((((covering_space (c , p)) s) /\ ((path g) /\ (((path_image g) SUBSET s) /\ (((pathfinish g) = (pathstart g)) /\ ((((homotopic_paths s) g) (linepath (a , a))) /\ ((path h) /\ (((path_image h) SUBSET c) /\ (!t. ((t IN (closed_interval ((CONS ((vec (NUMERAL _0)) , (vec (NUMERAL (BIT1 _0))))) NIL))) ==> ((p (h t)) = (g t))))))))))) ==> ((pathfinish h) = (pathstart h)))))))))
