N EVEN_OR_ODD
C |- (!n. ((EVEN n) \/ (ODD n)))
T c\/ cEVEN f0 cODD f0
D TRUTH
A |- T
T cT
D num_INDUCTION
A |- (!P. (((P (NUMERAL _0)) /\ (!n. ((P n) ==> (P (SUC n))))) ==> (!n. (P n))))
T c==> c/\ * f0 cNUMERAL c_0 ! c==> * f0 b0 * f0 cSUC b0 ! * f0 b0
D EVEN
A |- (((EVEN (NUMERAL _0)) = T) /\ (!n. ((EVEN (SUC n)) = (~ (EVEN n)))))
T c/\ c= cEVEN cNUMERAL c_0 cT ! c= cEVEN cSUC b0 c~ cEVEN b0
D ODD
A |- (((ODD (NUMERAL _0)) = F) /\ (!n. ((ODD (SUC n)) = (~ (ODD n)))))
T c/\ c= cODD cNUMERAL c_0 cF ! c= cODD cSUC b0 c~ cODD b0
D NOT_EVEN
A |- (!n. ((~ (EVEN n)) = (ODD n)))
T c= c~ cEVEN f0 cODD f0
D NOT_ODD
A |- (!n. ((~ (ODD n)) = (EVEN n)))
T c= c~ cODD f0 cEVEN f0
+ |- ((EVEN (NUMERAL _0)) \/ (ODD (NUMERAL _0)))
T c\/ cEVEN cNUMERAL c_0 cODD cNUMERAL c_0
+ |- (((((EVEN (NUMERAL _0)) \/ (ODD (NUMERAL _0))) /\ (!n. (((EVEN n) \/ (ODD n)) ==> ((EVEN (SUC n)) \/ (ODD (SUC n)))))) ==> (!n. ((EVEN n) \/ (ODD n)))) = (!n. ((EVEN n) \/ (ODD n))))
T c= c==> c/\ c\/ cEVEN cNUMERAL c_0 cODD cNUMERAL c_0 ! c==> c\/ cEVEN b0 cODD b0 c\/ cEVEN cSUC b0 cODD cSUC b0 ! c\/ cEVEN b0 cODD b0 ! c\/ cEVEN b0 cODD b0
+ |- (((!t1. (!t2. ((t1 \/ t2) = (t2 \/ t1)))) ==> ((\t1. (!t2. ((t1 \/ t2) = (t2 \/ t1)))) t1)) = ((\t1. (!t2. ((t1 \/ t2) = (t2 \/ t1)))) t1))
T c= c==> ! ! c= c\/ b0 b1 c\/ b1 b0 * / ! c= c\/ b0 b1 c\/ b1 b0 f0 * / ! c= c\/ b0 b1 c\/ b1 b0 f0
- |- ((t \/ t) = t)
T c= c\/ f0 f0 f0
- ((EVEN n) \/ (ODD n)) |- (T = ((EVEN n) \/ (ODD n)))
T c==> c\/ cEVEN f0 cODD f0 c= cT c\/ cEVEN f0 cODD f0
- ((EVEN n) \/ (ODD n)) |- (((EVEN n) \/ (ODD n)) = T)
T c==> c\/ cEVEN f0 cODD f0 c= c\/ cEVEN f0 cODD f0 cT
