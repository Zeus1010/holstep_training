N HAS_REAL_DERIVATIVE_SIN
C |- (!x. (((has_real_derivative sin) (cos x)) (atreal x)))
T chas_real_derivative part csin ccos f0 catreal f0
D TRUTH
A |- T
T cT
D HAS_COMPLEX_DERIVATIVE_AT_WITHIN
A |- (!f. (!f'. (!x. (!s. ((((has_complex_derivative f) f') (at x)) ==> (((has_complex_derivative f) f') ((within (at x)) s)))))))
T c==> chas_complex_derivative f0 f1 cat f2 chas_complex_derivative f0 f1 cwithin cat f2 f3
D CX_SIN
A |- (!x. ((Cx (sin x)) = (csin (Cx x))))
T c= cCx csin f0 ccsin cCx f0
D CX_COS
A |- (!x. ((Cx (cos x)) = (ccos (Cx x))))
T c= cCx ccos f0 cccos cCx f0
D HAS_COMPLEX_DERIVATIVE_CSIN
A |- (!z. (((has_complex_derivative csin) (ccos z)) (at z)))
T chas_complex_derivative part ccsin cccos f0 cat f0
D HAS_COMPLEX_REAL_DERIVATIVE_AT
A |- (!f. (!g. (!h. (((((has_complex_derivative h) (Cx g)) ((within (at (Cx x))) real)) /\ (!y. ((h (Cx y)) = (Cx (f y))))) ==> (((has_real_derivative f) g) (atreal x))))))
T c==> c/\ chas_complex_derivative f0 cCx f1 cwithin cat cCx f2 part creal ! c= * f0 cCx b0 cCx * f3 b0 chas_real_derivative f3 f1 catreal f2
+ (!f. (!g. (!h. (((((has_complex_derivative h) (Cx g)) ((within (at (Cx x))) real)) /\ (!y. ((h (Cx y)) = (Cx (f y))))) ==> (((has_real_derivative f) g) (atreal x)))))) |- (!g. (!h. (((((has_complex_derivative h) (Cx g)) ((within (at (Cx x))) real)) /\ (!y. ((h (Cx y)) = (Cx (f y))))) ==> (((has_real_derivative f) g) (atreal x)))))
T c==> ! ! ! c==> c/\ chas_complex_derivative b2 cCx b1 cwithin cat cCx f0 part creal ! c= * b2 cCx b3 cCx * b0 b3 chas_real_derivative b0 b1 catreal f0 ! ! c==> c/\ chas_complex_derivative b1 cCx b0 cwithin cat cCx f0 part creal ! c= * b1 cCx b2 cCx * f1 b2 chas_real_derivative f1 b0 catreal f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
