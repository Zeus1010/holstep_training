N REAL_MEASURABLE_IMP_REAL_LEBESGUE_MEASURABLE
C |- (!s. ((real_measurable s) ==> (real_lebesgue_measurable s)))
T c==> creal_measurable f0 creal_lebesgue_measurable f0
D TRUTH
A |- T
T cT
D MEASURABLE_IMP_LEBESGUE_MEASURABLE
A |- (!s. ((measurable s) ==> (lebesgue_measurable s)))
T c==> cmeasurable f0 clebesgue_measurable f0
D REAL_MEASURABLE_MEASURABLE
A |- (!s. ((real_measurable s) = (measurable ((IMAGE lift) s))))
T c= creal_measurable f0 cmeasurable * * part cIMAGE part clift f0
D REAL_LEBESGUE_MEASURABLE
A |- (!s. ((real_lebesgue_measurable s) = (lebesgue_measurable ((IMAGE lift) s))))
T c= creal_lebesgue_measurable f0 clebesgue_measurable * * part cIMAGE part clift f0
+ |- (((!s. ((real_lebesgue_measurable s) = (lebesgue_measurable ((IMAGE lift) s)))) ==> ((\s. ((real_lebesgue_measurable s) = (lebesgue_measurable ((IMAGE lift) s)))) s)) = ((\s. ((real_lebesgue_measurable s) = (lebesgue_measurable ((IMAGE lift) s)))) s))
T c= c==> ! c= creal_lebesgue_measurable b0 clebesgue_measurable * * part cIMAGE part clift b0 * / c= creal_lebesgue_measurable b0 clebesgue_measurable * * part cIMAGE part clift b0 f0 * / c= creal_lebesgue_measurable b0 clebesgue_measurable * * part cIMAGE part clift b0 f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
