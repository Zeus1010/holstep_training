N BASIS_HAS_SIZE_UNIV
C |- (!b. (((independent b) /\ ((span b) = UNIV)) ==> (b HAS_SIZE (dimindex UNIV))))
T c==> c/\ cindependent f0 c= * part cspan f0 part cUNIV cHAS_SIZE f0 cdimindex part cUNIV
D TRUTH
A |- T
T cT
D BASIS_HAS_SIZE_DIM
A |- (!v. (!b. (((independent b) /\ ((span b) = v)) ==> (b HAS_SIZE (dim v)))))
T c==> c/\ cindependent f0 c= * part cspan f0 f1 cHAS_SIZE f0 cdim f1
D DIM_UNIV
A |- ((dim UNIV) = (dimindex UNIV))
T c= cdim part cUNIV cdimindex part cUNIV
+ |- ((((independent b) /\ ((span b) = v)) ==> (b HAS_SIZE (dim v))) = T)
T c= c==> c/\ cindependent f0 c= * part cspan f0 f1 cHAS_SIZE f0 cdim f1 cT
+ ((dim UNIV) = (dimindex UNIV)) |- ((dimindex UNIV) = (dim UNIV))
T c==> c= cdim part cUNIV cdimindex part cUNIV c= cdimindex part cUNIV cdim part cUNIV
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
+ |- ((dimindex UNIV) = (dim UNIV))
T c= cdimindex part cUNIV cdim part cUNIV
- |- ((x = x) = T)
T c= c= f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
