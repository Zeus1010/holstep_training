N PASTECART_CMUL
C |- (!x1. (!y1. (!c. (((pastecart ((% c) x1)) ((% c) y1)) = ((% c) ((pastecart x1) y1))))))
T c= cpastecart c% f0 f1 c% f0 f2 c% f0 cpastecart f1 f2
D TRUTH
A |- T
T cT
D FSTCART_PASTECART
A |- (!x. (!y. ((fstcart ((pastecart x) y)) = x)))
T c= cfstcart cpastecart f0 f1 f0
D SNDCART_PASTECART
A |- (!x. (!y. ((sndcart ((pastecart x) y)) = y)))
T c= csndcart cpastecart f0 f1 f1
D PASTECART_EQ
A |- (!x. (!y. ((x = y) = (((fstcart x) = (fstcart y)) /\ ((sndcart x) = (sndcart y))))))
T c= c= f0 f1 c/\ c= cfstcart f0 cfstcart f1 c= csndcart f0 csndcart f1
D FSTCART_CMUL
A |- (!x. (!c. ((fstcart ((% c) x)) = ((% c) (fstcart x)))))
T c= cfstcart c% f0 f1 c% f0 cfstcart f1
D SNDCART_CMUL
A |- (!x. (!c. ((sndcart ((% c) x)) = ((% c) (sndcart x)))))
T c= csndcart c% f0 f1 c% f0 csndcart f1
+ |- (((!x. (!y. ((sndcart ((pastecart x) y)) = y))) ==> ((\x. (!y. ((sndcart ((pastecart x) y)) = y))) x)) = ((\x. (!y. ((sndcart ((pastecart x) y)) = y))) x))
T c= c==> ! ! c= csndcart cpastecart b0 b1 b1 * / ! c= csndcart cpastecart b0 b1 b1 f0 * / ! c= csndcart cpastecart b0 b1 b1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
