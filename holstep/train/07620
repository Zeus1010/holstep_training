N COMPACT_SEGMENT
C |- ((!a. (!b. (compact (closed_segment ((CONS (a , b)) NIL))))) /\ (!a. (!b. ((compact (open_segment (a , b))) = (a = b)))))
T c/\ ! ! ccompact * part cclosed_segment cCONS c, b0 b1 cNIL ! ! c= ccompact * part copen_segment c, b0 b1 c= b0 b1
D TRUTH
A |- T
T cT
D COMPACT_EQ_BOUNDED_CLOSED
A |- (!s. ((compact s) = ((bounded s) /\ (closed s))))
T c= ccompact f0 c/\ cbounded f0 cclosed f0
D BOUNDED_SEGMENT
A |- ((!a. (!b. (bounded (closed_segment ((CONS (a , b)) NIL))))) /\ (!a. (!b. (bounded (open_segment (a , b))))))
T c/\ ! ! cbounded * part cclosed_segment cCONS c, b0 b1 cNIL ! ! cbounded * part copen_segment c, b0 b1
D CLOSED_SEGMENT
A |- ((!a. (!b. (closed (closed_segment ((CONS (a , b)) NIL))))) /\ (!a. (!b. ((closed (open_segment (a , b))) = (a = b)))))
T c/\ ! ! cclosed * part cclosed_segment cCONS c, b0 b1 cNIL ! ! c= cclosed * part copen_segment c, b0 b1 c= b0 b1
+ |- ((\b. ((closed (open_segment (a , b))) = (a = b))) b)
T * / c= cclosed * part copen_segment c, f0 b0 c= f0 b0 f1
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
