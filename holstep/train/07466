N HAS_REAL_VECTOR_DERIVATIVE_WITHIN
C |- ((((has_real_derivative f) f') ((within (atreal x)) s)) = (((has_vector_derivative (lift o (f o drop))) (lift f')) ((within (at (lift x))) ((IMAGE lift) s))))
T c= chas_real_derivative f0 f1 cwithin catreal f2 f3 chas_vector_derivative * * part co part clift * * part co f0 part cdrop clift f1 cwithin cat clift f2 * * part cIMAGE part clift f3
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D REAL_MUL_SYM
A |- (!x. (!y. (((real_mul x) y) = ((real_mul y) x))))
T c= creal_mul f0 f1 creal_mul f1 f0
D LIFT_DROP
A |- ((!x. ((lift (drop x)) = x)) /\ (!x. ((drop (lift x)) = x)))
T c/\ ! c= clift cdrop b0 b0 ! c= cdrop clift b0 b0
D FORALL_LIFT
A |- ((!x. (P x)) = (!x. (P (lift x))))
T c= ! * f0 b0 ! * f0 clift b0
D LIFT_EQ
A |- (!x. (!y. (((lift x) = (lift y)) = (x = y))))
T c= c= clift f0 clift f1 c= f0 f1
D LIFT_CMUL
A |- (!x. (!c. ((lift ((real_mul c) x)) = ((% c) (lift x)))))
T c= clift creal_mul f0 f1 c% f0 clift f1
D has_vector_derivative
A |- (!f. (!f'. (!net. ((((has_vector_derivative f) f') net) = (((has_derivative f) (\x. ((% (drop x)) f'))) net)))))
T c= chas_vector_derivative f0 f1 f2 chas_derivative f0 / c% cdrop b0 f1 f2
D HAS_REAL_FRECHET_DERIVATIVE_WITHIN
A |- ((((has_real_derivative f) f') ((within (atreal x)) s)) = (((has_derivative (lift o (f o drop))) (\x. ((% f') x))) ((within (at (lift x))) ((IMAGE lift) s))))
T c= chas_real_derivative f0 f1 cwithin catreal f2 f3 chas_derivative * * part co part clift * * part co f0 part cdrop / c% f1 b0 cwithin cat clift f2 * * part cIMAGE part clift f3
+ |- (((!c. (((% c) (lift x)) = (lift ((real_mul c) x)))) ==> ((\c. (((% c) (lift x)) = (lift ((real_mul c) x)))) c)) = ((\c. (((% c) (lift x)) = (lift ((real_mul c) x)))) c))
T c= c==> ! c= c% b0 clift f0 clift creal_mul b0 f0 * / c= c% b0 clift f0 clift creal_mul b0 f0 f1 * / c= c% b0 clift f0 clift creal_mul b0 f0 f1
- |- (((!x. ((lift (drop x)) = x)) ==> ((\x. ((lift (drop x)) = x)) x)) = ((\x. ((lift (drop x)) = x)) x))
T c= c==> ! c= clift cdrop b0 b0 * / c= clift cdrop b0 b0 f0 * / c= clift cdrop b0 b0 f0
+ |- (((!x. ((drop (lift x)) = x)) ==> ((\x. ((drop (lift x)) = x)) x)) = ((\x. ((drop (lift x)) = x)) x))
T c= c==> ! c= cdrop clift b0 b0 * / c= cdrop clift b0 b0 f0 * / c= cdrop clift b0 b0 f0
+ |- ((!x. ((lift ((real_mul f') x)) = (lift ((real_mul (drop (lift x))) f')))) = ((\x. ((% f') x)) = (\x. ((% (drop x)) (lift f')))))
T c= ! c= clift creal_mul f0 b0 clift creal_mul cdrop clift b0 f0 c= / c% f0 b0 / c% cdrop b0 clift f0
+ |- ((\x. (!y. (((lift x) = (lift y)) = (x = y)))) x)
T * / ! c= c= clift b0 clift b1 c= b0 b1 f0
- |- ((\x. ((lift (drop x)) = x)) x)
T * / c= clift cdrop b0 b0 f0
- |- (((\x. (f x)) y) = (f y))
T c= * / * f0 b0 f1 * f0 f1
- |- ((lift (drop x)) = x)
T c= clift cdrop f0 f0
