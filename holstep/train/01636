N Local_lemmas.AFF_GT_SUB_AFF_UNION
C |- (((aff_gt X) Y) SUBSET (aff (X UNION Y)))
T cSUBSET * * part caff_gt f0 f1 * part caff * * part cUNION f0 f1
D SUBSET_REFL
A |- (!s. (s SUBSET s))
T cSUBSET f0 f0
D DIFF_EQ_EMPTY
A |- (!s. ((s DIFF s) = EMPTY))
T c= * * part cDIFF f0 f0 part cEMPTY
D Trigonometry.UIVNNRR2
A |- (!s. (!t. (((aff_gt s) EMPTY) = ((hull affine) s))))
T c= * * part caff_gt f0 part cEMPTY * * part chull part caffine f0
D Sphere.aff
A |- (aff = (hull affine))
T c= part caff * part chull part caffine
D Local_lemmas.AFF_GT_MONO
A |- (!S. ((S SUBSET Y) ==> (((aff_gt X) Y) SUBSET ((aff_gt (X UNION S)) (Y DIFF S)))))
T c==> cSUBSET f0 f1 cSUBSET * * part caff_gt f2 f1 * * part caff_gt * * part cUNION f2 f0 * * part cDIFF f1 f0
+ |- ((\s. ((s DIFF s) = EMPTY)) s)
T * / c= * * part cDIFF b0 b0 part cEMPTY f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
