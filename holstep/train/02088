N FORALL_BOOL_THM
C |- ((!b. (P b)) = ((P T) /\ (P F)))
T c= ! * f0 b0 c/\ * f0 cT * f0 cF
D TRUTH
A |- T
T cT
D BOOL_CASES_AX
A |- (!t. ((t = T) \/ (t = F)))
T c\/ c= f0 cT c= f0 cF
+ ((P T) /\ (P F)) |- ((b = T) ==> (P b))
T c==> c/\ * f0 cT * f0 cF c==> c= f1 cT * f0 f1
+ (!b. (P b)) |- ((P b) = T)
T c==> ! * f0 b0 c= * f0 f1 cT
+ (b = T), ((P T) /\ (P F)) |- ((b = T) /\ (P b))
T c==> c= f0 cT c==> c/\ * f1 cT * f1 cF c/\ c= f0 cT * f1 f0
+ ((P T) /\ (P F)) |- ((P b) = T)
T c==> c/\ * f0 cT * f0 cF c= * f0 f1 cT
+ |- ((!b. (P b)) ==> ((P T) /\ (P F)))
T c==> ! * f0 b0 c/\ * f0 cT * f0 cF
+ ((b = T) ==> (P b)), ((b = F) ==> (P b)) |- (P b)
T c==> c==> c= f0 cT * f1 f0 c==> c==> c= f0 cF * f1 f0 * f1 f0
- ((P T) /\ (P F)) |- ((P T) = T)
T c==> c/\ * f0 cT * f0 cF c= * f0 cT cT
- |- ((!b. t) = t)
T c= ! f0 f0
+ |- (((!b. (P b)) ==> ((P T) /\ (P F))) /\ (((P T) /\ (P F)) ==> (!b. (P b))))
T c/\ c==> ! * f0 b0 c/\ * f0 cT * f0 cF c==> c/\ * f0 cT * f0 cF ! * f0 b0
+ |- (((P T) /\ (P F)) ==> (!b. (P b)))
T c==> c/\ * f0 cT * f0 cF ! * f0 b0
- |- ((\P. ((!x. (~ (P x))) = (~ (?x. (P x))))) P)
T * / c= ! c~ * b0 b1 c~ c? / * b0 b1 f0
- ((P T) /\ (P F)) |- (T = (P T))
T c==> c/\ * f0 cT * f0 cF c= cT * f0 cT
+ (b = T) |- ((P T) = (P b))
T c==> c= f0 cT c= * f1 cT * f1 f0
- ((P T) /\ (P F)) |- ((P F) = T)
T c==> c/\ * f0 cT * f0 cF c= * f0 cF cT
+ (b = F), ((P T) /\ (P F)) |- (P b)
T c==> c= f0 cF c==> c/\ * f1 cT * f1 cF * f1 f0
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
- |- ((!x. (~ (P x))) = (~ (?x. (P x))))
T c= ! c~ * f0 b0 c~ c? / * f0 b0
- ((P T) /\ (P F)) |- (T = (P F))
T c==> c/\ * f0 cT * f0 cF c= cT * f0 cF
- |- (((!P. ((!x. (~ (P x))) = (~ (?x. (P x))))) ==> ((\P. ((!x. (~ (P x))) = (~ (?x. (P x))))) P)) = ((\P. ((!x. (~ (P x))) = (~ (?x. (P x))))) P))
T c= c==> ! c= ! c~ * b0 b1 c~ c? / * b0 b1 * / c= ! c~ * b0 b1 c~ c? / * b0 b1 f0 * / c= ! c~ * b0 b1 c~ c? / * b0 b1 f0
- |- (((!x. (~ (P x))) = (~ (?x. (P x)))) = T)
T c= c= ! c~ * f0 b0 c~ c? / * f0 b0 cT
