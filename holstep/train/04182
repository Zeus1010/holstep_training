N POLYTOPE_IMP_CLOSED
C |- (!s. ((polytope s) ==> (closed s)))
T c==> cpolytope f0 cclosed f0
D TRUTH
A |- T
T cT
D COMPACT_IMP_CLOSED
A |- (!s. ((compact s) ==> (closed s)))
T c==> ccompact f0 cclosed f0
D POLYTOPE_IMP_COMPACT
A |- (!s. ((polytope s) ==> (compact s)))
T c==> cpolytope f0 ccompact f0
+ |- ((\p'. (!q'. (((polytope s) = p') ==> ((p' ==> ((closed s) = q')) ==> (((polytope s) ==> (closed s)) = (p' ==> q')))))) p')
T * / ! c==> c= cpolytope f0 b0 c==> c==> b0 c= cclosed f0 b1 c= c==> cpolytope f0 cclosed f0 c==> b0 b1 f1
+ |- ((polytope s) ==> ((closed s) = T))
T c==> cpolytope f0 c= cclosed f0 cT
+ |- ((compact s) ==> ((closed s) = T))
T c==> ccompact f0 c= cclosed f0 cT
- (polytope s) |- (T = (polytope s))
T c==> cpolytope f0 c= cT cpolytope f0
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- (polytope s) |- ((polytope s) = T)
T c==> cpolytope f0 c= cpolytope f0 cT
