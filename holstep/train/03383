N SURJECTIVE_INVERSE_o
C |- (!f. ((!y. (?x. ((f x) = y))) = ((f o (inverse f)) = I)))
T c= ! c? / c= * f0 b1 b0 c= * * part co f0 * part cinverse f0 part cI
D TRUTH
A |- T
T cT
D FUN_EQ_THM
A |- (!f. (!g. ((f = g) = (!x. ((f x) = (g x))))))
T c= c= f0 f1 ! c= * f0 b0 * f1 b0
D o_THM
A |- (!f. (!g. (!x. (((f o g) x) = (f (g x))))))
T c= co f0 f1 f2 * f0 * f1 f2
D I_THM
A |- (!x. ((I x) = x))
T c= cI f0 f0
D SURJECTIVE_INVERSE
A |- (!f. ((!y. (?x. ((f x) = y))) = (!y. ((f ((inverse f) y)) = y))))
T c= ! c? / c= * f0 b1 b0 ! c= * f0 cinverse f0 b0 b0
