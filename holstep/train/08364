N Packing3.VORONOI_LIST_SING
C |- (!V. (!u. (((voronoi_list V) ((CONS u) NIL)) = ((voronoi_closed V) u))))
T c= * * part cvoronoi_list f0 cCONS f1 cNIL * * part cvoronoi_closed f0 f1
D TRUTH
A |- T
T cT
D set_of_list
A |- (((set_of_list NIL) = EMPTY) /\ ((set_of_list ((CONS h) t)) = (h INSERT (set_of_list t))))
T c/\ c= * part cset_of_list cNIL part cEMPTY c= * part cset_of_list cCONS f0 f1 * * part cINSERT f0 * part cset_of_list f1
D Sphere.VORONOI_LIST
A |- (!V. (!wl. (((voronoi_list V) wl) = ((voronoi_set V) (set_of_list wl)))))
T c= * * part cvoronoi_list f0 f1 * * part cvoronoi_set f0 * part cset_of_list f1
D Packing3.VORONOI_SET_SING
A |- (!V. (!u. (((voronoi_set V) (u INSERT EMPTY)) = ((voronoi_closed V) u))))
T c= * * part cvoronoi_set f0 * * part cINSERT f1 part cEMPTY * * part cvoronoi_closed f0 f1
