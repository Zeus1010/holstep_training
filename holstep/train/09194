N CARD_EQ_BALL
C |- (!a. (!r. (((real_lt (real_of_num (NUMERAL _0))) r) ==> ((ball (a , r)) =_c UNIV))))
T c==> creal_lt creal_of_num cNUMERAL c_0 f0 c=_c * part cball c, f1 f0 part cUNIV
D TRUTH
A |- T
T cT
D REAL_NOT_LT
A |- (!x. (!y. ((~ ((real_lt x) y)) = ((real_le y) x))))
T c= c~ creal_lt f0 f1 creal_le f1 f0
D OPEN_BALL
A |- (!x. (!e. (open (ball (x , e)))))
T copen * part cball c, f0 f1
D BALL_EQ_EMPTY
A |- (!x. (!e. (((ball (x , e)) = EMPTY) = ((real_le e) (real_of_num (NUMERAL _0))))))
T c= c= * part cball c, f0 f1 part cEMPTY creal_le f1 creal_of_num cNUMERAL c_0
D CARD_EQ_OPEN
A |- (!s. (((open s) /\ (~ (s = EMPTY))) ==> (s =_c UNIV)))
T c==> c/\ copen f0 c~ c= f0 part cEMPTY c=_c f0 part cUNIV
+ |- ((\s. (((open s) /\ (~ (s = EMPTY))) ==> (s =_c UNIV))) s)
T * / c==> c/\ copen b0 c~ c= b0 part cEMPTY c=_c b0 part cUNIV f0
+ |- ((\q'. ((((real_lt (real_of_num (NUMERAL _0))) r) = p') ==> ((p' ==> (((ball (a , r)) =_c UNIV) = q')) ==> ((((real_lt (real_of_num (NUMERAL _0))) r) ==> ((ball (a , r)) =_c UNIV)) = (p' ==> q'))))) q')
T * / c==> c= creal_lt creal_of_num cNUMERAL c_0 f0 f1 c==> c==> f1 c= c=_c * part cball c, f2 f0 part cUNIV b0 c= c==> creal_lt creal_of_num cNUMERAL c_0 f0 c=_c * part cball c, f2 f0 part cUNIV c==> f1 b0 f3
- |- ((t ==> t) = T)
T c= c==> f0 f0 cT
- |- ((t /\ t) = t)
T c= c/\ f0 f0 f0
